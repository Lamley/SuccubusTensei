@EXIST_キャラデータ_ID
#LOCALSIZE 1
#LOCALSSIZE 1

;呼び名から
;要素名→ID変換用関数
;必要なら各オブジェクトで同様に作成のこと
@キャラデータ_ID(ARG:0, ARGS:0, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS V_NAME                  ;作成する変数名
#DIMS C_NAME, OBJ_ID_LAST + 1 ;データ保持
#DIM  O_ID                    ;参照用
IF ARGS:0 == "RESET"
	VARSET C_NAME
	FOR O_ID, 1, OBJ_ID_LAST + 1
		C_NAME:O_ID = %GET_STR(, "キャラデータ", O_ID, "呼び名")%
	NEXT
ELSE
	O_ID = FINDELEMENT(C_NAME, ARGS:0, , , 1)
	CALLF MAKE_INT(V_NAME, O_ID)
ENDIF



;存否関数
;F関数はTRYCALL系が使えないので通常関数で存否配列を作っちゃう
@EXIST_キャラデータ1
#LOCALSIZE 1
#LOCALSSIZE 1

;オブジェクト本体
@キャラデータ1(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "霊夢")

;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（襦袢）セット")

CASE "普段着：飾品"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("飾品", "袖"))
CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "蝴蝶結"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "普段着：襪子"
	IF FLAG:2 > 0
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("襪子", "長筒襪"))
	ELSE
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("襪子", "襪子"))
	ENDIF
CASE "普段着：上半身内袴１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身内袴１", "裹胸"))
CASE "普段着：裙子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("裙子", "裙子"))
CASE "普段着：上半身上衣１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上衣１", "博麗の巫女服"))

CASE "固有指令"
	CALLF MAKE_STR(V_NAME, "神簽契約")
CASE "固有指令実行可能"
	CALLF MAKE_INT(V_NAME, !FLAG:神簽契約)

;ここは依頼IDが関連しない情報
;依頼発生率スカ2/一般3/固有5
CASE "依頼なし発生率"
	CALLF MAKE_INT(V_NAME, 2)
CASE "一般依頼発生率"
	CALLF MAKE_INT(V_NAME, 3)
CASE "固有依頼発生率"
	CALLF MAKE_INT(V_NAME, 5)
CASE "固有依頼最終要素"
	CALLF MAKE_INT(V_NAME, 103)
CASEELSE
	;固有依頼関連のデータにバトンタッチ
	SIF STRCOUNT(O_DATA, "依頼")
		CALLF 固有依頼1(ARG, O_DATA, V_NAME)
ENDSELECT



