		ENDIF
	;回復速度
	CASE 130
		IF ARG:1 == -1
			RETURNF "回復慢"
		ELSEIF ARG:1 == 1
			RETURNF "回復快"
		ENDIF
	CASE 143
		SELECTCASE ARG:1
			CASE 1
				RETURNF "動物耳"
			CASE 2
				RETURNF "ウサミミ"
			CASE 3
				RETURNF "イヌミミ"
			CASE 4
				RETURNF "ネコミミ"
		ENDSELECT
	;音楽知識
	CASE 161
		SELECTCASE ARG:1
			CASE 1
				RETURNF "楽器知識"
			CASE 2
				RETURNF "歌唱知識"
			CASE 3
				RETURNF "舞踏知識"
		ENDSELECT
	;音感
	CASE 162
		IF ARG:1 == -1
			RETURNF "音痴"
		ELSEIF ARG:1 == 1
			RETURNF "音感"
		ELSEIF ARG:1 == 2
			RETURNF "絶対音感"
		ENDIF
	;氷精
	CASE 164
		IF ARG:1 == 1
			RETURNF "氷精"
		ELSEIF ARG:1 == 2
			RETURNF "雪女"
		ENDIF
	;炮友
	CASE 185
		IF ARG:1 == 1
			RETURNF "炮友"
		ELSEIF ARG:1 == 2
			RETURNF "愛人"
		ENDIF
	;非生物
	CASE 192
		IF ARG:1 == 1
			RETURNF "非生物"
		ELSEIF ARG:1 == 2
			RETURNF "霊体"
		ENDIF
	;神霊
	CASE 193
		IF ARG:1 == 2
			RETURNF "死神"
		ELSEIF ARG:1 == 3
			RETURNF "閻魔"
		ENDIF
	;人間
	CASE 194
		IF ARG:1 == 2
			RETURNF "魔法使い"
