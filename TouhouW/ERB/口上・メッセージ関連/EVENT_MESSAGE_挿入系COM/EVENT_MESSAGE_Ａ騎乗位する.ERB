;-------------------------------------------------
;94,Ａ騎乗位
;-------------------------------------------------
@MESSAGE_COM94
;抜かずに
IF FLAG:70 == 1
	IF TCVAR:PLAYER:101 == TARGET && TARGET == TFLAG:103
		;対象が射精
		IF NOWEX:TARGET:射精
			PRINTFORM %CALLNAME:PLAYER%
			IF ABL:PLAYER:Ａ感覚 >= 3
				PRINTFORML 自己淫荡地扭起腰、贪婪索求着直腸传来的快楽
			ELSE
				PRINTFORML 努力的搖動起腰、奉仕着%CALLNAME:TARGET%的男性器
			ENDIF
			PRINTFORML 在時停中令%CALLNAME:TARGET%的%TINKO(TARGET)%达到了絶頂、将精液注入了%CALLNAME:PLAYER%的直腸…
			;調教者も射精
			SIF NOWEX:PLAYER:射精
				PRINTFORML 同時从%CALLNAME:PLAYER%的前端也迸発出了灼热的白濁液、在空中飘舞…
		;前ターンでＡ中出し
		ELSEIF TCVAR:PLAYER:104
			PRINTFORML %CALLNAME:PLAYER%并不想放过在時停中射过精的%CALLNAME:TARGET%的%TINKO(TARGET)%、再次開始晃起腰来
			PRINTFORML 満溢而出的精液和腸液把接合部位打湿了、扯出了粘糊糊的有点脏的白线…
		ELSE
			PRINTFORML %CALLNAME:PLAYER%并不想放过在時停中的%CALLNAME:TARGET%的%TINKO(TARGET)%、再次開始晃起腰来…
		ENDIF
	ELSE
		PRINTFORML %CALLNAME:PLAYER%让時停中的%CALLNAME:TARGET%仰面躺下并跨坐在了腰上
		SIF TEQUIP:PLAYER:0 & 2
			PRINTFORM 通过%SHOW_BOTTOM(PLAYER,2)%的間隙
		IF EXP:PLAYER:Ａ経験 >= EXPLV:4
			PRINTFORML 用已経変得柔软湿润的肛门呑下了%TINKO(TARGET)%
		ELSEIF EXP:PLAYER:Ａ経験 >= EXPLV:3
			PRINTFORML 小心的用張開的肛门呑下了%TINKO(TARGET)%
		ELSE
			PRINTFORML 用还很排斥異物的肛门呑下了%TINKO(TARGET)%
		ENDIF
		;ちんちん
		IF TALENT:TARGET:2 & 2
			PRINTFORM 散発着热气的%CALLNAME:TARGET%的%TINKO(TARGET)%
		;にせちん
		ELSE
			PRINTFORM 無機質的穿戴式假陰茎
		ENDIF
		PRINTFORML 侵犯腸壁的感覚、讓%CALLNAME:PLAYER%喘息不已…
	ENDIF
ELSEIF CFLAG:MASTER:イタズラ == 2
	IF TCVAR:PLAYER:101 == TARGET && TARGET == TFLAG:103
		;対象が射精
		IF NOWEX:TARGET:射精
			PRINTFORM %CALLNAME:PLAYER%
			IF ABL:PLAYER:Ａ感覚 >= 3
				PRINTFORML 自己淫荡地扭起腰、贪婪索求着直腸传来的快楽
			ELSE
				PRINTFORML 努力的搖動起腰、奉仕着%CALLNAME:TARGET%的男性器
			ENDIF
			PRINTFORML 在睡眠中%CALLNAME:TARGET%被侵犯的%TINKO(TARGET)%高潮了、将精液注入了%CALLNAME:PLAYER%的直腸…
			;調教者も射精
			SIF NOWEX:PLAYER:射精
				PRINTFORML 同時从%CALLNAME:PLAYER%的前端也迸発出了灼热的白濁液、在空中飘舞…
		;前ターンでＡ中出し
		ELSEIF TCVAR:PLAYER:104
			PRINTFORML %CALLNAME:PLAYER%并不想放过睡眠中的射過精的%CALLNAME:TARGET%的%TINKO(TARGET)%、再次開始晃起腰来
			PRINTFORML 満溢而出的精液和腸液把接合部位打湿了、扯出了粘糊糊的有点脏的白线…
		ELSE
			PRINTFORML %CALLNAME:PLAYER%并不想放过睡眠中的%CALLNAME:TARGET%的%TINKO(TARGET)%、再次開始晃起腰来…
		ENDIF
	ELSE
		PRINTFORML %CALLNAME:PLAYER%讓睡眠中的%CALLNAME:TARGET%仰面躺下跨在了腰上
		SIF TEQUIP:PLAYER:0 & 2
			PRINTFORM 通过%SHOW_BOTTOM(PLAYER,2)%的間隙
		IF EXP:PLAYER:Ａ経験 >= EXPLV:4
			PRINTFORML 用已経変得柔软湿润的肛门呑下了%TINKO(TARGET)%
		ELSEIF EXP:PLAYER:Ａ経験 >= EXPLV:3
			PRINTFORML 小心的用張開的肛门呑下了%TINKO(TARGET)%
		ELSE
			PRINTFORML 用还很排斥異物的肛门呑下了%TINKO(TARGET)%
		ENDIF
		;ちんちん
		IF TALENT:TARGET:2 & 2
			PRINTFORM 散発着热气的%CALLNAME:TARGET%的%TINKO(TARGET)%
		;にせちん
		ELSE
			PRINTFORM 無機質的穿戴式假陰茎
		ENDIF
		PRINTFORML 侵犯腸壁的感覚、讓%CALLNAME:PLAYER%喘息不已…
	ENDIF
ELSE
	IF TCVAR:PLAYER:101 == TARGET && TARGET == TFLAG:103
		;対象が射精
		IF NOWEX:TARGET:射精
			PRINTFORM %CALLNAME:PLAYER%
			IF ABL:PLAYER:Ａ感覚 >= 3
				PRINTFORML 自己淫荡地扭起腰、贪婪索求着直腸传来的快楽
			ELSE
				PRINTFORM 拼命扭動着腰、奉仕着%CALLNAME:TARGET%的
				IF TALENT:TARGET:2 & 2
					PRINTFORM %TINKO(TARGET)%
				;にせちん
				ELSE
					PRINTFORM 穿戴式假陰茎
				ENDIF
				PRINTL …
			ENDIF
			PRINTFORM 很快高潮了的%CALLNAME:TARGET%的那话儿一颤一颤的痙攣着、把精液注入了%CALLNAME:PLAYER%的直腸里
			;調教者も射精
			SIF NOWEX:PLAYER:射精
				PRINTFORM 同時从%CALLNAME:PLAYER%的前端也迸発出了灼热的白濁液、在空中飘舞
			PRINTFORML …
		;前ターンでＡ中出し
		ELSEIF TCVAR:PLAYER:104
			PRINTFORM %CALLNAME:PLAYER%圧在%CALLNAME:TARGET%身上緊緊地收縮着菊穴
			PRINTFORM 被溢出的精液和腸液弄湿的接合部、拉出了黏糊糊的帯着些許汚漬的白線…
		ELSE
			PRINTFORM %CALLNAME:PLAYER%
			IF TALENT:TARGET:2 & 2
				PRINTFORM %TINKO(TARGET)%
			;にせちん
			ELSE
				PRINTFORM 穿戴式假陰茎
			ENDIF
			PRINTFORML 紧缠住并、淫らに腰を振っている
		ENDIF
	ELSE
		PRINTFORM %CALLNAME:PLAYER%在%CALLNAME:TARGET%
		IF ABL:施虐属性 >= 3
			PRINTFORM 的命令下
		ELSE
			PRINTFORM 請求下
		ENDIF
		PRINTFORM 跨坐在%CALLNAME:TARGET%腰上、把
		IF TALENT:TARGET:2 & 2
			PRINTFORM %TINKO(TARGET)%
		;にせちん
		ELSE
			PRINTFORM 穿戴式假陰茎
		ENDIF
		PRINTFORML 的前端頂在了自己肛門上
		SIF TEQUIP:PLAYER:0 & 2
			PRINTFORM 通过%SHOW_BOTTOM(PLAYER,2)%的間隙
		IF EXP:PLAYER:Ａ経験 >= EXPLV:4
			PRINTFORM 帯着陶酔的表情用柔軟的肛門、像在軽咬一般把%CALLNAME:TARGET%的
			IF TALENT:TARGET:2 & 2
				PRINTFORM %TINKO(TARGET)%
			;にせちん
			ELSE
				PRINTFORM 穿戴式假陰茎
			ENDIF
			PRINTFORML 呑了進去
		ELSEIF EXP:PLAYER:Ａ経験 >= EXPLV:3
			PRINTFORM 慢慢地讓
			IF TALENT:TARGET:2 & 2
				PRINTFORM %TINKO(TARGET)%
			;にせちん
			ELSE
				PRINTFORM 穿戴式假陰茎
			ENDIF
			PRINTFORML 插入了自己已経張開的肛門
		ELSE
			PRINTFORML 完全不习惯異物的肛門被強行插入、讓%CALLNAME:MASTER%漏出了苦鳴
		ENDIF
		;ちんちん
		IF TALENT:TARGET:2 & 2
			PRINTFORM 散発着热气的%CALLNAME:TARGET%的%TINKO(TARGET)%
		;にせちん
		ELSE
			PRINTFORM 無機質的穿戴式假陰茎
		ENDIF
		PRINTFORML 侵犯腸壁的感覚、讓%CALLNAME:PLAYER%喘息不已…
	ENDIF
ENDIF