;==================================================
;デート帰りに告白（恋人が付く）
;==================================================
@M_KOJO_SPEVENT_K110_2(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;告白内容
	;この告白内容表示後に受けるかどうかの選択分が表示されます
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		;初回
		IF FIRSTTIME(1003)
			PRINTFORMW  「ねえ、%CALLNAME:MASTER%」
			PRINTFORMW  探女はおずおずと話を切り出してきた。
			PRINTFORMW  口に手を当てて数瞬考えこんだ後少しずつ口を開いた
			PRINTFORMW  「あの、ね」
			PRINTFORMW  「今日は、楽しかった」
			PRINTFORMW  「ありがとう」
			PRINTFORMW  「一つだけ、告白したいことがあるの」
			PRINTFORMW  
			PRINTFORMW  「%CALLNAME:MASTER%のことが、好き」
			PRINTFORMW  「%CALLNAME:MASTER%となら私は堕ちてもいい」
			PRINTFORMW  「射抜かれちゃったの、私の心」
			PRINTFORMW  「飛び立てない私に、主人公は翼をくれた」
			PRINTFORMW  「そして私を自由にしてくれたの」
			PRINTFORMW  「私はそらは蒼い物だと思っていたけれど」
			PRINTFORMW  「そらは広いものだと主人公は教えてくれた」
			PRINTFORMW  「もっと%CALLNAME:MASTER%と一緒に居たい」
			PRINTFORMW  「もっと%CALLNAME:MASTER%の事を知りたい」
			PRINTFORMW  「もっと%CALLNAME:MASTER%と色んな物を見てみたい」
			PRINTFORMW  「…」
			PRINTFORMW  「私では…駄目…？」
		;二回目の告白
		ELSE
			PRINTFORMW 「…しつこいかもしれないけど」
			PRINTFORMW 「好き…%CALLNAME:MASTER%のこと…」
			PRINTFORMW 「一度も嘘をつかないで」
			PRINTFORMW 「一度も後悔しない」
			PRINTFORMW 「そんな風に思えるようになったのも主人公に出逢ってから」
			PRINTFORMW 「お願い…貴方の側に居させて…」
			PRINTFORMW 探女は懇願するようにこちらを見つめている。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白オッケー（恋人が付く）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORMW 「…本当？」
		PRINTFORMW 「…本当に本当？」
		PRINTFORMW 「………」
		PRINTFORMW 「大好き」
		PRINTFORMW 探女はそう言って%CALLNAME:MASTER%に抱きついてきた。
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白失敗
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 「…」
		PRINTFORMW 「…分かった」
		PRINTFORMW 「…でも…」
		PRINTFORMW 「好き…だから…」
		PRINTFORMW 探女はそう言って去っていった。
		RETURN 1
	ENDIF

ENDIF
RETURN 1

;==================================================
;デート帰り
;==================================================
@M_KOJO_SPEVENT_K110_3(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;通常帰宅
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF CFLAG:318 == 1
			PRINTFORMW 「………」
			PRINTFORMW 探女は無言で去っていった。
		ELSEIF TALENT:恋人
			PRINTFORMW 「ん…じゃあね…」
			PRINTFORMW 「…また、明日」
			PRINTFORMW 探女ははにかんで別れの挨拶を告げた。
			PRINTFORMW （会える…よね…）
		ELSEIF TALENT:恋慕
			PRINTFORMW 「…じゃあね…」
			PRINTFORMW 「また、会いましょう」
			PRINTFORMW 探女はそう言って去っていった
			PRINTFORMW （会えたら…いいな…）
		ELSEIF TALENT:思慕
			PRINTFORMW 「今日はここまで」
			PRINTFORMW 「楽しかった…じゃあね…」
			PRINTFORMW 探女はそう言って去っていった
		ELSE
			PRINTFORMW 「それじゃ…さようなら」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印消去イベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		ELSEIF TALENT:恋人
			PRINTFORMW 「別に…」
			PRINTFORMW 「もう怒ってない」
			PRINTFORMW 「許す」
		ELSE
			PRINTFORMW 「怒ってない」
			PRINTFORMW 「これはホント」
		ENDIF
	RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印2>1レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:恋人
			PRINTFORMW 「いいけど、まだ怒ってる」
			PRINTFORMW 「まだ誠意を感じられない」
		ELSE
			PRINTFORMW 「わかった…」
			PRINTFORMW 「でもまだ怒ってる」
			PRINTFORMW 探女はまだ少し怒ってるようだ。
			PRINTW
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印3>2レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF TALENT:恋人
			PRINTFORMW 「………」
			PRINTFORMW 「少しだけ許してあげる」
		ELSE
			PRINTFORMW 「駄目…」
			PRINTFORMW 「許さない」
			PRINTFORMW 探女はそれでも少しだけ怒気を緩めた。
			PRINTW
		RETURN 1
	ENDIF
	PRINTFORML
ENDIF
RETURN 1

