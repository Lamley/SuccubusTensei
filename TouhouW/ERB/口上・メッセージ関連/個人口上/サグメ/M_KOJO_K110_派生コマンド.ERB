;==================================================
;5,手淫口交
;==================================================
@M_KOJO_MESSAGE_SCOM_K110_5
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K110_5_1
RETURN RESULT
@M_KOJO_MESSAGE_SCOM_K110_5_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
			;時姦中
			IF FLAG:70
				PRINTFORMW 凍てついた時の中で探女の口腔を犯し始めた。
			ELSE
				PRINTFORMW 「…んちゅ…れろ…」
				PRINTFORMW 「あ…っ…手…ベトベト…」
			ENDIF
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 凍てついた時の中で探女の手に陰茎を握らせながら口腔を犯し始めた。
	;恋慕
	ELSEIF TALENT:恋慕
		IF RAND:3 == 0
			PRINTFORMW 「ん…ちゅ…」
			PRINTFORMW 「…ん…出るの…？」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「っ…ん…」
			PRINTFORMW 「…れろ…」
		ELSE
			PRINTFORMW 「っく…じゅぶ…れろっ…」
			PRINTFORMW 「…ん…いっぱい…出して…%UNICODE(0x2665) *1%」
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		IF RAND:3 == 0
			PRINTFORMW 「あ…じゅる…じゅぶ…」
			PRINTFORMW 「…ん…れろっ…」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「じゅぶ…れろ…ん…」
			PRINTFORMW 「ん…恥垢…いっぱい…」
		ELSE
			PRINTFORMW 「…汚い…」
			PRINTFORMW 「…れろ…綺麗にしてあげる…%UNICODE(0x2665) *1%」
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「れろっ…じゅる…」
			PRINTFORMW 「んっ…不味い…」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「…れろっ…」
			PRINTFORMW 「…んっ…大きい…」
		ELSE
			PRINTFORMW 「…ん…れろっ…」
			PRINTFORMW 「じゅぶ…んっ…」
		ENDIF
	ENDIF
ENDIF
;未記入
;CALLFORM M_KOJO_MESSAGE_COM_K110_{SELECTCOM}_1
RETURN 1

