@M_KOJO_MESSAGE_COM_K40_300
;コマンド共通メッセージ(TRAIN_MESSAGE) 
;削除すると「～話をした。」とかが消えます
CALL TRAIN_MESSAGE
;口上メイン呼出
CALL M_KOJO_MESSAGE_COM_K40_300_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K40_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && !FLAG:70 
	IF 0
	;いちゃいちゃ話
	ELSEIF RAND:4 == 0 && TALENT:恋慕
		IF 0
		ELSEIF RAND:5 == 0
			PRINTFORMW 「お仕事お疲れ様%UNICODE(0x2665)%」
			PRINTFORMW 「私もね、此処の人たちにはお世話になっているけれど……」
			PRINTFORMW 「でも、私には……何もできないから……」
			PRINTFORMW 「だから、ね？」
			PRINTFORMW 「せめて%CALLNAME:MASTER%の疲れを癒してあげれば」
			PRINTFORMW 「それが%CALLNAME:MASTER%の仕事の潤滑油となって」
			PRINTFORMW 「此処の人たちへの恩返しにもなるって、そう思っているんだよ」
			PRINTFORMW 「だから……私に、させて……」
			PRINTFORMW 「%CALLNAME:MASTER%を癒す役目を……私に……」
			PRINTFORMW 「疲れも、気苦労も……全部、全部%CALLNAME:MASTER%のココロと共に吸いだす役目を」
			PRINTFORMW 「私に……果たさせて」
			PRINTFORMW 「私が……してあげたい%UNICODE(0x2665)%」
		ELSEIF RAND:4 == 0
			;恋人同士がすることといったら、えっちよりもキスよりも、これでしょう。
			PRINTFORMW 小傘と『あっち向いてホイ』をすることにした。
			PRINTL  
			CALL M_KOJO_MESSAGE_COM_K40_302_1_HOI
			;主人公が勝ち
			IF RESULT == 1
				PRINTFORMW 「あんっ、負けちゃったぁ」
			;小傘が勝ち
			ELSEIF RESULT == 2
				PRINTFORMW 「んふふー♪　私の勝ちぃ%UNICODE(0x2665)%」
			ENDIF
		ELSEIF RAND:3 == 0
			PRINTFORMW 「ねぇ……%CALLNAME:MASTER%？」
			PRINTFORMW 「その……大好き、だよ……」
			PRINTFORMW 「あっ……べ、別に驚かせるとかそんなつもりじゃなくて……」
			PRINTFORMW 「ただ……毎日にでも言ってあげたほうがいいと、思ったから……」
			PRINTFORMW 「ねぇ？　%CALLNAME:MASTER%は私のこと、好き？」
			PRINTFORMW 「できるなら……毎日でも聞きたい……%CALLNAME:MASTER%の好きって言葉、聞かせて欲しいの」
		ELSEIF RAND:2
			PRINTFORMW 「う～ん、どうしたら驚いて貰えるのかしら？」
			PRINTFORMW 「私の一体どこに驚かない要因があるっていうのよ」
			PRINTFORMW 「えっ？　やっぱり容姿が可愛らしいのが原因？」
			PRINTFORMW 「むぅ……そんなこと言われたら、もうこのままでいいやって……」
			PRINTFORMW 「人に驚いて貰えなくても、%CALLNAME:MASTER%が可愛いと言ってくれるこのままでいいやって」
			PRINTFORMW 「そう……思っちゃうよ……」
			PRINTFORMW 「ねぇ……？　%CALLNAME:MASTER%は、私の姿が今の姿から変わったら……嫌？」
			PRINTFORMW 「……」
			PRINTFORMW 「……そう、どんな姿でも、小傘は小傘……かぁ」
			PRINTFORMW 「そんな愛おしそうに唐傘を撫でながら言われたら、その言葉を疑うこともできないねぇ%UNICODE(0x2665)%」
		ELSE
			PRINTFORMW 「ねぇねぇ、私の唐傘、どう思う？」
			PRINTL  
			PRINTFORMW ――茄子みたいな不気味な色に時代遅れのマヌケな顔だ。
			PRINTL  
			PRINTFORMW 「あ、ひっどっーい」
			PRINTL  
			PRINTFORMW ――だがそれがいい！！
			PRINTL  
			PRINTFORMW 「えっ？」
			PRINTL  
			PRINTFORMW ――その時代遅れな顔がいい！！　それこそ古来より人を驚かせ心を糧にしてきた、妖怪の鑑ではござらぬか。
		ENDIF

	;場所の話
	ELSEIF RAND:3 == 0
		IF 0
		ELSE
			;場所別の会話は拘りすぎると本気で大変なので後回し
			CALL M_KOJO_MESSAGE_COM_K40_300_1_ETC
		ENDIF
	;季節と天気の話
	ELSEIF RAND:2
		;季節ごと汎用文
		IF RAND:3 == 0
			CALL M_KOJO_MESSAGE_COM_K40_300_1_SEASON
		;春
		ELSEIF DAY:2 == 1
			SELECTCASE TIME:5
			;晴天
			CASE 0, 1
			;外にいる
				IF K40_WHERE_ABOUT() == "室外"
					PRINTFORMW 「ん～♪　絶好の花見日和ね」
					PRINTFORMW 「そこで、どうかしら？　唐傘は両天傘なのよ？」
					PRINTFORMW 「ね、入らない？　ダメ？」
					PRINTL  
					PRINTFORMW 雨天ならともかくも、こうも天気がよくては紫の化け傘は目立って仕方がない。
					PRINTFORMW 滑稽な顔の下で花見をする男女……誰かに見られてはお笑い草だ。
					IF TALENT:恋人
						PRINTFORMW しかし、小傘と共に笑いの種になるならそれも……
						PRINTFORMW そんな考えが%CALLNAME:MASTER%の頭の片隅を過った……かもしれない。
					ELSE
						PRINTFORML 苦笑いを浮かべながら提案を辞退すると、
						PRINTFORMW やっぱり、といった表情を浮かべたが、すぐに周りの華溢れる景色に目を移していった。
					ENDIF
				;命蓮寺内にいる
				ELSE
					PRINTFORMW 「この頃はすっかり温かくなってきたね」
					PRINTFORMW 「なんだか部屋の中にまで、花の匂いが漂ってくるよう……」
					PRINTFORMW 「ん……いい香り」
				ENDIF
			;陰天
			CASE 2, 3
				;外にいる
				IF K40_WHERE_ABOUT() == "室外"
					PRINTFORMW 「なんだかパッとしない天気ね」
					PRINTFORMW 「春のにわか雨は激しいから、ちゃんと気を付けないとね」
					PRINTFORMW 「その点私がいれば安心！　なんなら今すぐにでも雨を呼んで唐傘の性能を……」
					PRINTFORMW 「あぁっ、待って…、冗談だからぐーはダメ、ぐーはしまって？　ねっ」
				;命蓮寺内にいる
				ELSE
					PRINTFORMW 「花陰天……ね」
					PRINTFORMW 「私、この季節の陰天空、嫌い」
					PRINTFORMW 「春雨の中傘を差して出かけていって」
					PRINTFORMW 「出かけ先で雨が上がって、持ち主に忘れられた傘たちが見上げるのは」
					PRINTFORMW 「いつもこんな天気……」
					PRINTFORMW 「……」
					PRINTFORMW 「なんてね、天気に悪気がないのは分かってる。ほんの冗談、だよ」
					PRINTFORMW 「持ち主が過ごしやすいと感じてくれるなら、私達もそれでいい」
				ENDIF
			;雨
			CASE 4, 5, 6
				;外にいる
				IF K40_WHERE_ABOUT() == "室外"
					PRINTFORMW 「『春雨じゃ、濡れて参ろう』」
					PRINTFORMW 「誰かが戯曲の中でそういったらしいね」
					PRINTFORMW 「でも折角私がいるんだし、%CALLNAME:MASTER%はそれやったら風邪ひくからやめておこうね」
					PRINTFORMW 「えっ、私？」
					PRINTFORMW 「私は妖怪だから別に濡れても平気……うん、平気……だし」
					PRINTFORMW 「そもそも傘の妖怪が雨の中傘を差さずにどうするのよっ」
				;命蓮寺内にいる
				ELSE
					PRINTFORMW 「春の雨といえば、草木を芽吹かせ花を咲かせる優しい雨と、人間は謳ってきたけれど」
					PRINTFORMW 「酷い時は激しい雷雨になるのもまたにわか雨としての一面なのよね……」
					PRINTFORMW 「私も、優しい雨は嫌いじゃないけれど」
					PRINTFORMW 「やっぱり、激しい雨風の方が性に合ってる気がするなぁ」
				ENDIF
			;霧
			CASE 7
				;外にいる
				IF K40_WHERE_ABOUT() == "室外"
					PRINTFORMW 「それにしてもすごい霧だね、一寸先も見えないってかんじ」
					PRINTFORMW 「雨風も雪も防げるけれど、霧ばかりはどうしようもないわ」
				;命蓮寺内にいる
				ELSE
					PRINTFORMW 「こんなにも深い霧の日は、参拝客を驚かせてやる絶好のチャンスだと思うの」
					PRINTFORMW 「白い霧に包まれ何もない所から誰かが出てきたら、それだけで腰を抜かすに違いないわ」
					PRINTFORMW 「だから……ね？　%CALLNAME:MASTER%も一緒に協力してくれる？」
					PRINTFORMW 「“危ないし、霧の中やってくる参拝客を無碍に出来ない”？　むぅ～」
				ENDIF
			;想定外・春汎用
			CASEELSE
				CALL M_KOJO_MESSAGE_COM_K40_300_1_SEASON
			ENDSELECT
		;夏
		ELSEIF DAY:2 == 2
			SELECTCASE TIME:5
			;晴天
			CASE 0, 1
				;外にいる
				IF K40_WHERE_ABOUT() == "室外"
					PRINTFORMW 「んっ、暑いねぇ～、日差しが眩しいねぇ……」
					PRINTFORMW 「かんかん照りの中を歩くのはさぞ辛いでしょう？」
					PRINTFORMW 「私の唐傘は何時でもウェルカムだよー」
				;命蓮寺内にいる
				ELSE
					PRINTFORMW 「部屋の中でも、やっぱり暑いものは暑いものね……」
					PRINTFORMW 「日差しは唐傘で防ぐことは出来ても、暑さは防げない……」
					PRINTFORMW 「熱気は遮ることはできても、蝉の鳴き声までは遮れない……」
					PRINTFORMW 「うー、あーつーいー」
				ENDIF
			;陰天
			CASE 2, 3
				;外にいる
				IF K40_WHERE_ABOUT() == "室外"
					PRINTFORMW 「夏の陰天空は過ごしやすい？」
					PRINTFORMW 「私は嫌よ……だって雨が降らないから傘を差せないし」
					PRINTFORMW 「日差しが無いから、やっぱり傘を差して貰えないんだもの」
				;命蓮寺内にいる
				ELSE
					PRINTFORMW 「日差しは弱くても、やっぱり暑いわ……蒸し蒸しするし」
					PRINTFORMW 「どうせならいっそ、大雨が降って欲しい……」
					PRINTFORMW 「ん、ごめんね。今のは傘の勝手な願望」
					PRINTFORMW 「人間サマの都合を考えない、傘の永遠の願い……だよ」
					PRINTL  
					PRINTFORMW しかし、雨が降れば確かに涼しくなるだろう……
				ENDIF
			;雨
			CASE 4, 5, 6
				;外にいる
				IF K40_WHERE_ABOUT() == "室外"
					PRINTFORMW 「梅雨の長雨、私、大好きよ」
					PRINTFORMW 「こうして人間の役に立てるんだもの」
					;恋人
					IF TALENT:恋人
						PRINTFORMW 「ううん、%CALLNAME:MASTER%だから、%CALLNAME:MASTER%の役に立てるから、だから幸せ」
						PRINTFORMW 「だからその、%CALLNAME:MASTER%も一緒に幸せになって欲しい」
						PRINTFORMW 「こうして雨の中、私と歩くのが幸せって、そう思ってもらえたら嬉しいな」
					;恋慕
					ELSEIF TALENT:恋慕
						PRINTFORMW 「しかも、大好きな%CALLNAME:MASTER%の役に立てる、%CALLNAME:MASTER%を雨風から守れる」
						PRINTFORMW 「だから、%CALLNAME:MASTER%は雨なんて嫌でしょうけれど、傘としての役割を全うさせてほしい」
						PRINTFORMW 「お願い、ね……」
					ELSE
						PRINTFORMW 「だからもう少し、化け傘の我儘に付き合って頂戴ね」
					ENDIF
				;命蓮寺内にいる
				ELSE
					PRINTFORMW 「雨が降ると、少し涼しくなるものね」
					PRINTFORMW 「私は……やっぱり雨の中外に行きたいなぁ……」
					;恋慕
					IF TALENT:恋慕
						PRINTFORMW 「%CALLNAME:MASTER%を連れ出して……ね♪」
					ENDIF
				ENDIF
			;霧
			CASE 7
				;外にいる
				IF K40_WHERE_ABOUT() == "室外"
					PRINTFORMW 「それにしてもすごい霧だね、一寸先も見えないってかんじ」
					PRINTFORMW 「雨風も雪も防げるけれど、霧ばかりはどうしようもないわ」
				;命蓮寺内にいる
				ELSE
					PRINTFORMW 「こんなにも深い霧の日は、参拝客を驚かせてやる絶好のチャンスだと思うの」
					PRINTFORMW 「白い霧に包まれ何もない所から誰かが出てきたら、それだけで腰を抜かすに違いないわ」
					PRINTFORMW 「だから……ね？　%CALLNAME:MASTER%も一緒に協力してくれる？」
					PRINTFORMW 「“危ないし、霧の中やってくる参拝客を無碍に出来ない”？　むぅ～」
				ENDIF
			;想定外・夏汎用
			CASEELSE
				CALL M_KOJO_MESSAGE_COM_K40_300_1_SEASON
			ENDSELECT
		;秋
		ELSEIF DAY:2 == 3
			SELECTCASE TIME:5
			;晴天
			CASE 0, 1
				;外にいる
				IF K40_WHERE_ABOUT() == "室外"
					PRINTFORMW 「わぁ……もみじが真っ赤に紅葉しててとても綺麗！」
					PRINTFORMW 「ねぇねぇ？　一面の紅葉景色の中に唐傘を差す少女、似合ってると思わない？」
				;命蓮寺内にいる
				ELSE
					PRINTFORMW 「このころはすっかり木々も色づいてきたよね」
					PRINTFORMW 「私の唐傘にももみじ柄を付けてみようかな……」
				ENDIF
			;陰天
			CASE 2, 3
				;外にいる
				IF K40_WHERE_ABOUT() == "室外"
					PRINTFORMW 「ううぅ～、木枯らし……寒い」
					PRINTFORMW 「最近寒くなってきたよね……こんな日光が差さない日なんか特に」
					PRINTFORMW 「%CALLNAME:MASTER%も風邪をひかないように気をつけてね」
				;命蓮寺内にいる
				ELSE
					PRINTFORMW 「外は寒いよ、人を驚かすのが嫌になりそう……」
					PRINTFORMW 「うー、がんばらなきゃ私っ！」
				ENDIF
			;雨
			CASE 4, 5, 6
				;外にいる
				IF K40_WHERE_ABOUT() == "室外"
					PRINTFORMW 「傘から出口ちゃだめよ？　冷たいんだから」
					PRINTFORMW 「ふっふっふ、秋の長雨……当分止まないわよ♪」
				;命蓮寺内にいる
				ELSE
					PRINTFORMW 「外は寒いよ、人を驚かすのが嫌になりそう……」
					PRINTFORMW 「本当は梅雨の次にかき入れ時なんだけれどねぇ」
				ENDIF
			;霧
			CASE 7
				;外にいる
				IF K40_WHERE_ABOUT() == "室外"
					PRINTFORMW 「よく回りが見えないなぁ……秋景色、結構楽しみにしてたのに」
					PRINTFORMW 「降り出さなきゃいいけれど……いやまぁ、私は嬉しいんだけれどね」
				;命蓮寺内にいる
				ELSE
					PRINTFORMW 「こんなにも深い霧の日は、参拝客を驚かせてやる絶好のチャンスだと思うの」
					PRINTFORMW 「白い霧に包まれ何もない所から誰かが出てきたら、それだけで腰を抜かすに違いないわ」
					PRINTFORMW 「だから……ね？　%CALLNAME:MASTER%も一緒に協力してくれる？」
					PRINTFORMW 「“危ないし、霧の中やってくる参拝客を無碍に出来ない”？　むぅ～」
				ENDIF
			;想定外・秋汎用
			CASEELSE
				CALL M_KOJO_MESSAGE_COM_K40_300_1_SEASON
			ENDSELECT
		;冬
		ELSE
			SELECTCASE TIME:5
			;晴天
			CASE 0, 1
				;外にいる
				IF K40_WHERE_ABOUT() == "室外"
					PRINTFORMW 「はぁ、日差しが暖かい……」
					PRINTFORMW 「日向を歩こうよ。少しでも暖かい方がいいでしょ」
				;命蓮寺内にいる
				ELSE
					PRINTFORMW 「私の恰好は寒そうに見える？」
					PRINTFORMW 「これでもちゃんと冬服なのー！　太腿の見過ぎじゃないかしら？」
				ENDIF
			;陰天
			CASE 2, 3
				;外にいる
				IF K40_WHERE_ABOUT() == "室外"
					PRINTFORMW 「寒いね……雪も降り出しそう」
					;恋慕
					IF TALENT:恋慕
						PRINTFORMW 「ほら……私の体をどうしたい？」
						PRINTFORMW 「ふふっ、%CALLNAME:MASTER%がどう思っていても、私は%CALLNAME:MASTER%の体を抱き寄せたいの♪」
					;恋慕でない
					ELSE
						PRINTFORMW 「私の体……抱き寄せたい？　じゃあちょっとだけ、ね♪」
					ENDIF
				;命蓮寺内にいる
				ELSE
					PRINTFORMW 「はぁ……寒い」
					PRINTFORMW 「でも此処は……博麗神社は外より暖かいね」
					;恋慕
					IF TALENT:恋慕
						PRINTFORMW 「そして此処はもっと……ふふふ」
						PRINTL  
						PRINTFORMW そう言って、小傘は%CALLNAME:MASTER%に擦り寄り、そっと抱きしめてきた。
					ENDIF
				ENDIF
			;霧
			CASE 7
				;外にいる
				IF K40_WHERE_ABOUT() == "室外"
					PRINTFORMW 「深い霧ね……雪は降ってないのだけれど」
					PRINTFORMW 「なんだかいつもより寒さが凍みるなぁ」
				;命蓮寺内にいる
				ELSE
					PRINTFORMW 「わぁ……外の景色が真っ白！」
					PRINTFORMW 「雪が降っているのか霧だけなのか分からないね」
				ENDIF
			;雪
			CASE 8, 10, 11, 12
				;外にいる
				IF K40_WHERE_ABOUT() == "室外"
					PRINTFORMW 「雪で傘が潰れてしまわないように、定期的に回さないとね」
					PRINTFORMW 「くるくるくる～っと」
				;命蓮寺内にいる
				ELSE
					PRINTFORMW 「外を見て……雪が降ってる、静かだね～」
					PRINTFORMW 「なんだか部屋の中の音まで外に吸い込まれていきそう」
				ENDIF
			;吹雪
			CASE 9
				;外にいる
				IF K40_WHERE_ABOUT() == "室外"
					PRINTFORMW 「んぁ…、凄い雪……傘が潰れちゃいそう」
					PRINTFORMW 「あまり持たないよ……早く非難しましょ？」
				;命蓮寺内にいる
				ELSE
					PRINTFORMW 「凄い雪ねぇ……びっくり」
					PRINTFORMW 「今日は誰かを驚かせに行くのはやめ、ね」
					
				ENDIF
			;霰
			CASE 13
				;外にいる
				IF K40_WHERE_ABOUT() == "室外"
					PRINTFORMW 「酷い霰……唐傘、破れないかしら？」
					PRINTFORMW 「頑丈なのが自慢なんだけれど、音が痛々しくてねぇ」
				;命蓮寺内にいる
				ELSE
					PRINTFORMW 「霙……凄いねぇ」
					PRINTFORMW 「屋根を打つ音がすごくて……破けてしまわないか心配だなぁ～」
				ENDIF
			;想定外・冬汎用
			CASEELSE
				CALL M_KOJO_MESSAGE_COM_K40_300_1_SEASON
			ENDSELECT
		ENDIF
	;其他・一般会話
	ELSE
		;後々、条件分岐に失敗するなどして代替案に用いることができるように、関数化。
		CALL M_KOJO_MESSAGE_COM_K40_300_1_ETC
	ENDIF
ENDIF
RETURN 1
;季節毎汎用会話文
@M_KOJO_MESSAGE_COM_K40_300_1_SEASON
;工事中
CALL M_KOJO_MESSAGE_COM_K40_300_1_ETC
RETURN 1

