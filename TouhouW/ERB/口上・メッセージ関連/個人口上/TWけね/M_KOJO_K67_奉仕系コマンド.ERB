	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中(FLAG:70)
	IF FLAG:70
		IF MARK:反発刻印 == 3
			;射精(ex:11)
			IF NOWEX:PLAYER:11
				PRINTFORMW 動かない慧音から冷ややかな視線を浴びながら限界を迎えた%CALLNAME:MASTER%は、
				PRINTFORMW 履物と慧音の足の間にたっぷりと精を放った
				PRINTFORMW 彼女の体勢を元街道にすると、履物の中でぐしゅりと湿った音が響いた
			ELSE
				PRINTFORMW %CALLNAME:MASTER%は動かない慧音の足を使って自慰を始めた
				PRINTFORMW 意識すらないのにも関わらず明確な敵意を孕んだ視線が心地良い
			ENDIF
			RETURN 1
		;射精(ex:11)
		ELSEIF NOWEX:PLAYER:11
			SELECTCASE RAND:2
			CASE 0
				PRINTFORMW あっという間に限界を迎えた%CALLNAME:MASTER%は、慧音の滑らかな脚に精を放った
				PRINTFORMW ただでさえ白く美しい慧音の脚が、白濁でさらに彩られていく
			CASE 1
				PRINTFORMW 程なくして%CALLNAME:MASTER%は慧音の土踏まずの間に射精した
				PRINTFORMW 射精の間も無機質かつ絶妙な圧迫感があり、腰が砕けそうだ
			ENDSELECT
			RETURN 1
		ELSE
			SELECTCASE RAND:2
			CASE 0
				PRINTFORMW がに股にさせ、両方の土踏まずの間にペニスを挟み込んだ
				PRINTFORMW なかなか身体が硬い様で、少々体勢作りに苦労した
				PRINTFORMW 筋を少し痛めてしまったかもしれない
			CASE 1
				PRINTFORMW 履物を脱がせて、足指の間にペニスを挟み込んだ
				PRINTFORMW 真顔なのが妙にしっくりくる
			ENDSELECT
			RETURN 1
		ENDIF
	ELSEIF TALENT:恋人
		;射精(ex:11)
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「ん、まだイくなよ」
				PRINTFORML 射精寸前になった途端、慧音が口を開く
				PRINTFORML 「先生が『良し』って許可出すまで我慢だ、できるだろ？」
				PRINTFORMW %CALLNAME:MASTER%は歯を食いしばって耐えるが、激しくなる責めに数秒で限界を迎えた
				PRINTFORML 「おっと」
				PRINTFORML 耐え切れず腰が跳ねた瞬間、慧音は器用に尿道口を足指で塞いでしまった
				PRINTFORML もう片足は依然竿を扱いたままだ
				PRINTFORML 恐怖を覚えるほどの責めに悲鳴を上げる%CALLNAME:MASTER%
				PRINTFORML 「はぁ……どーして先生の言うことが聞けないのかなー……？」
				PRINTFORMW 力を振り絞って許しを請うと、嗜虐に濁った瞳と目が合った
			CASE 1
				PRINTFORMW 「あれ？足で満足してしまったのか」
				PRINTFORMW 「せっかく畑の準備はできていたのに……種蒔きはお預けだな、ふふ」
			CASE 2
				PRINTFORMW 「ほら、もっと出せるだろ？」
				PRINTFORMW 射精の瞬間、ペニスにかかる体重が増す
				PRINTFORMW 「どうしたどうした、あんなに孕ませたがっていた先生の子宮は目の前だぞ？」
				PRINTFORMW 自ら入口を見せびらかしながら、巧みな足技で精を搾り取っていく
			ENDSELECT
			RETURN 1
		ELSE
			SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「え？痛い？」
				PRINTFORMW 「演技が下手だな%CALLNAME:MASTER%は……仕方ないな、もっと強くしてやる」
			CASE 1
				PRINTFORMW 「教師に足蹴にされたいだなんて、随分とねじ曲がった性癖を持ったものだ」
				PRINTFORMW 「……人のことは言えないけどね、ふふ」
			ENDSELECT
			RETURN 1
		ENDIF
	ELSEIF TALENT:恋慕
		;射精(ex:11)
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「……気持ち良さそうだな、良かった」
				PRINTFORMW 指導の甲斐あって、慧音の足捌きもなかなかのものになってきた
				PRINTFORMW 彼女も楽しんでいるのか、射精してもなかなか足を止めてくれない
				PRINTFORMW 半ば無理矢理、すぐに勃起させられてしまう
			CASE 1
				PRINTFORMW 「あっ……」
				PRINTFORMW 「……あったかい……」
				PRINTFORMW 熱に浮かされたような表情で白濁にまみれた足を見つめている
			ENDSELECT
			RETURN 1
		ELSE
			SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「ん……イきそう、なのか？」
				PRINTFORML %CALLNAME:MASTER%の表情から限界を察した慧音は、ペニスから足を離してしまう
				PRINTFORMW たまらず腰を浮かせて追いかけると、同じだけ逃げていく
				PRINTFORMW 息を荒げて懇願すると、彼女の口元が緩んでいるのが見えた
				PRINTFORMW 施虐狂の気があるのかもしれない
			CASE 1
				PRINTFORMW 股を広げて足での奉仕を命じられた慧音
				PRINTFORMW とろりと秘唇から愛液が垂れたのが見えた……気がする
			ENDSELECT
			RETURN 1
		ENDIF
	ELSEIF MARK:不埒刻印 == 3
		;射精(ex:11)
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「……なんか複雑だな」
				PRINTFORMW 「滅茶苦茶されるよりはマシか……」
			CASE 1
				PRINTFORMW 「あっ……」
				PRINTFORMW 「踏まれてイったのか……？」
			ENDSELECT
			RETURN 1
		ELSE
			SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「簡単に命令するけどな、意外と難しいんだぞ」
				PRINTFORMW 「普段こんなことしないし……」
			CASE 1
				PRINTFORMW 「踏んでほしいって……ここ、大事な場所なんじゃないのか」
				PRINTFORMW 「どうなっても責任とれないからな」
			ENDSELECT
			RETURN 1
		ENDIF
		RETURN 1
	ELSE
		PRINTFORMW 「足で？」
		PRINTFORMW 「踏み潰せばいいのか？違う？」
		RETURN 1
	ENDIF
ENDIF
RETURN 1
