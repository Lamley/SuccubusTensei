		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中(FLAG:70)
	IF FLAG:70
		IF MARK:反発刻印 == 3
			;射精(ex:11)
			IF NOWEX:PLAYER:11
				PRINTFORMW 限界を迎える直前、慧音の喉奥にペニスを突き込む
				PRINTFORMW 食道へ直接精液を流し込み、ゆっくり引き抜き口内もしっかり白濁で満たす
				PRINTFORMW こちらを鋭く睨みながら口から精液を垂らす様子はなかなかに滑稽だった
			ELSE
				PRINTFORMW 固い表情の慧音の口にペニスを押し込んだ
				PRINTFORMW かなりの抵抗があったが、オナホとしては丁度いい程度だ
			ENDIF
			RETURN 1
		;射精(ex:11)
		ELSEIF NOWEX:PLAYER:11
			PRINTFORMW ぴくりとも動かない慧音の口にたっぷりと射精した
			PRINTFORMW ペニスを引き抜くと、こぽりと小さく泡がたった
			RETURN 1
		ELSE
			SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 時間の止まった慧音の口にペニスを押し込んだ
				PRINTFORMW 腰をゆすると、端正な顔立ちが蹂躙されていく
			CASE 1
				PRINTFORMW 明後日の方向を見つめている慧音の口で自慰を始めた
				PRINTFORMW 静寂の中、かき出された唾液がじゅぷじゅぷと水音を立てる
			ENDSELECT
			RETURN 1
		ENDIF
	ELSEIF TALENT:恋人
		;射精(ex:11)
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「んっ……っ……」
				PRINTFORMW こくっ……こくり
				PRINTFORMW 艶やかな慧音の喉から卑猥な音が響く
				PRINTFORMW 「……ん……ふっ……じゅぷっじゅるる」
				PRINTFORMW うっとりした表情で精液を嚥下すると、そのまま離さずに口淫を再開してしまった
			CASE 1
				PRINTFORMW 「っ……ふぅ……」
				PRINTFORMW 「%CALLNAME:MASTER%……先生、まだ全然足りないぞ……？」
				PRINTFORMW 「……けぷっ」
			ENDSELECT
			RETURN 1
		ELSE
			SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「いつもながらご立派なことだ」
				PRINTFORMW 「『この後』に備えて、しっかり準備しておかないとな……ふふ」
			CASE 1
				PRINTFORMW 「匂いだけでクラクラしてくるな」
				PRINTFORMW 「えへへ、いただきまぁす……」
			CASE 2
				PRINTFORMW 「ほら、『先生、咥えて下さい』ってお願いしなよ」
				PRINTFORMW 「今ならた～～っぷりとご褒美をやるぞ……？」
			ENDSELECT
			RETURN 1
		ENDIF
	ELSEIF TALENT:恋慕
		;射精(ex:11)
		IF NOWEX:PLAYER:11
				PRINTFORMW 「ん……っ」
				PRINTFORMW 射精と共にペニスを深く咥え、そのまま慧音の動きが止まる
				PRINTFORMW 食道への射精が収まると、モノを咥えたまま何かを訴える様にこちらを見つめてきた
				PRINTFORMW 彼女の伝えたいことはわからなかったが、ペニスは温かい口内で再び硬度を取り戻していた
			RETURN 1
		ELSE
			SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 目の前にペニスをもってくると、自然に奉仕を始めた
				PRINTFORMW だいぶ積極的になった様に感じるが、気のせいだろうか
			CASE 1
				PRINTFORMW こちらの様子を上目遣いで伺ってくる慧音
				PRINTFORMW 舌の動きそのものは控え目だが、しっかりと奉仕してくれている
			ENDSELECT
			RETURN 1
		ENDIF
	ELSEIF MARK:不埒刻印 == 3
		;射精(ex:11)
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「んっ……くふっ」
				PRINTFORMW 「出すなら出すって言えよ……服が汚れるじゃないか」
			CASE 1
				PRINTFORMW 「うえぇ……」
				PRINTFORMW 「確かに、前よりは慣れたけどさ……」
			ENDSELECT
			RETURN 1
		ELSE
			SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「顔が赤いって？」
				PRINTFORMW 「当たり前だろ、こんなモノ目の前に出されたら誰だって赤くなるよ」
			CASE 1
				PRINTFORMW 「うう……一日食欲なくなるからやりたくないんだ」
				PRINTFORMW 「わかった、わかったから押し付けないで！」
			ENDSELECT
			RETURN 1
		ENDIF
		RETURN 1
	ELSE
		PRINTFORMW 「はぁ……」
		PRINTFORMW 「やればいいんだろ、やれば」
		RETURN 1
	ENDIF
ENDIF
RETURN 1

