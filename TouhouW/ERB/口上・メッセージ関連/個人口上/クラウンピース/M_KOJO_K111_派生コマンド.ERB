;==================================================
;5,手淫口交
;==================================================
@M_KOJO_MESSAGE_SCOM_K111_5
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K111_5_1
RETURN RESULT
@M_KOJO_MESSAGE_SCOM_K111_5_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
;口交と同じ
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF
	IF FLAG:時間停止
		RETURN 1
	ELSEIF NOWEX:MASTER:11
		SELECTCASE RAND:2
		CASE 0
			PRINTFORMW 「んっ%UNICODE(0x2764)% んっ%UNICODE(0x2764)% ぷぁっ……イキそうなのね」
			PRINTFORMW 「いいわよ、おくちに出して%UNICODE(0x2764)% んっんっ%UNICODE(0x2764) * 2% ん～～～っっ%UNICODE(0x2764)%」
		CASE 1
			PRINTFORMW 「んっ%UNICODE(0x2764)% んっ%UNICODE(0x2764)% あっ、汁がいっぱい出てきた……%UNICODE(0x2764)%」
			PRINTFORMW 「そろそろイきそう？ じゃあこのまま……んぐっ、ぐっ、じゅっ、ん～～っっ%UNICODE(0x2764)%」
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 「先っちょ、きもちいい？%UNICODE(0x2764)% ぺろぺろ%UNICODE(0x2764)%」
			PRINTFORMW 「あむっ…れろれろ……じゅるっ%UNICODE(0x2764)%」
		CASE 1
			PRINTFORMW 「あーん%UNICODE(0x2764)%」
			PRINTFORMW 「れろれろ……じゅぽっじゅぽっ%UNICODE(0x2764)% ひもひいい？ れろれろれろ……%UNICODE(0x2764)%」
		CASE 2
			PRINTFORMW 「んむっ、ちゅ…じゅるる……%UNICODE(0x2764)%」
			PRINTFORMW 「%CALLNAME:MASTER%のおちんちん、おいひいよ…%UNICODE(0x2764)%」
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF
