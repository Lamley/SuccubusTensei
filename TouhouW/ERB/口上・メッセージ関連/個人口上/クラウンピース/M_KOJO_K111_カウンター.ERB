	LOCALS = ぴょっこりおちんぽ
ELSEIF TALENT:MASTER:形状 == 2
	LOCALS = へなちょこおちんぽ
ELSEIF TALENT:MASTER:形状 == 4
	LOCALS = フラッシュおちんぽ
ELSEIF TALENT:MASTER:形状 == 5
	LOCALS = 凶悪おちんぽ
ELSEIF TALENT:MASTER:形状 == 6
	LOCALS = ごんぶとおちんぽ
ELSE
	IF TALENT:MASTER:濃厚精液
		LOCALS = 濃厚おちんぽ
	ELSE
		LOCALS = おちんぽ
	ENDIF
ENDIF
RETURNF LOCALS

