;※※※※※※※※※※※※※※※※※※※※※※※※※※
;"EVENT"系イベント
;※※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;部屋で遭遇
;==================================================
@M_KOJO_EVENT_K111_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1 && !TCVAR:111:今日会ったか
		;ハロウィン
		IF DAY:2 == 3 && DAY:3 == 30
			PRINTFORMW 「トリック ☆ オア ☆ トリート！！」
			PRINTFORMW 「お菓子ちょーだいっ♪」
			PRINTFORMW 「それとも%CALLNAME:MASTER%はイタズラの方がいいのかな～？」
			PRINTFORMW 「……どっちもじゃダメ？」
		;心情不快
		ELSEIF TALENT:111:心情 == -1 || CFLAG:111:ブチギレ
			PRINTFORMW 「あ、%CALLNAME:MASTER%……」
		;それ以外
		ELSE
			PRINTFORML 「あ、%CALLNAME:MASTER%！」
			;月
			IF CFLAG:111:現在位置 >= 1000 && CFLAG:111:現在位置 < 1100
				;恋人
				IF TALENT:111:恋人
					PRINTFORMW 「会いに来てくれたの！？」
					PRINTFORMW 「わーい♪ %CALLNAME:MASTER%大好きー%UNICODE(0x2764)%」
				ELSE
					PRINTFORMW 「なにしにきたの？」
					PRINTFORMW 「せっかくだし遊ぼうよー！」
				ENDIF
			;10日以上会ってない
			ELSEIF DAY - CFLAG:111:1002 >= 10
				;恋人or恋慕
				IF TALENT:111:恋人 || TALENT:111:恋慕
					PRINTFORMW 「最近見かけなかったから寂し…じゃなくて、心配だったよ！」
					PRINTFORMW 「ねえ、ぎゅってしていい……？」
				ELSE
					PRINTFORMW 「久しぶりだね、元気してた？」
				ENDIF
			;？
			ELSEIF !RAND:100
				PRINTFORML 「あたっ！！」
				PRINTFORMW 克労恩皮絲は駆け寄ってこようとして盛大にすっ転んだ
			ELSE
				SELECTCASE RAND:4
				CASE 0
					PRINTFORMW 「およ？ ほっぺに飯粒が付いてるよっ！」
				CASE 1
					PRINTFORMW 「今日は何して遊ぼうかな～」
				CASE 2
					PRINTFORMW 「いまひま？ ねえひま？？」
				CASEELSE
					PRINTFORMW 「ねえねえ遊ぼー！」
				ENDSELECT
			ENDIF
		ENDIF
		;バレンタインチョコ
		IF DAY:2 == 4 && DAY:3 == 14
			SELECTCASE CFLAG:111:好感度
				CASE 1000 TO 3000
					;黑雷神巧克力
					PRINTFORMW 「はいこれ！」
				CASE 3001 TO 5000
					;巧克力
					PRINTFORMW 「はいこれ、巧克力！」
				CASE IS > 5000
					IF EXP:ARG:愛情経験 < 100
						;高級巧克力
						PRINTFORMW 「はいこれ、ハッピ～バレンタイ～ン！」
					ELSE
						;手作りチョコ
						PRINTFORMW 「はいこれ！ ご主人様と一緒に作ってきたの！」
						PRINTFORMW 「ハッピ～バレンタインっ☆」
					ENDIF
			ENDSELECT
		ENDIF
		GOTO FINISH
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2 && !TCVAR:111:今日会ったか
		;10日以上会ってない
		IF DAY - CFLAG:111:1002 >= 10
			PRINTFORMW 「あ！ %CALLNAME:MASTER%！」
			;恋人or恋慕
			IF TALENT:111:恋人 || TALENT:111:恋慕
				PRINTFORMW 「最近見かけなかったから寂し…じゃなくて、心配だったよ！」
			ELSE
				PRINTFORMW 「久しぶりだね、元気してた？」
			ENDIF
		;心情不快
		ELSEIF TALENT:111:心情 == -1
			PRINTFORMW 「ふーんふーん」
		;それ以外
		ELSE
			SELECTCASE RAND:4
			CASE 0
				PRINTFORMW 「やっほー、お散歩してたの！」
			CASE 1
				PRINTFORMW 「やっほー、なにしてたの？」
			CASE 2
				PRINTFORMW 「やっほー、あ、お取り込み中だった？」
			CASEELSE
				PRINTFORMW 「やっほー、きちゃった」
			ENDSELECT
		ENDIF
		GOTO FINISH
	ENDIF
	;-------------------------------------------------
	;TARGETが浴室に入ってきて一緒に入る
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	;不知羞恥と大体入ってくる
	IF LOCAL:1 && ARG == 3
		PRINTFORML 「あ！ %CALLNAME:MASTER%入ってたの！」
		PRINTFORMW 「えへ\/\/\/ じゃあ一緒に入ろっ\/\/\/」
		GOTO FINISH
	ENDIF
	;-------------------------------------------------
	;TARGETが浴室に入ってきて出て行った
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORML 「あ！ %CALLNAME:MASTER%入ってたの！」
		PRINTFORMW 「ごめんねー、お邪魔しましたー」
		GOTO FINISH
	ENDIF
	;-------------------------------------------------
	;TARGETが浴室に入ってきて追い出された
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORML 「あ！ %CALLNAME:MASTER%入ってたの！」
		PRINTFORMW 「ふふーん♪ あたいが入るから%CALLNAME:MASTER%は出てってね♪」
		GOTO FINISH
	ENDIF
	;-------------------------------------------------
	;外出先で遭遇した
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		PRINTFORML 「あ！ %CALLNAME:MASTER%！」
		IF TALENT:111:恋慕
			SELECTCASE RAND:4
			CASE 0
				PRINTFORMW 「こんなところで会うとは奇遇ね！」
			CASE 1
				PRINTFORMW 「ねえねえ、一緒にどこか行かない？」
			CASE 2
				PRINTFORMW 「ひといでおでかけ？ あたいと遊ぼうよ～」
			CASEELSE
				PRINTFORMW 「遊ぼっ遊ぼっ♪」
			ENDSELECT
		ELSE
			PRINTFORMW 「ねえねえ、なにしてるのー？」
		ENDIF
		GOTO FINISH
	ENDIF
	;-------------------------------------------------
	;他キャラとデート中に遭遇した
	;ARG:1にデート相手
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		PRINTFORML 「あ！ %CALLNAME:MASTER%！」
		IF TALENT:111:恋慕
			;赫喀提亜とデート
			IF FLAG:デート相手 == 113
				PRINTFORML 「ご、ご主人様……！」
				PRINTFORMW 「えっと、あの、あたいはここに居ても良いですか……？」
			;純狐とデート
			ELSEIF FLAG:デート相手 == 112
				PRINTFORML 「ゆ、友人様……！」
				PRINTFORMW 「えっと、あの、あたいはここに居ても良いですか……？」
			;妖精とデート
			ELSEIF TALENT:(FLAG:デート相手):妖精
				PRINTFORMW 「なによそいつ、仲良いの？」
			;それ以外
			ELSE
				PRINTFORMW 「むっ、だーれその娘？」
			ENDIF
		ELSE
			;赫喀提亜とデート
			IF FLAG:デート相手 == 113
				PRINTFORML 「ご主人様も！」
				PRINTFORMW 「お二人でなにしてたんですか？」
			;純狐とデート
			ELSEIF FLAG:デート相手 == 112
				PRINTFORML 「友人様も！」
				PRINTFORMW 「お二人でなにしてたんですか？」
			ENDIF
			PRINTFORMW 「ねえねえ、なにしてるのー？」
		ENDIF
		GOTO FINISH
	ENDIF
ENDIF
$FINISH
;最後に会った日
CFLAG:111:1002 = DAY
RETURN 1


;==================================================
;朝起きたとき（おはよう口上）
;==================================================
@M_KOJO_EVENT_K111_2(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;非同棲かつ相手が先に起きてる
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;TARGET私室にMASTERが入っている（恋慕時添い寝中就寝時のみ？
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTL 
			PRINTFORMW 「おはよう！ よく眠れた？」
			SELECTCASE RAND:7
			CASE 0
				PRINTFORML 「寝言で女の子の名前呼んでたよ？」
				PRINTFORMW 「どういうことかな？ うん？」
				TALENT:111:心情 = -1
			CASE 1, 2
				PRINTFORML 「ふふ♪ %CALLNAME:MASTER%ったらニヤニヤしながら寝てるんだもの」
				PRINTFORMW 「一体どんな夢を見たのかしら？」
			CASE 3, 4
				PRINTFORML 「クスクス……きゃははは♪」
				PRINTFORMW 「ごめんね、お顔に落書きしちゃったの！」
			CASEELSE
				;昨晩満足
				IF CFLAG:111:1001 == DAY - 1
					PRINTFORML 「えへ%UNICODE(0x2764)% 昨日はいっぱい愛でてもらっちゃった♪」
					PRINTFORMW 「また今度えっちしようね♪」
					TALENT:111:心情 = 1
				ELSE
					PRINTFORMW 「ふふ♪ %CALLNAME:MASTER%の寝顔おもしろかったよ♪」
				ENDIF
			ENDSELECT
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTER私室にTARGETが入ってきてる(恋慕無しでも入ってくる可能性有り
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ相手が起きている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ相手が寝ている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERが起きていて、相手が起きてきた
	;添い寝時TARGET起床はこちら（TARGET衰弱or添い寝中TARGET睡魔負け後そのままMASTERが起き続けた上で相手が起床した場合限定
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
