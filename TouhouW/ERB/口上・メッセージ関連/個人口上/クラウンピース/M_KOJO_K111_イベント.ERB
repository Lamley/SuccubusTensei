			RETURN 1
		ENDIF
	CASE 1
		;-------------------------------------------------
		;受けた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTFORMW 「わーい♪ %CALLNAME:MASTER%やさしー♪」
			RETURN 1
		ENDIF
	CASE 2
		;-------------------------------------------------
		;断った
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTFORML 「ぶー、いじわる」
			PRINTFORMW 「ま、いっか♪ じゃあ今度ね♪」
			RETURN 1
		ENDIF
	CASE 3
		;-------------------------------------------------
		;部屋に誘われた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTFORML 「ねえねえ、今日はあたいのうちに来ない？」
			IF FIRSTTIME("お招き", 1)
				PRINTFORML 「今日は両親がいないのよ！」
				PRINTFORMW 「……いつもいないけど」
