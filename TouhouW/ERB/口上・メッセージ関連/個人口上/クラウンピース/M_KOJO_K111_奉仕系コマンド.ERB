			PRINTFORMW 「出そうなんでしょ%UNICODE(0x2764)% ほらっ%UNICODE(0x2764)% イっちゃえ%UNICODE(0x2764)% イっちゃえ%UNICODE(0x2764)%」
		CASE 1
			PRINTFORMW 「あらー？ もしかして手でイっちゃうのー？」
			PRINTFORMW 「ふふっ%UNICODE(0x2764)% ほらっ%UNICODE(0x2764)% イっていいよっ%UNICODE(0x2764)% イって%UNICODE(0x2764)% イって%UNICODE(0x2764)%」
		ENDSELECT
	ELSE
		SELECTCASE RAND:2
		CASE 0
			PRINTFORMW 「どう？きもちいい？」
			PRINTFORMW 「いいよ%UNICODE(0x2764)% いっぱいきもちよくなって%UNICODE(0x2764)% ほらっ%UNICODE(0x2764)% ほらほらっ%UNICODE(0x2764)%」
		CASE 1
				PRINTFORMW 「きもちいいんでしょ%UNICODE(0x2764)%」
				PRINTFORMW 「我慢しないのー%UNICODE(0x2764)% えいっ%UNICODE(0x2764)% えいっ%UNICODE(0x2764)%」
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF
RETURN 1

