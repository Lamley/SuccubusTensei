@M_KOJO_MESSAGE_COM_K111_1_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM) && !FLAG:時間停止
		PRINTFORMW 「ふぇぇ……そこは汚いよう……」
		PRINTFORMW 「あっ！！ あっ、あっ%UNICODE(0x2764)% だめっ……%UNICODE(0x2764) * 2%」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;膣内射精直後
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:100 == PLAYER
		IF TCVAR:102
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;絶頂した
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && NOWEX:0
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:時間停止
		RETURN 1
	;継続舐陰
	ELSEIF PREVCOM == SELECTCOM
		SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 「はわわぁっ%UNICODE(0x2764)% あっ%UNICODE(0x2764)% あああっ%UNICODE(0x2764) * 2%」
			PRINTFORMW 「もっと%UNICODE(0x2764)% もっと%UNICODE(0x2764)% きもちい～～っ%UNICODE(0x2764) * 2%」
		CASE 1
			PRINTFORMW 「あっ%UNICODE(0x2764)% もっと%UNICODE(0x2764)% あんっ%UNICODE(0x2764)%」
			PRINTFORMW 「はげしくぅっ%UNICODE(0x2764)% ぺろぺろしてっ%UNICODE(0x2764) * 2%」
		CASE 2
			PRINTFORMW 「%CALLNAME:MASTER%っ%UNICODE(0x2764)% どーお？ お、おいしい？\/\/\/」
			PRINTFORMW 「あたいのおまんこおいしーい？\/\/\/」
		ENDSELECT
	;それ以外
	ELSE
		SELECTCASE RAND:2
		CASE 0
			PRINTFORMW 「また舐めるの……？」
			PRINTFORMW 「い、いいよ……はい…な、なめてぇっ%UNICODE(0x2764) * 2%」
		CASE 1
			PRINTFORMW 「舐めたいの？ じゃあ、はいっ！」
			PRINTFORMW 「あたいのおまんこぺろぺろしてっ%UNICODE(0x2764)%」
		ENDSELECT
	ENDIF
ENDIF
