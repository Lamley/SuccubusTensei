@M_KOJO_MESSAGE_COM_K111_VSEX(ARG)
SIF FLAG:時間停止
	RETURN 1
IF TALENT:111:処女 == 1
	PRINTFORMW 「ふぇっ！？ ちょ、ちょっと待って！」
	PRINTFORMW 「い…挿入れるのね……？」
	PRINTFORMW 「ちょ、ちょっと怖いけど、大丈夫……」
	PRINTFORMW 「初めてが%CALLNAME:MASTER%で、あたい嬉しい%UNICODE(0x2764)%」
	PRINTFORMW 「だからあたいがんばるよ！」
	;騎乗位
	IF ARG == 65
		PRINTFORMW 「じゃあ、%CALLNAME:MASTER%のおちんぽ、挿入れちゃうね%UNICODE(0x2764)%」
	;それ以外
	ELSE
		PRINTFORMW 「あ、あたいのおまんこに、%CALLNAME:MASTER%のおちんぽ挿入れて%UNICODE(0x2764)%%UNICODE(0x2764)%」
	ENDIF
ELSE
	SELECTCASE RAND:5
	CASE 0
		PRINTFORMW 「んっ%UNICODE(0x2764)% んんっ%UNICODE(0x2764)% あんっ%UNICODE(0x2764)% あんっ%UNICODE(0x2764)% 」
	CASE 1
		PRINTFORMW 「%CALLNAME:MASTER%っ%UNICODE(0x2764)% %CALLNAME:MASTER%っ%UNICODE(0x2764)% あっ%UNICODE(0x2764)%」
	CASE 2
		PRINTFORMW 「んぁ%UNICODE(0x2764)% あんっ%UNICODE(0x2764)% きもちいいっ%UNICODE(0x2764)%」
		PRINTFORMW 「もっとっ%UNICODE(0x2764)% もっとぉ%UNICODE(0x2764)%%UNICODE(0x2764)%」
	CASE 3
		PRINTFORMW 「あっ%UNICODE(0x2764)% あんっ%UNICODE(0x2764)% はっ%UNICODE(0x2764)% はげしっ%UNICODE(0x2764)% いぁっ%UNICODE(0x2764)%」
	CASE 4
		PRINTFORMW 「ああっ%UNICODE(0x2764)% いいっ%UNICODE(0x2764)% %CALLNAME:MASTER%のおちんぽっ%UNICODE(0x2764)%」
		PRINTFORMW 「しゅ、しゅごいっ%UNICODE(0x2764)% きもちいっ%UNICODE(0x2764)% あんっ%UNICODE(0x2764)%」
	ENDSELECT
ENDIF

