@M_KOJO_MESSAGE_COM_K111_300
;コマンド共通メッセージ(TRAIN_MESSAGE) 
;削除すると「～話をした。」とかが消えます
;CALL TRAIN_MESSAGE
DRAWLINE
;口上メイン呼出
CALL M_KOJO_MESSAGE_COM_K111_300_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K111_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;ここに口上書いて下さい
	IF ALCOHOL_FACE(111) == "酔いどれ" || ALCOHOL_FACE(111) == "ぐでぐで"
		PRINTFORML %CALLNAME:PLAYER%は%CALLNAME:TARGET%に話し掛けてみたが、%CALLNAME:TARGET%は呂律が回っておらず何を言っているかも聴き取れない
	ELSE
		PRINTFORM %CALLNAME:PLAYER%は%CALLNAME:TARGET%と
		IF TALENT:111:恋慕
			PRINTDATA
				DATAFORM 隣り合って
				DATAFORM 少し照れながら
				DATAFORM 微笑みを交わしながら
				DATAFORM 和やかな雰囲気で
				DATAFORM 少し熱くなりながら
				DATAFORM じゃれ合いながら
			ENDDATA
		ELSE
			PRINTDATA
				DATAFORM くだらない
				DATAFORM たわいのない
				DATAFORM 世間
				DATAFORM 平和な
				DATAFORM よくある
				DATAFORM ちょっとした
				DATAFORM 可笑しな
			ENDDATA
		ENDIF
		IF TFLAG:SELECTCOMの分岐 > 0
			PRINTFORML 話をして盛り上がった
		ELSEIF TFLAG:SELECTCOMの分岐 < 0
			PRINTFORML 話をしていたが話が途切れてしまった……
		ELSE
			PRINTFORML 話をした
		ENDIF
	ENDIF
	PRINTW 
	IF ALCOHOL_FACE(111) == "酔いどれ" || ALCOHOL_FACE(111) == "ぐでぐで"
		IF !RAND:2
			PRINTFORML 「きゃははは♪ おしゃけだ～いしゅき♪ おしゃけしゅきな%CALLNAME:MASTER%もしゅきぃ%UNICODE(0x2764)%」
			PRINTFORMW 「～～ッ！！＠＃！＄％！？＊＊＊＊ッ！！」
		ELSE
			PRINTFORML 「しょれでぇ～～、ごひゅびんひゃまがぁ～～～ぶぁ～～～～～って！」
			PRINTFORMW 「れもあらいはひゅ～～ぐ、ひゅ～～～～ぐひゃひぇひゅへ、ひゃ～はははは！！」
		ENDIF
		PRINTFORML 
		PRINTFORMW ほとんど会話にならなかった……
	ELSEIF TALENT:111:心情 == -1 || CFLAG:111:ブチギレ
		PRINTFORML 「なーに！？」
		PRINTFORMW 「あたいじゃなくて、他の娘と喋ってれば？」
	;会話不能
	ELSEIF TCVAR:111:会話不能
		PRINTFORML 「その話さっきしたー」
		PRINTFORMW 「つまんないー」
	;失敗
	ELSEIF TFLAG:SELECTCOMの分岐 == -1
		PRINTFORML 「……ふーん」
		PRINTFORMW 「何の話かよく分からないや」
	;夏,野外,昼間,汗フラグなしで確定
	ELSEIF DAY:2 == 2 && OUTROOF(CFLAG:111:現在位置) && !TCVAR:111:350 && TIME >= 600 && TIME <= 960
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML 「毎日暑いね……」
			PRINTFORMW 「灼熱地獄に比べたらこれでも涼しい方なんだろうケド」
		CASE 1
			PRINTFORMW 「地上もこんなに暑いなんて……」
		CASE 2
			PRINTFORML 「ぐでー……あついー……」
			PRINTFORMW 「%CALLNAME:MASTER%、この暑さなんとかならないのー？」
		ENDSELECT
		PRINTFORML 克労恩皮絲は夏用の短い袴襪を履いているが
		PRINTFORMW ムンムンとした熱気でだいぶ蒸れているようだ……
		TCVAR:111:350 = 1
	;レア
	ELSEIF !RAND:200
		PRINTFORMW 「へっくしっ！」
	;状況会話
	ELSEIF !RAND:12
		;春,野外
		IF DAY:2 == 1 && OUTROOF(CFLAG:111:現在位置)
			IF !RAND:2
				PRINTFORML 「お花がきれいね」
				PRINTFORMW 「地獄にも血針草って花があるよ」
				PRINTFORMW 「人の血を吸って成長してー……」
			ELSE
				PRINTFORML 「うー、最近なんだか目がかゆくて……」
				PRINTFORMW 「鼻もムズムズするし、風邪ひいちゃったかなぁ？」
			ENDIF
		;秋,野外
		ELSEIF DAY:2 == 3 && OUTROOF(CFLAG:111:現在位置)
			IF !RAND:2
				PRINTFORML 「紅葉ってきれいだね」
				PRINTFORMW 「まるで返り血を浴びたみたい♪」
			ELSE
				PRINTFORMW 「秋は"もみじ狩り"ってスポーツをするんでしょ？」
				PRINTFORMW 「"もみじ"ってどのぐらい強いの？」
			ENDIF
		;冬,野外
		ELSEIF DAY:2 == 4 && OUTROOF(CFLAG:111:現在位置)
			IF !RAND:2
				PRINTFORMW 「ねえ%CALLNAME:MASTER%……」
				PRINTFORMW 「さむい……」
			ELSE
				PRINTFORML 「わぁー！ 雪よ！」
				PRINTFORMW 「きゃははははー！」
				PRINTFORMW 「%CALLNAME:MASTER%、遊ぼ♪ 遊ぼ♪」
			ENDIF
		ELSEIF TALENT:111:恋慕
			GOTO RENBO
		ELSE
			GOTO SONOTA
		ENDIF
	;恋慕
	ELSEIF TALENT:111:恋慕
		$RENBO
		SELECTCASE RAND:13
		CASE 0
			PRINTFORML 「ここはいいね、苦しそうにしてる人がいなくて」
			PRINTFORMW 「地獄は怨嗟の声しか聞こえないから退屈でしょうがないよ」
		CASE 1
			PRINTFORML 「%CALLNAME:MASTER%は何が好きなのー？」
			PRINTFORMW 「あたいはねー……」
		CASE 2
			PRINTFORML 「この服かわいいでしょー！」
			PRINTFORMW 「ご主人様に作ってもらったのよ♪」
		CASE 3
			PRINTFORML 「今日は何して遊ぼっかなー」
			PRINTFORMW 「芜菁でも育ててみようかな」
		CASE 4
			PRINTFORML 「友人様は知ってる？」
			PRINTFORMW 「友人様はとっても優しくて、いつもなでなでしてくれるの」
		CASE 5
			PRINTFORML 「あたいは人を狂わす能力があるけど」
			PRINTFORMW 「%CALLNAME:MASTER%はどんな能力があるの？」
			PRINTFORMW 「あたいと本気で戦ったらどうなるかな？」
		CASE 6
			PRINTFORML 「%CALLNAME:MASTER%は人間よねー」
			PRINTFORMW 「あたいは妖精だけど……」
			PRINTFORMW 「%CALLNAME:MASTER%はどう思う？」
		CASE 7
			PRINTFORML 「ねえ%CALLNAME:MASTER%ー」
			PRINTFORMW 「えへへ、よんだだけー」
		CASE 8
			PRINTFORMW 「あーあ、%CALLNAME:MASTER%が地獄に堕ちてくれたら地獄もちょっとは楽しくなるのになー」
		CASE 9
			PRINTFORML 「地上は楽しいけど、なんか妖精への風当たりは強いのよね」
			PRINTFORMW 「サベツってやつ？ ま、あたいは全然気にならないケド」
		CASE 10
			PRINTFORMW 「なんかおもしろいところに行きたいなー♪ カジノとか！」
		CASE 11
			PRINTFORML 「ねえ、%CALLNAME:MASTER%」
			PRINTFORMW 「あたいってその……かわいい、かな？」
			PRINTFORMW 「あ！！ やっぱりなんでもない！」
		CASEELSE
			PRINTFORML 「オー、セーイキャンユーシー♪」
			PRINTFORMW 「地獄でブームの歌だよ♪」
		ENDSELECT
	;通常
	ELSE
		$SONOTA
		SELECTCASE RAND:8
		CASE 0
			PRINTFORML 「地上は至る所に自然が溢れていて感動しちゃった」
			PRINTFORMW 「本当に地獄とは正反対なのね」
		CASE 1
			PRINTFORML 「%CALLNAME:MASTER%の話っていつも可怪しいわね」
			PRINTFORMW 「地上人はみんなそうなのかな」
		CASE 2
			PRINTFORML 「あたいはねえ、人を狂わすことができるの」
			PRINTFORML 「月の光よりずうっと強力で純粋な狂気」
			PRINTFORMW 「どう？ 味わってみたいと思わない？」
		CASE 3
			PRINTFORML 「それってどういうこと？」
			PRINTFORMW 「もっと聞かせて！」
		CASE 4
			PRINTFORML 「ご主人様は地獄の神様だよ」
			PRINTFORMW 「月と地球と異界に１人ずつ居て……え？ よくわからない？」
		CASE 5
			PRINTFORMW 「漢堡が食べたくなってきた！」
		CASE 6
			PRINTFORMW 「ねえ%CALLNAME:MASTER%、きいてきいて！」
		CASEELSE
			PRINTFORML 「困ったことがあったらご主人様に聞いてみるといいよ」
			PRINTFORMW 「なんだって解決してくれちゃうんだから」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
