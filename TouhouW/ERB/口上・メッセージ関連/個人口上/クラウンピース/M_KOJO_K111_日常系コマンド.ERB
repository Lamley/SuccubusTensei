@M_KOJO_MESSAGE_COM_K111_302_1
#DIM 分岐数
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
;恋慕に0,1以外の数字が入った時のために一応
IF TALENT:111:恋慕
	分岐数 = 8
ELSE
	分岐数 = 4
ENDIF
IF LOCAL
	;心情不快
	IF TALENT:111:心情 == -1 || CFLAG:111:ブチギレ
		PRINTFORML %CALLNAME:PLAYER%は%CALLNAME:TARGET%との身体接触を図ろうとした
		PRINTW 
		PRINTFORML 「むぅ……！」
		PRINTFORMW 「今はそんな気分じゃ……」
		RETURN 1
	;背中を流す
	ELSEIF BATHCHECK(CFLAG:MASTER:現在位置) && IN_HOME(CFLAG:MASTER:現在位置)
		PRINTFORML %CALLNAME:PLAYER%は%CALLNAME:TARGET%の背中を流してあげた
		PRINTW 
		PRINTFORML 「洗ってくれるの？ じゃあお願いっ♪」
		PRINTFORMW 「やんっ%UNICODE(0x2764)% 触りすぎだぉ♪」
	ELSE
		PRINTFORML %CALLNAME:PLAYER%は%CALLNAME:TARGET%との身体接触を図った
		PRINTW 
		SELECTCASE RAND:分岐数
		CASE 0
			PRINTFORML 「きゃははは！」
			PRINTFORMW 「くすぐったいー！」
		CASE 1
			PRINTFORML 「なにするのさ！」
			PRINTFORMW 「えいえい！とお！」
		CASE 2
			PRINTFORML 「羽が気になるの？」
			PRINTFORMW 「やさしくさわってね」
		CASE 3
			PRINTFORML 「ちょっとだけ狂ってみる？」
			PRINTFORMW 「ほらほら、どう？」
		;ここから恋慕
		CASE 4
			PRINTFORML 「あっ……\/\/\/」
			PRINTFORMW 「それ気持ちいいかも\/\/\/」
		CASE 5
			PRINTFORMW 「すりすりしてすりすり！」
		CASE 6
			PRINTFORML 「ねえぺろぺろしていい？」
			PRINTFORMW 「ぺろぺろ、ぺろぺろ」
		CASE 7
			PRINTFORML 「やぁ……\/\/\/」
			PRINTFORMW 「それはちょっと恥ずかしいよう……\/\/\/」
		ENDSELECT
	ENDIF
	;大成功
	IF TFLAG:SELECTCOMの分岐 == 1
		PRINTFORMW 「きゃはっ♪ %CALLNAME:MASTER%、楽しいね♪」
	;失敗
	ELSEIF TFLAG:SELECTCOMの分岐 == -1
		PRINTFORMW 「いたっ！ もー、ぶつけないでよー！」
	ENDIF
ENDIF
RETURN 1
