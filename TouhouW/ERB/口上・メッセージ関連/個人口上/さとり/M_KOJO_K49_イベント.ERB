	IF LOCAL:1 && ARG == 2
	;満足して解放
		IF 陥落状態() >= 4
			IF 陥落状態() == 5
			ELSE
			ENDIF
		ELSEIF 陥落状態() == 3
		ELSEIF 陥落状態() == 2
		ELSEIF 陥落状態() == 1
		ELSE
		ENDIF
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
	;満足したところを押し倒された
		IF 陥落状態() >= 4
			IF 陥落状態() == 5
			ELSE
			ENDIF
		ELSEIF 陥落状態() == 3
		ELSEIF 陥落状態() == 2
		ELSEIF 陥落状態() == 1
		ELSE
		ENDIF
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
	;壊れるまでしてあげる
		IF 陥落状態() >= 4
			IF 陥落状態() == 5
			ELSE
			ENDIF
		ELSEIF 陥落状態() == 3
		ELSEIF 陥落状態() == 2
		ELSEIF 陥落状態() == 1
		ELSE
		ENDIF
		PRINTFORML
		RETURN 1
	ENDIF
ENDIF
