;300,会話
;==================================================
@M_KOJO_MESSAGE_COM_K49_300
;コマンド共通メッセージ(TRAIN_MESSAGE) 
;削除すると「～話をした。」とかが消えます
IF CFLAG:1496 == 2 && !RAND:2
	CALL PET49()
ELSEIF CFLAG:1496 == 1 && !RAND:2
	CALL ONRYO49()
ENDIF
SIF 陥落状態() < 3 && !TCVAR:302 && (BASE:酒気 < (MAXBASE:酒気 / 10) * 4)
	CALL TRAIN_MESSAGE
;口上メイン呼出
CALL M_KOJO_MESSAGE_COM_K49_300_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K49_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF (BASE:酒気 > (MAXBASE:酒気 / 10) * 4)
		DRAWLINE
		SIF FLAG:口上色
			CALLFORM M_KOJO_COLOR_K49
		CALL DRUNKER49()
	ELSEIF TCVAR:302
		DRAWLINE
		SIF FLAG:口上色
			CALLFORM M_KOJO_COLOR_K49
		IF 陥落状態() >= 4
			IF 陥落状態() == 5
				PRINTFORML 「主様の声は心地良いですけど、たまにはのんびりしましょう？」
				PRINTFORML 「『つまらないだろう』？　主様が側に居るだけで嬉しいですから」
				PRINTFORML 「『可愛い奴』……って、主様が素敵だからです」
			ELSE
				PRINTFORML 「主様、無理に喋ろうとしなくても良いですよ」
				PRINTFORML 「ふふ、そうやって、話題を探しているところも、素敵ですから」
				PRINTFORML 「それに、心を見せてもらうのも嬉しいことですし」
			ENDIF
		ELSEIF 陥落状態() == 3
			PRINTFORML 「%CALLNAME:MASTER%の声は好きですが、心を見るのも好きなんです」
			PRINTFORML 「無理に会話をしようとしなくても良いのでは」
		ELSEIF 陥落状態() == 2
			PRINTFORML 「話すことがないのなら黙っていて下さい」
		ELSEIF 陥落状態() == 1
			PRINTFORML 「口が動かないなら、無理に話さなくても良いですよ」
		ELSE
			PRINTFORML 「何故話すこともないのに、口を動かそうとするんですか？」
		ENDIF
		PRINTL
		RESETCOLOR
	ELSEIF 陥落状態() >= 4
		IF 陥落状態() == 5
			DRAWLINE
			PRINTFORM %CALLNAME:MASTER%の言葉に、
			IF CFLAG:TARGET:添い寝中
				PRINTFORML 蕩けきった表情で、ぎゅっと抱きつく覚が見つめてくる
			ELSEIF CHK_DATENOW(CFLAG:TARGET:デート中)
				PRINTFORML 彼我の指を絡めた覚が、ひまわりのような笑顔を浮かべている
			ELSE
				PRINTFORML 覚が満面に笑みを浮かべ、ぎゅっと抱きついてきた
			ENDIF
			PRINTL
			SIF FLAG:口上色
				CALLFORM M_KOJO_COLOR_K49
			SELECTCASE RAND:2
				CASE 1
					PRINTFORML 「それでどうなったんですか？」
					PRINTFORMDL %CALLNAME:MASTER%の話に、覚が目を輝かせて続きを促してくる
					PRINTFORML 「『飽きないのか』？　ないですよー、主様の声と”声”が同じなんですから」
					PRINTFORMDL そう言って朗らかな笑顔を浮かべる覚
					PRINTFORML 「本当に主様は素敵です」
					PRINTFORMDL まっすぐな好意の馮る言葉に、なぜか気恥ずかしさを覚えた
				CASE 0
					PRINTFORML 「主様は本当に妙な話をご存じですね」
					PRINTFORMDL 不思議そうにそんな事を良いながら覚が寄り添ってくる
					PRINTFORML 「『知ってる話だろう』？　知識として知っていますが、経験した人の話は別物ですから」
					PRINTFORMDL なにが嬉しいのか笑顔でのたまう覚
					PRINTFORML 「それに、大好きな主様の声が聞けるのも嬉しいですから」
					PRINTFORMDL そんな覚の笑顔に、なんとなく口の端が緩んでしまった
			ENDSELECT
		ELSE
			DRAWLINE
			PRINTFORM %CALLNAME:MASTER%の言葉に、
			IF CFLAG:TARGET:添い寝中
				PRINTFORML うっとりとした表情で、密着する覚が見つめてくる
			ELSEIF CHK_DATENOW(CFLAG:TARGET:デート中)
				PRINTFORML 彼我の指を絡めた覚が、蕩けるような笑みを向けてくる
			ELSE
				PRINTFORML 覚が笑みを浮かべて、腕を絡めて体を預けてきた
			ENDIF
			PRINTL
			SIF FLAG:口上色
				CALLFORM M_KOJO_COLOR_K49
			SELECTCASE RAND:3
				CASE 2
					PRINTFORML 「主様の声は、とても心地良いですね」
					PRINTFORMDL 初めて会ったときの冷淡さが欠片も無い言葉に、自然と笑みが浮かぶ
					PRINTFORMW 「……あの時は、まだ家族以外の他者に心を許すつもりはありませんでしたから」
					PRINTFORMDL 今から裏切るのも楽しそうだ、そう思うと同時、覚が恥ずかしげにほほを染める
					PRINTFORML 「時止めだけでは得られない、奉仕される悦楽を捨てても良いと？」
					PRINTFORMDW 今すぐ奉仕を始めたいと思っているのだろう、目元を潤ませる覚にただ笑みを向けた
				CASE 1
					PRINTFORML 「主様はそう言うことが本当にお好きですね」
					PRINTFORMDL 笑みを浮かべながらの覚の言葉に、自然と苦笑が浮かぶ
					PRINTFORMW 「『そうとは限らない』？　ふふ、主様は嘘を吐かないから、よく解るのですよ」
					PRINTFORMDL 嘘なら山ほど吐いている、そう答えを返そうとして
					PRINTFORML 「主様は思いと裏腹の言葉は口にしません。それでは私に嘘を吐いた事になりませんよ」
					PRINTFORMDW そう言いながら、含羞の紅を浮かべる覚に、何も言わずにただ抱き寄せた
				CASE 0
					PRINTFORML 「主様との会話は、本当に楽しいです」
					PRINTFORMDL 楽しませてやる義理は無い、そんなことを思うと同時、覚が笑みを深めた
					PRINTFORMW 「ふふ、ではなぜ会話を行うのです？」
					PRINTFORMDL 単なる気まぐれだと内心で答える%CALLNAME:MASTER%
					PRINTFORML 「ふふふ、そう言うことにしておきましょう」
					PRINTFORMDW 嬉しげなその言葉に以前とはちがい、なぜか楽しさを覚えた
			ENDSELECT
		ENDIF
		RESETCOLOR
	ELSEIF 陥落状態() == 3
		DRAWLINE
		IF CFLAG:TARGET:添い寝中
			PRINTFORML 体温を感じられる近さで、笑顔を浮かべる覚に話しかける
		ELSEIF CHK_DATENOW(CFLAG:TARGET:デート中)
			PRINTFORML 嬉しそうな笑顔で寄り添ってくる覚に、話を向けた
		ELSE
			PRINTFORML 楽しそうな笑みを浮かべる覚に言葉を向けた
		ENDIF
		PRINTL
		SIF FLAG:口上色
			CALLFORM M_KOJO_COLOR_K49
		SELECTCASE RAND:3
			CASE 2
				PRINTFORML 「……ふふ、%CALLNAME:MASTER%の声が聞けるというのは素敵ですね」
				PRINTFORMDL 話の内容はどうでも良いのかと、問い掛けるより早く
				PRINTFORML 「思いと声が同じというのが、心地良いのです」
				PRINTFORMDW そう言って笑う覚に返す言葉は出てこない
			CASE 1
				PRINTFORML 「%CALLNAME:MASTER%が言うならそうなのでしょう」
				PRINTFORMDL 嬉しげに笑う覚に、実際はどうか解らないけどな、と内心で呟く
				PRINTFORML 「ふふ、そういう所も含めて、そうなのだと思っているんですけどね」
				PRINTFORMDW 嬲るつもりが軽くいなされて、苦笑しか返せない
			CASE 0
				PRINTFORML 「なるほど、%CALLNAME:MASTER%らしい話ですね」
				PRINTFORMDL 感心したように頷く覚に、これなら簡単に嘘がつけそうだと思う%CALLNAME:MASTER%
				PRINTFORML 「……恋は盲目と言いますけど、嘘を見抜けない程、私の”目”は陰天ませんよ」
				PRINTFORMDW クスクスと笑いながらの言葉に、返答の代わりに%CALLNAME:MASTER%は溜め息を漏らした
		ENDSELECT
		RESETCOLOR
	ELSEIF 陥落状態() == 2
		PRINTFORML 「……失せなさい下郎」
		PRINTFORMDL 開口一番、そんなことを口にする覚に、ニヤニヤが止まらない
		PRINTFORML 「『そんなに家族が大切か』？　……下劣卑賤汚穢な輩に言う義理はありませんね」
		PRINTFORMDW その冷めた瞳と憤怒を押し殺し震える唇に、%CALLNAME:MASTER%は笑みを押し殺せない
	ELSEIF 陥落状態() == 1
		SELECTCASE RAND:3
			CASE 2
				PRINTFORML 「なるほど、そう言うこともあるのですね」
				PRINTFORMDW 柔らかい表情の覚に、自然と笑みが浮かんでしまう
			CASE 1
				PRINTFORML 「まあ、%CALLNAME:MASTER%らしくも無い失敗をしたんですね」
				PRINTFORMDW どことなく楽しげな覚に、苦笑しか返せない
			CASE 0
				PRINTFORML 「それはそれは、さぞ見物だったでしょうね」
				PRINTFORMDW そう言った覚が、珍しく口の端に笑みを登らせている
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
			CASE 2
				PRINTFORML 「……別に喋る必要はないのでは？」
				PRINTFORMDW 単なる気まぐれだと内心で呟き、冷め切った表情の覚をみやった
			CASE 1
				PRINTFORML 「ええ、聞いてますよ。”目”で見る方が早いですが」
				PRINTFORMDW そう曰う覚だが、それでも無視はしないあたりにらしさを感じた
			CASE 0
				PRINTFORML 「つまらない話ですね、それだけですか？」
				PRINTFORMDW 言葉とは裏腹に、続きを聞きたげな雰囲気を漂わせている
		ENDSELECT
	ENDIF
	PRINTFORMW 
ENDIF
RETURN 1
;==================================================
;301,泡茶
;==================================================
@M_KOJO_MESSAGE_COM_K49_301
IF CFLAG:1496 == 2 && !RAND:2
	CALL PET49()
ELSEIF CFLAG:1496 == 1 && !RAND:2
	CALL ONRYO49()
ENDIF
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K49_301_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K49_301_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF (BASE:酒気 > (MAXBASE:酒気 / 10) * 4)
		CALL DRUNKER49()
	ELSEIF 陥落状態() >= 4
		IF GETBIT (CFLAG:1490,3)
			IF TALENT:恋慕
				IF 陥落状態() == 5
					PRINTFORML 「主様、今日はお薬は無しですか？　残念です」
					PRINTFORMDL 入れて欲しいと言外に告げる覚に苦笑が浮かぶ
					PRINTFORMW 「だって、主様がお薬入れるときはＨするときですから」
					PRINTFORMDL 満面に笑みを浮かべてつぶやく覚
					PRINTFORML 「入ってないお茶も、美味しいから好きですけど」
					PRINTFORMDW そう言いながら、お茶に口をつけた
				ELSE
					PRINTFORML 「主様、お薬は入れないのですか？」
					PRINTFORMDL 入れて欲しいのか？　と、問い掛けた%CALLNAME:MASTER%に、覚が苦笑を浮かべる
					PRINTFORMW 「出来れば遠慮はしたいですが、主様がお望みでしたら構いませんよ」
					PRINTFORMDL いまはそう言う気分じゃないと思い
					PRINTFORML 「ふふ、そう言う主様の気まぐれは嫌いではありませんよ」
					PRINTFORMDW 呟いた覚が、お茶に口をつけた
				ENDIF
			;恋慕も持たずに恋人とな？
			ELSE
				PRINTFORML 「良かった、お薬は入れてないんですね」
				PRINTFORMDL 入れても飲まないくせに、そう思うと同時、覚が口の端に笑みを浮かべた
				PRINTFORMW 「当然です。いくら恋仲とはいえ、やって良いことと悪いことがあります」
				PRINTFORML 「睦み合いの最中ならともかく、他人に見つかるようなことはしないでほしいです」
				PRINTFORMDL その言葉に応えることなく、%CALLNAME:MASTER%は秘蔵のお茶請けをこっそり取り出す
				PRINTFORMDW 嬉しげな表情で、覚がそれを受け取った
			ENDIF
		ELSE
			IF 陥落状態() == 5
				PRINTFORML 「うふふ、主様のお茶、美味しいから好きです」
				PRINTFORMDL 満面の笑顔でそう言われて、%CALLNAME:MASTER%も悪い気はしない
				PRINTFORML 「坦率に嬉しい、とは思えないんですか？」
				PRINTFORMDL 覚の言葉に、心が見えない以上、世辞と取った方が良いと答える
				PRINTFORML 「そういう主様はキライです」
				PRINTFORMDL 唇をとがらせる覚に、苦笑しながらその頭を撫でてやった
			ELSE
				PRINTFORML 「主様、ありがとうございます。嬉しいです」
				PRINTFORMDL 受け取ったお茶とお茶請けを、嬉しそうに眺める覚
				PRINTFORMW 「『単なる気まぐれ』？　ふふ、気まぐれでも主様がしてくれるのが嬉しいんです」
				PRINTFORMDL くすくすと笑いながら、覚がお茶とお茶請けに口をつける
				PRINTFORML 「あの子達に淹れて貰うのとはまた違う嬉しさなんですよ」
				PRINTFORMDW 覚の言葉に、嬲るつもりだった言葉が霧消した
			ENDIF
		ENDIF
	ELSEIF 陥落状態() == 3 || TALENT:184 || TALENT:185
		IF GETBIT (CFLAG:1490,3)
			PRINTFORML 「あら、お薬は淹れないんですか？」
			PRINTFORMDL くすくすと笑いながら覚が呟く
			PRINTFORML 「『今は不要』？　ふふ、ふふふふ、%CALLNAME:MASTER%は本当に気まぐれですね」
			PRINTFORMDW 何が楽しいか、くすくすと笑いながら、覚が手にした茶に口をつけた
		ELSE
			PRINTFORML 「美味しいですね。%CALLNAME:MASTER%が淹れてくれるからでしょうか」
			PRINTFORMDL 誰が淹れても変わりは無い、そう呟いた瞬間、覚が笑みを苦笑に変えた
			PRINTFORML 「いえ、ウチの子達でも、阿燐は得意ですが、阿空は苦手ですしね」
			PRINTFORMDW あの二人ならさもありなん、内心で思うと同時、覚が苦笑しながら頷いた
		ENDIF
	ELSEIF 陥落状態() == 2
		IF GETBIT (CFLAG:1490,3)
			PRINTFORML 「『薬は入れてない』……、入れないのが当たり前でしょう」
			PRINTFORMDL 冷たい目で睨んでくる覚
			PRINTFORML 「『今度こそ』？　残念ですが、無駄ですね」
			PRINTFORMDW 深々と溜め息を吐いた覚が、呆れたような表情を浮かべている
		ELSE
			PRINTFORML 「お茶を淹れた程度で、懐柔できるなんて思わないことですね」
			PRINTFORMDL 心情不快そうに呟きながらも、差し出したお茶とお茶請けを手にする覚
			PRINTFORML 「……何か？」
			PRINTFORMDW 僅かに楽しげにお茶をたしなむ覚に、ただにやにやと笑いを浮かべた
		ENDIF
	ELSEIF 陥落状態() == 1
		IF GETBIT (CFLAG:1490,3)
			PRINTFORML 「よかった、今日は薬は入っていませんね」
			PRINTFORMDW にっこりと笑みを浮かべる覚に、答えを濁した
		ELSEIF TFLAG:193 == 1
			PRINTFORML 「%CALLNAME:MASTER%は泡茶のは上手ですね」
			PRINTFORMDW 楽しげな笑みを浮かべた覚に、笑みを返す%CALLNAME:MASTER%だった
		ELSEIF TFLAG:193 == -1
			PRINTFORML 「……えと、有り難いのですが、これはちょっと」
			PRINTFORMDW 困惑する覚に、首を傾げて味を見た%CALLNAME:MASTER%はあまりの不味さに凍り付いた
		ELSE
			PRINTFORML 「ふふ、ありがとうございます」
			PRINTFORMDW 口元をほころばせて、覚がお茶に口をつけている
		ENDIF
	ELSE
		IF GETBIT (CFLAG:1490,3)
			PRINTFORML 「今後も、薬はまぜないで下さい」
			PRINTFORMDW 不愉快そうに呟いて、覚がお茶を手にした
		ELSEIF TFLAG:193 == 1
			PRINTFORML 「ありがとうございます……、美味しい」
			PRINTFORMDW 覚が、珍しく少しだけ楽しげに口元をほころばせた
		ELSEIF TFLAG:193 == -1
			PRINTFORML 「……どうも」
			PRINTFORMDW 一口含んだ瞬間、覚が言い難い表情を浮かべた
		ELSE
			PRINTFORML 「ありがとうございます」
			PRINTFORMDW 普段と変わりない表情で受け取って、覚がお茶を飲んでいる
		ENDIF
	ENDIF
	CLEARBIT CFLAG:1490,3
ENDIF
RETURN 1
;==================================================
;302,身体接触
;==================================================
@M_KOJO_MESSAGE_COM_K49_302
IF CFLAG:1496 == 2 && !RAND:2
	CALL PET49()
ELSEIF CFLAG:1496 == 1 && !RAND:2
	CALL ONRYO49()
ENDIF
SIF (BATHCHECK(CFLAG:MASTER:現在位置) && IN_HOME(CFLAG:MASTER:現在位置) && 陥落状態() < 4) || !BATHCHECK(CFLAG:MASTER:現在位置) || (BATHCHECK(CFLAG:MASTER:現在位置) && (CFLAG:MASTER:現在位置 == 大衆浴場 || CFLAG:MASTER:現在位置 == 旧地獄温泉))
	CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K49_302_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K49_302_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF (BASE:酒気 > (MAXBASE:酒気 / 10) * 4)
		CALL DRUNKER49()
	ELSEIF BATHCHECK(CFLAG:MASTER:現在位置) && IN_HOME(CFLAG:MASTER:現在位置)
		IF 陥落状態() >= 4
			DRAWLINE
			IF !RAND:2
				PRINTFORML %CALLNAME:PLAYER%は覚に背中を流してもらった
				PRINTL
				SIF FLAG:口上色
					CALLFORM M_KOJO_COLOR_K49
				IF 陥落状態() == 5
					PRINTFORML 「主様のお背中、綺麗です」
					PRINTFORMDL うっとりとした呟きに、振り返った%CALLNAME:MASTER%は覚の目が潤んでいる事に気づく
					PRINTFORML 「『欲情してるのか』？　はい、そーですよ。主様を見てるとオマンコの奥が熱くなっちゃいます」
					PRINTFORMDL 赤裸々な発言に苦笑を浮かべる%CALLNAME:MASTER%
					PRINTFORML 「それじゃ、お背中流しますね。ん、ふぅ、はふ……」
					PRINTFORMDL 泡を纏った覚が体をこすりつけてくるが、%CALLNAME:MASTER%の体を使って自慰をしてるようにしかみえない
				ELSE
					PRINTFORML 「それでは、失礼しますね」
					PRINTFORMDL 恭しく囁いた覚が、自身の肌に泡を纏い体を押し付けてくる
					PRINTFORMW 「ふふ、こう言うのも、良いと思いませんか主様？」
					PRINTFORMDL 背中に感じる柔らかさと、質の違う固さに、自然と脂下がってくる
					PRINTFORML 「喜んで貰えて嬉しいです。はぁ、主様の体は心地良い」
					PRINTFORMDW うっとりとした表情の覚に、欲情がわき上がってきた
				ENDIF
			ELSE
				PRINTFORML %CALLNAME:PLAYER%は覚の背中を流した
				PRINTL
				SIF FLAG:口上色
					CALLFORM M_KOJO_COLOR_K49
				IF 陥落状態() == 5
					PRINTFORML 「主様、ちゃんと主様の手で隅々まで洗ってくださいね？」
					PRINTFORMDL 満面に笑みを浮かべる覚のおねだりに、苦笑しながら従う%CALLNAME:MASTER%
					PRINTFORMW 「『相変わらずすべすべだ』？　ふふ、主様のためにいつも磨いていますから
					PRINTFORMDL そう言ったかと思うと、いきなりこちらの手首を掴んで、乳房と秘処へと導く
					PRINTFORML 「だから、ここも、きれいにしてください、ね？」
					PRINTFORMDW 柔らかな乳房と震える頂、愛蜜が絡む秘処を、指で丹念に洗った
				ELSE
