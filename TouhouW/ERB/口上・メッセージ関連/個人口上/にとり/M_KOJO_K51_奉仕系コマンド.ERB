		RETURN 0
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		PRINTFORMW 
		RETURN 0
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 0
	ENDIF
ENDIF
RETURN 1

