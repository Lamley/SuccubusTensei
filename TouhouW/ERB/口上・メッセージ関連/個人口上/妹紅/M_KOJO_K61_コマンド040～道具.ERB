;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)

		PRINTFORMW 
		RETURN 1
	ENDIF
	IF TEQUIP:PLAYER:避孕套 && EXP:出産経験 && !FLAG:70
		IF 陥落状態() >= 4
			PRINTFORML 「……%MASTERNAME:61%、何を考えているのかなぁ？」
			PRINTFORML 「笑顔が恐い？　……当たり前じゃない。なんで子作りしてるのにゴムなんか」
			PRINTFORML 「えいっ！　焼却処分完了、ほーら、ちゃんと種付けして、赤ちゃん孕ませて？」
		ELSEIF 陥落状態() == 3
			PRINTFORML 「……なんでゴムなんか使おうとするかな？　私を孕ませたくない？」
			PRINTFORML 「%CALLNAME:MASTER%がどう言おうと、赤ちゃん産ませてほしいんだから、コレは没収」
		ELSEIF 陥落状態() == 2
			PRINTFORML 「子作りしてるんだから、こんなの使ったらダメだよ」
		ENDIF
		TEQUIP:PLAYER:避孕套 = 0
		PRINTFORML 
		PRINTFORMW 妹紅に無理矢理避孕套を剥ぎ取られた
	;基本セット
	;時姦中
	ELSEIF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF 陥落状態() == 3
		PRINTFORMW 
		RETURN 1
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

