;-------------------------------------------
;調教後のテキスト
;-------------------------------------------
@M_KOJO_MESSAGE_PALAMCNG_C_K61
CALL PALAM_MESSAGE_C
;処女喪失
IF TCVAR:破瓜
	;正真正銘処女喪失
	IF TCVAR:破瓜 == 1
		IF 陥落状態() >= 4
			PRINTFORML 「…………輝夜との殺しあいで、……さんざん痛い目みてきたけど」
			PRINTFORML 「これ、全然、ちがう痛み、だね……、痛ぅ……」
			PRINTFORML 「でも、すごく嬉しいんだよ。%MASTERNAME:61%に初めてを捧げたって証だしね」
			PRINTFORML 「……ん、涙？　ふふ、うれしいから、だよ」
			PRINTFORML 「あ、ん、涙なんて、なめても美味しく無いと思うけど」
			PRINTFORML 「でも、なんだか、すごく嬉しいな」
		ELSEIF 陥落状態() == 3
			PRINTFORML 「あいたたたた……、長く生きてきたけど、これは初めての痛みだわ」
			PRINTFORML 「でも、%CALLNAME:MASTER%が初めての人になってくれて、嬉しい」
			PRINTFORML 「私には、縁のないことだと思ってたから」
			PRINTFORML 「ふふ、これからも、いっぱいかわいがってね」
		ELSEIF 陥落状態() >= 1
			PRINTFORML 「っっ……、いた、い……、これ、痛くて、辛いよ」
			PRINTFORML 「なんで、こんな……」
		ELSE
			PRINTFORML 「くぅ…………」
		ENDIF
	;再生処女喪失
	ELSEIF TCVAR:破瓜 == 2
		IF 陥落状態() >= 4
			PRINTFORML 「んっ、これ、痛いってば、わざわざ再生させるって、悪趣味」
			PRINTFORML 「……前より慣れてるように見える？　体だけの痛みだものね」
			PRINTFORML 「ホントの初めての時は、心の奥底を切り刻まれたような感じだったし」
		ELSEIF 陥落状態() == 3
			PRINTFORML 「くぅ、このスケベ……。わざわざ再生させて、何が楽しいんだか」
			PRINTFORML 「こっちはただ痛いだけなのにさ」
		ELSE
			PRINTFORML 「痛ぅ…………、へんたい……」
		ENDIF
	;無自覚非処女喪失
	ELSEIF TCVAR:破瓜 == -1
		IF 陥落状態() >= 4
			PRINTFORML 「……ん、痛く、ない？　どうして、初めては痛いって聞いてたのに」
			PRINTFORML 「もしかして時止で奪った？」
			PRINTFORML 「%MASTERNAME:61%のバカ、いくらなんでもそれはひどいんじゃないかな」
		ELSEIF 陥落状態() == 3
			PRINTFORML 「んっ、痛くない……、ホッとしたような、すこし残念のような」
			PRINTFORML 「で、その困ったような顔はどういうことかな？」
		ELSE
			PRINTFORML 「んぅ……、なんで痛くないんだろ……」
		ENDIF
	ENDIF
	PRINTFORMW 
ENDIF

;Ａ処女喪失
IF TCVAR:Ａ破瓜
	;正真正銘Ａ処女喪失
	IF TCVAR:Ａ破瓜 == 1
		IF 陥落状態() >= 4
			PRINTFORML 「んくぅ……、はぁ……ん、この、へんたい」
			PRINTFORML 「お尻なんて、いれるところじゃ、ないのにさ」
			PRINTFORML 「%MASTERNAME:61%は、こんな変態行為して満足？」
			PRINTFORML 「……うるさい、気持よくさせたそっちが悪い」
			PRINTFORML 「ひんっ！　ちょ、今は触らなっっ！！」
			PRINTFORML 「くぅ、この、へんたいぃ」
		ELSEIF 陥落状態() == 3
			PRINTFORML 「は、はぁ、ん……、くるし、い、ってば」
			PRINTFORML 「声に、艶が乗ってる？　うるさい、焼かれたい？」
			PRINTFORML 「……おしりで、気持よくなるようしたのは、%CALLNAME:MASTER%じゃない」
			PRINTFORML 「このへんたい」
		ELSE
			PRINTFORML 「くぅ、こっち、でなんて……、んぅ……」
		ENDIF
	;無自覚Ａ処女喪失
	ELSEIF TCVAR:Ａ破瓜 == -1
		IF 陥落状態() >= 4
			PRINTFORML 「はぁはぁ、んっ、なんでだろ、あんまり、くるしくない」
			PRINTFORML 「……もしかして、シテた？　……へんたい」
			PRINTFORML 「はぁ、ん、へんたい……、こっちが気持ちいいなんて」
		ELSEIF 陥落状態() == 3
			PRINTFORML 「はふ、ん、きもちいい、ってか、あんまり、苦しくない」
			PRINTFORML 「%CALLNAME:MASTER%が、ちゃんと慣らしてくれたから、かな」
		ELSE
			PRINTFORML 「んっ、ふぅ……、なんか、想像より、くるしくない、もんだね、んぅ」
		ENDIF
	ENDIF
	PRINTFORMW 
ENDIF
	
;フェラで射精後のお掃除
IF (TCVAR:MASTER:112 == 1 || TCVAR:MASTER:112 == 2) && SELECTCOM == 81
	IF ABL:LOCAL:精液中毒 < 3
		IF 陥落状態() >= 4
			PRINTFORML 「れろ、ちゅぱ……、んー、これでいい？」
			PRINTFORML 「えぅ、でも、やっぱ、これ嫌い。%MASTERNAME:61%は飲んで欲しいんだろうけど」
			PRINTFORML 「自分の中に入ってきたのを舐めるのも、あんまりいい気分じゃないし」
		ELSEIF 陥落状態() == 3
			PRINTFORML 「れろ、れろん……、はい、綺麗になった」
			PRINTFORML 「でも、中出ししたあとに、お口で掃除なんて、キチクだよねぇ」
		ELSE
			PRINTFORML 「れろ、ちゅぱ、んっ、これでいいでしょ、はいおしまい」
		ENDIF
	;Ｖ出し
	ELSEIF TCVAR:MASTER:112 == 1
		IF 陥落状態() >= 4
			PRINTFORML 「はぁ、ん、れろ、ちゅぱ、じゅるる、れろ……」
			PRINTFORML 「これが、れろ、ちゅる、おまんこ、かきまわしてたんだ、はぁはぁ」
			PRINTFORML 「じゅるるる、れろ、れろん、じゅる、ちゅぱ、はぁ、もっともっとぉ」
		ELSEIF 陥落状態() == 3
			PRINTFORML 「ちゅぱ、れろ、おまんこに、じゅる、はいって、たの、なめてる、れろん」
			PRINTFORML 「なんか、ちゅる、興奮、するね。ちゅぅぅ、ちゅぽんっ、じゅるる」
		ELSE
			PRINTFORML 「れろ……、はいおしまい。……自分の中に入ってたのを舐めるなんてしたいわけ無いでしょ」
		ENDIF
	ELSEIF TCVAR:MASTER:112 == 1
		IF 陥落状態() >= 4
			PRINTFORML 「んっ、れろ、んぅ、……れろ」
			PRINTFORML 「ねえ、%MASTERNAME:61%、これ、やめてほしい」
			PRINTFORML 「いくらなんでも、後ろに入ってたのを舐めるなんて、こんなのしたくないよ」
		ELSEIF 陥落状態() == 3
			PRINTFORML 「……ぅ、れろん、ん、はい、おしまい」
			PRINTFORML 「いくらなんでも、後ろに入れられてたのなんて、嫌だよ。できれば許してほしい」
		ELSE
			PRINTFORML 「……ちろ、終わり。いくらなんでもこれはさすがに無理だってば」
		ENDIF
	ENDIF
	PRINTFORMW 
ENDIF

;-------------------------------------------
;刻印取得
;-------------------------------------------
@M_KOJO_MESSAGE_MARKCNG_K61
;このままだと地の文が表示されません
;地の文を表示したい方は、下記のコメントアウトを削除して下さい
CALL MARK_MESSAGE
IF !FLAG:70
	;苦痛刻印
	IF TFLAG:22 == 1
		PRINTFORMW 「くぅ、痛いのには、慣れてる、けど」
	ELSEIF TFLAG:22 == 2
		PRINTFORMW 「なんで、こんなこと、痛ぅ」
	ELSEIF TFLAG:22 == 3
		PRINTFORMW 「本気で、おこる、よ。痛いってば」
	ENDIF

	;快楽刻印
	IF TFLAG:23 == 1
		PRINTFORMW 「ふぁっ……、なな、なんでもない」
	ELSEIF TFLAG:23 == 2
		PRINTFORMW 「あふ、んくぅっ！　き、気持ち良いかも」
	ELSEIF TFLAG:23 == 3
		PRINTFORMW 「ひぁぁあっっ！！」
	ENDIF

	;不埒刻印
	;快楽による刻印
	;LV1
	IF TFLAG:24 == 11
		PRINTFORMW 「あんっ、もう、しょうがないなぁ」
	;LV2
	ELSEIF TFLAG:24 == 12
		PRINTFORMW 「ふぁっ！　ん、ふふ、ねえ、そんなにしたい？」
	;LV3
	ELSEIF TFLAG:24 == 13
		PRINTFORMW 「ひぁんっ！！　も、もっと、して、ほしい、んくぅっ！」
	;屈服、恭順による刻印
	;LV1
	ELSEIF TFLAG:24 == 31
		PRINTFORMW 「ふふ、ただ見てるだけだよ」
	;LV2
	ELSEIF TFLAG:24 == 32
		PRINTFORMW 「ねえ、してほしいことがあったら遠慮無く言ってよ」
	;LV3
	ELSEIF TFLAG:24 == 33
		PRINTFORMW 「なんでもしてあげる、ううん、私にさせて欲しいな」
	ENDIF

	;反発刻印
	IF TFLAG:21 == 1
		PRINTFORMW 「……ふん」
	ELSEIF TFLAG:21 == 2
		PRINTFORMW 「……はぁ……何？」
	ELSEIF TFLAG:21 == 3
		PRINTFORMW 「…………死にたい？」
	ENDIF
ENDIF

