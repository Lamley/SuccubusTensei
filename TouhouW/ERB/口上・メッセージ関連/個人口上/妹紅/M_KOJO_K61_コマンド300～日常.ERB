;300,会話
;==================================================
@M_KOJO_MESSAGE_COM_K61_300
;コマンド共通メッセージ(TRAIN_MESSAGE) 
;削除すると「～話をした。」とかが消えます
CALL TRAIN_MESSAGE
;口上メイン呼出
CALL M_KOJO_MESSAGE_COM_K61_300_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K61_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;ここに口上書いて下さい
	IF TCVAR:302
		IF 陥落状態() >= 4
			PRINTFORMW 「ん？　何か話？　話すことがないなら、黙ってるのも良いものだよ」
		ELSEIF 陥落状態() == 3
			PRINTFORMW 「ん、話す必要はないんじゃないかな」
		ELSEIF 陥落状態() == 2
			PRINTFORMW 「ふふ、話せないなら触って欲しいな」
		ELSEIF 陥落状態() == 1
			PRINTFORMW 「無理に話す必要はないよ」
		ELSE
			PRINTFORMW 「…………」
		ENDIF
	ELSE
		IF 陥落状態() >= 4
			PRINTDATA
				DATALIST
					DATAFORM 「ん、ふふ、%MASTERNAME:61%もそんな失敗するんだ」
					DATAFORM 「時間が止められるなら、失敗なんてなかったことに出来そうだけど」
					DATAFORM 「失敗のフォローは出来ても、失敗そのものは取り消せないんだ。ふふ、なんとなく安心した」
				ENDLIST
				DATALIST
					DATAFORM 「輝夜とのケンカは程ほどに？」
					DATAFORM 「%MASTERNAME:61%まで、慧音みたいなこと言わないでよ」
					DATAFORM 「私の場合、どうせ普通の弾幕ごっこでも、体中ボロボロになるのにさ」
				ENDLIST
				DATALIST
					DATAFORM 「ふふ、%MASTERNAME:61%との会話は心地良いね」
					DATAFORM 「長い人生で、初めての恋人だからかな」
					DATAFORM 「そうなって欲しい人は、以前にもいたけど、ね」
				ENDLIST
			ENDDATA
		ELSEIF 陥落状態() == 3
			PRINTDATA
				DATALIST
					DATAFORM 「そう、だね。人里は見てて楽しいよ」
					DATAFORM 「まあ、あまりいい目で見られてはいないけど、慧音が居るしね」
				ENDLIST
				DATALIST
					DATAFORM 「慧音と知り合うまでは、吃飯はその辺の野草とかだったな」
					DATAFORM 「おかげで、どんなものでも食べられる様にはなったけど」
				ENDLIST
				DATALIST
					DATAFORM 「%CALLNAME:MASTER%は、本当に面白いね」
					DATAFORM 「この私にちょっかいかけてくるんだから、当然だけど」
				ENDLIST
			ENDDATA
		ELSEIF 陥落状態() == 2
			PRINTDATA
				DATAFORM 「ん、ふふ、こういうのも良いね」
				DATAFORM 「ねえ、話しだけで良いの？　もっと良いことシたくない？」
				DATAFORM 「まあ、会話も楽しいけど、もっと楽しいこともしたいね」
			ENDDATA
		ELSEIF 陥落状態() == 1
			PRINTDATA
				DATAFORM 「あ、うん、そうだね」
				DATAFORM 「うん、うん、へぇ、そうなんだ」
				DATAFORM 「あ、なるほど、そう言うこともあるのか」
			ENDDATA
		ELSE
			PRINTFORMW 「……ああ、うん」
		ENDIF
		PRINTL 
	ENDIF
ENDIF
RETURN 1
;==================================================
;301,泡茶
;==================================================
@M_KOJO_MESSAGE_COM_K61_301
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K61_301_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K61_301_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF 陥落状態() >= 4
		PRINTFORML 「はぁ、美味しい……、でも、%MASTERNAME:61%が口移しでくれたらもっと美味しいんだけどねぇ」
		PRINTFORML 「んっ！？　ちゅぅっ、……こくこくん……こく……はぁ、おいしぃ……」
		PRINTFORML 「これ、癖になりそう……」
	ELSEIF 陥落状態() == 3
		PRINTFORML 「いつも美味しいお茶淹れてくれてありがとう」
		PRINTFORML 「でも、私なんかよりも他の人に、……あ、ごめん、淹れてくれたのに変なこといって」
	ELSEIF 陥落状態() == 2
		IF TFLAG:193 == 1
			PRINTFORML 「ありがとう、ふふ、それじゃお礼、ちゅっ」
		ELSEIF TFLAG:193 == -1
			PRINTFORML 「……えと、嬉しいんだけど、これはちょっと勘弁して」
		ELSE
			PRINTFORML 「ふふ、ありがとう。お礼には何がシテほしい？」
		ENDIF
	ELSEIF 陥落状態() == 1
		IF TFLAG:193 == 1
			PRINTFORML 「ありがとう、すごく美味しいよ」
		ELSEIF TFLAG:193 == -1
			PRINTFORML 「あ、ありがたいけど、これはちょっと厳しいかな」
		ELSE
			PRINTFORML 「あ、ありがとう、淹れてくれて」
		ENDIF
	ELSE
		IF TFLAG:193 == 1
			PRINTFORML 「……ありがとう、美味しいよ」
		ELSEIF TFLAG:193 == -1
			PRINTFORML 「……っっ！？　……えと、嫌がらせ？」
		ELSE
			PRINTFORML 「…………ありがとう」
		ENDIF
	ENDIF
	PRINTFORMW 
ENDIF
RETURN 1
