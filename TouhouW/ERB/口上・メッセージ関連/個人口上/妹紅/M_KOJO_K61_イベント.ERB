;==================================================
@M_KOJO_EVENT_K61_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
IF GETBIT (CFLAG:1610,0)
	LOCAL = 0
	CLEARBIT CFLAG:1610,0
ENDIF
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF 陥落状態() >= 4
			PRINTFORML 「いらっしゃい、%MASTERNAME:61%」
			PRINTFORML 「近すぎないかって？　愛しの君の側に近寄るのは当然のことだよね」
			PRINTFORML 「ちゅっ、これでいい？　良いなら、頭撫でてほしいなぁ」
		ELSEIF 陥落状態() == 3
			PRINTFORML 「ねえ、することは解ってるよね？」
			PRINTFORML 「ん……、他人の体温って凄く安心するね」
		ELSEIF 陥落状態() == 2
			PRINTFORML 「ああ、いらっしゃい。さあ、ナニをしようかな」
		ELSEIF 陥落状態() == 1
			PRINTFORML 「良く来たね、暇だったら話でもしようか」
		ELSE
			PRINTFORML 「ああ、%CALLNAME:MASTER%か。どうも」
		ENDIF
		PRINTFORML 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF 陥落状態() >= 4
			PRINTFORML 「%MASTERNAME:61%、こっちにきて」
			PRINTFORML 「ちゅっ、ちゅっ……ん、美味しい、ごちそうさま」
			PRINTFORML 「%MASTERNAME:61%の顔をみたらガマンできなくなったんだもの」
		ELSEIF 陥落状態() == 3
			PRINTFORML 「あ、%CALLNAME:MASTER%。……ぎゅっ」
			PRINTFORML 「苦しい？　別にいいじゃない、擁抱くらい」
		ELSEIF 陥落状態() == 2
			PRINTFORML 「押し倒したい？　押し倒されたい？　私はどちらでも良いよ」
		ELSEIF 陥落状態() == 1
			PRINTFORML 「あ、丁度%CALLNAME:MASTER%のところにいくつもりだったんだよね」
		ELSE
			PRINTFORML 「あ、ん、どうも」
		ENDIF
		PRINTFORML 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが浴室に入ってきて一緒に入る
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF 陥落状態() >= 4
			PRINTFORML 「あ、%MASTERNAME:61%……、うふふ、そうやって見られるの、凄く嬉しい」
			PRINTFORML 「どうせだったら、このまま洗いっこしたいなぁ」
			PRINTFORML 「そのあとも、ね？」
		ELSEIF 陥落状態() == 3
			PRINTFORML 「ねえ、こうやってお互いの裸身を見せ合えるのって良いよね」
			PRINTFORML 「お触り禁止、なんてね。そうなったら%CALLNAME:MASTER%には辛いだろうけど」
		ENDIF
		PRINTFORML 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが浴室に入ってきて出て行った
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		IF 陥落状態() == 2
			PRINTFORML 「……あ、ちょっと見取れてただけだから、また後でね」
		ELSEIF 陥落状態() == 1
			PRINTFORML 「っ！　あ、ご、ごめん、すぐ出て行くから」
		ELSE
			PRINTFORML 「……」
		ENDIF
		PRINTFORML
		RETURN 1
