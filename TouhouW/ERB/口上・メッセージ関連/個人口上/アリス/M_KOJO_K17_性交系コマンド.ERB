;-------------------------------------------------
IF LOCAL && TALENT:処女 == 1
	CALL K17_MESSAGE
	CALL K17_CLL_COUNTER_REACTIONMESSAGE
	CALL K17_処女喪失(TARGET)
	RETURN 1
ENDIF

;-------------------------------------------------
;体勢分岐
;正常位　立位
;-------------------------------------------------
IF CFLAG:K17C_CF_喘ぎタイプ & K17C_喘ぎF_襲い
	;襲い系
	;拘束系みる
	IF TEQUIP:拘束
		cStyle = 拘束されて
	ELSEIF TEQUIP:縄
		cStyle = 縛られて
	ELSE
		cStyle = 無理矢理
	ENDIF
ELSE
	;ねだり系
	IF TFLAG:SELECTCOMの分岐
		cStyle = 立ったまま向き合って
	ELSE
