	CALL TRAIN_MESSAGE
	CALL K17_処女喪失(TARGET)
	SIF !RESULT
		CALL M_KOJO_MESSAGE_COM_K17_睡眠愛撫
ELSE
	CALL K17_SET_KOJO_COLOR
	CALL M_KOJO_MESSAGE_COM_K17_60_1
	PRINTW
ENDIF
