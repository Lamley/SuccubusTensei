iTarget_Num = GET_TARGETNUM()
CALL K17_SET_AUTO_PVOICE(K17_GRT_EX(),K17C_部位V)
;-------------------------------------------------
;60,正常位
;処女
;記入チェック（=0, 非表示、1, 表示）
