;-------------------------------------------------
;60,正常位,立位
;非処女　受け_おねだり系
;恋慕　一人　挿入瞬間のみ（体勢変え×
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
{
IF LOCAL && CFLAG:K17C_CF_喘ぎタイプ == K17C_受け_おねだり系 && K17_GRT(TARGET) >= K17C_恋慕 && 
	TCVAR:V挿入管理フラグ == -1 && iTarget_Num == 1
}
	;挿入
	CALL K17_MESSAGE
	IF FIRSTTIME(@"{SELECTCOM}口上-{K17C_受け_おねだり系}")
		;初回
		PRINTFORML 「ひゃぁぁんっ%K17_HEART()%　%cStyle%っ入って来たぁ%K17_HEART()%」
		PRINTFORML （身体開くってこういうこと、なのね……%K17_CNAME()%に身体開くの……私の身体が勝手に喜んじゃってる）
		PRINTW
		PRINTFORMDL %CALLNAME:MASTER%は自分の%TINKO(MASTER)%はどうか、と言わんばかりに突き立てた
		PRINTFORML 「ひゃぁんっ%K17_AP_DEC()%　そんなにっ、主張しなくてもぉ%K17_AP_DEC()%私のあそこ……%K17_CNAME()%の%K17_CALL_G(MASTER,1)%の形で、ぴったり……なのよ」
		IF TALENT:MASTER:性別 >= 2
			PRINTFORML （%K17_CALL_G(MASTER)%の大きさが気になるのかしら……？　勃起してくれて……硬くしてくれるだけで最高なのに……）
			PRINTW
			PRINTFORML 「あのね……私はその……%K17_CNAME()%の%K17_CALL_G(MASTER)%硬くて、すごく、いい……のよ？」
		ENDIF
		PRINTFORML 「あぁぁぁんっ%K17_AP_DEC()%……急に興奮しないでったらぁ」
	ELSE
		;二回目以降
		PRINTFORML 「%K17_RS("ひゃぁぁんっ/ひぅぅうぅ/あぁぁんっ/ああぁぁんっ")%%K17_HEART()%　%cStyle%っ入って来たぁ%K17_AP_DEC(1)%」
		PRINTDATAL
			DATAFORM 「おかえりなさぁい、%K17_CNAME()%%K17_AP_DEC(1)%」
			DATAFORM 「やっぱりこの体勢で%K17_RS(@"%K17_CALL_P(K17C_部位M_キス)%して/手を握って/ぎゅってして")%もらうの……%K17_RS("好きぃ/しゅきぃ")%%K17_AP_DEC(1)%」
			DATAFORM 「遠慮しないで……%K17_RS(@"いっぱい%K17_CALL_G(MASTER)%気持ちよくなってね/いつでも%K17_RS("出して/ぴゅっぴゅして/イッちゃって")%いいからね")%%K17_AP_DEC(1)%」
		ENDDATA
	ENDIF
	CALL K17_CLL_COUNTER_REACTIONMESSAGE
	PRINTFORML 「%K17_AUTO_PVOICE()%」
	PRINTFORMDL %CALLNAME:TARGET%は%CALLNAME:MASTER%の両腕を背中に回して、ぎゅっと抱き付いてきた
	PRINTFORML （%K17_RS("いっぱい/一生懸命/夢中で/気持ちよさそうに")%腰振って……なんだか%K17_RS("かわいい/しあわせぇ/嬉しいっ")%……もっと私を欲しがってね、%K17_CNAME(MASTER,1)%）
	RETURN 1
