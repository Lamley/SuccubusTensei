			ENDDATA
		ENDIF
		;せりふ
		SELECTCASE RAND:3
			CASE 0
				IF TEQUIP:拘束
					PRINTFORML 「こんな%K17_RS("私を/女の子を")%%K17_RS("動けなく/束縛/拘束/人形扱い")%してっ、恥ずかしくないのっ%K17_RS(@"この卑怯者/変態\@TALENT:MASTER:性別==2?野郎#\@/最低/レイプ魔")%っ！！」
				ELSE
					PRINTFORML 「こんな%K17_RS("私を/女の子を")%%K17_RS("無理矢理/力ずくで/好き勝手に")%してっ、恥ずかしくないのっ%K17_RS(@"この卑怯者/変態\@TALENT:MASTER:性別==2?野郎#\@/最低/レイプ魔")%っ！！」
				ENDIF
			CASE 1
				;くっころ
				PRINTFORML 「くっ……%K17_RS("信じてたのに/勝手にしなさい/絶対許さないわ/好きにしなさい/殺しなさい")%っ！！」
			CASE 2
				PRINTFORML 「今ならまだ%K17_RS("許してあげるわ/戻れるわ/引き返せるわ")%……くっぅ……お願いっ%K17_RS("やめて/やめなさい/待って")%っ！！」
		ENDSELECT
	ENDIF
	;避妊
	SIF !(TEQUIP:MASTER:避孕套) && !(TCVAR:TARGET:口服避孕薬)
