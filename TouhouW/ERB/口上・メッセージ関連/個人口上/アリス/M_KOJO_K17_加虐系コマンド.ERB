;==================================================
;107,拘束PLAY
;==================================================
@M_KOJO_MESSAGE_COM_K17_107
IF FLAG:時間停止 || CFLAG:TARGET:睡眠
	CALL TRAIN_MESSAGE
	CALL M_KOJO_MESSAGE_COM_K17_睡眠
ELSE
	CALL TRAIN_MESSAGE
	CALL M_KOJO_MESSAGE_COM_K17_107_1
	PRINTW
ENDIF
RETURN 1

@M_KOJO_MESSAGE_COM_K17_107_1
#DIM iType = 0
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	
	;-------------------------------------------------
	;タイプ保存
	;-------------------------------------------------
	IF TEQUIP:拘束
		;拘束のみが入った時　つまり装着した時タイプ一時保存
		iType = CFLAG:K17C_ID:K17C_CF_喘ぎタイプ
		CFLAG:K17C_CF_喘ぎタイプ = K17C_受け_無理矢理系
	ELSE
		;拘束が入ってないでここ来た場合は外した時
		;縄ありなら先に解除しないとこっちもできないみたい
		CFLAG:K17C_ID:K17C_CF_喘ぎタイプ = iType
	ENDIF
	;-------------------------------------------------
	;泥酔先処理
	;-------------------------------------------------
	IF TCVAR:泥酔
		IF TEQUIP:拘束
			PRINTFORML 「捕まっちゃった～あははっ」
		ELSE
			PRINTFORML 「ふーん……外すんだぁ……」
		ENDIF
	ENDIF
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初回　装着時
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(@"{SELECTCOM}口上") && TEQUIP:拘束
		PRINTFORML 「なっ……拘束って……なんかこれ……本当に動けないんだけれど……」
		PRINTFORMDL %CALLNAME:TARGET%は抵抗ことなく大人しく%CALLNAME:MASTER%に拘束されてしまった
		PRINTFORML 「こんなことしたいだなんて……変態っ」
		PRINTW
		PRINTFORMDL やはり嫌だったのだろうか……
		PRINTFORML 「くっ……好きにしなさいよ、もうっ」
		PRINTFORML （いやがっても無理矢理……されちゃうんだ……）
	ELSEIF TEQUIP:拘束
		;無理矢理中出ししたことあるか見る
		IF CFLAG:無理矢理膣射
			PRINTFORML 「もう……仕方ないわね……%K17_RS("変態っ/すけべっ/えっち/ばかっ")%！」
		ELSE
			PRINTFORML 「また……拘束……いやって言ってもするんでしょ、もうっ%K17_RS("変態っ/すけべっ/えっち/ばかっ")%！」
		ENDIF
		PRINTFORML （%K17_RS("動けない相手に、好き勝手する気/拘束して、人形みたいに扱う気")%……なんだ……）
	ELSE
		;外した時
		PRINTFORML 「ふぅ……%K17_RS("きつかったわ/開放された感じよ/恐くなっちゃった？")%」
	ENDIF
ENDIF
RETURN 1

