;==================================================
;105,縄
;縄→拘束×
;拘束→縄○
;==================================================
@M_KOJO_MESSAGE_COM_K17_105
IF FLAG:時間停止 || CFLAG:TARGET:睡眠
	CALL TRAIN_MESSAGE
	CALL M_KOJO_MESSAGE_COM_K17_睡眠
ELSE
	CALL TRAIN_MESSAGE
	CALL M_KOJO_MESSAGE_COM_K17_105_1
	PRINTW
ENDIF
RETURN 1

@M_KOJO_MESSAGE_COM_K17_105_1
#DIM iType = 0
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	
	IF TEQUIP:縄 && !TEQUIP:拘束
		;縄が入った時　つまり装着した時タイプ一時保存
		iType = CFLAG:K17C_ID:K17C_CF_喘ぎタイプ
		CFLAG:K17C_CF_喘ぎタイプ = K17C_受け_合意襲い系
	ELSE
		;縄が入ってないでここ来た場合は外した時
		;拘束もなかったらタイプ戻す
		SIF !TEQUIP:拘束
			CFLAG:K17C_ID:K17C_CF_喘ぎタイプ = iType
	ENDIF
	
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初回　装着時
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(@"{SELECTCOM}口上") && TEQUIP:縄
		PRINTFORML 「なっ……縛りたいって……ばかじゃないの？　いやよ、もうっ」
		PRINTFORMDL %CALLNAME:TARGET%は口ではそう言うが、抵抗はしなかった……
		PRINTFORML （こんな……縛り付けて、いやがっても、無理矢理……しちゃう気なんだ……）
		PRINTFORMDL %CALLNAME:MASTER%は構わず%CALLNAME:TARGET%を緊縛していった
	ELSEIF TEQUIP:縄
		;装着二回目以降
		IF TEQUIP:拘束
			;拘束縄　無理矢理になる
			PRINTFORML 「拘束までして……こんな縛るなんて、%K17_RS("変態っ/すけべっ/えっち/ばかっ")%！」
			PRINTFORML （こ……これ……%K17_RS("犯されちゃう/めちゃくちゃにされちゃう")%んだ……）
		ELSE
			;縄のみ
			PRINTFORML 「これ……いやって言ったわよね……？」
			PRINTFORML （無理矢理%K17_RS("するの好きなんだから/されるの癖になっちゃうよ")%……もうっ）
		ENDIF
	ELSE
		;外した時
		IF TEQUIP:拘束
			;まだ拘束はあり
			PRINTFORML 「拘束も外しなさいったら……」
		ELSE
			PRINTFORML 「ふぅ……今度は普通にするの……？」
		ENDIF
		
	ENDIF
	
ENDIF
RETURN 1

