@M_KOJO_MESSAGE_COM_K17_9_1
#DIM iType = 0
#DIMS cText
VARSET cText
;-------------------------------------------------
;感覚部位を選定
;-------------------------------------------------
IF TEQUIP:振動棒
	CALL K17_SET_AUTO_PVOICE(K17_GRT_EX(),K17C_部位V)
ELSEIF TEQUIP:肛用振動棒
	CALL K17_SET_AUTO_PVOICE(K17_GRT_EX(),K17C_部位A)
ELSEIF TEQUIP:淋浴
	CALL K17_SET_AUTO_PVOICE(K17_GRT_EX(),K17C_部位C)
ELSE
	CALL K17_SET_AUTO_PVOICE(K17_GRT_EX(),RAND:2?K17C_部位C#K17C_部位B)
ENDIF

;-------------------------------------------------
;9,自慰
;性癖あり
;-------------------------------------------------
IF K17_RT_FG() & K17C_RTF_性癖 && !(CFLAG:K17C_CF_喘ぎタイプ & K17C_喘ぎF_襲い) && GET_TARGETNUM() == 1
	;性癖あり　ねだり系　二人きり
	;妄想発表
	;一旦タイプ切り替え
	iType = CFLAG:K17C_CF_喘ぎタイプ
	cText = %K17_H妄想()%
	CALL K17_SET_AUTO_PVOICE(,,K17_喘ぎタイプ_H妄想(RESULT:0,RESULT:1))
	
	PRINTFORMDL %CALLNAME:MASTER%は%CALLNAME:TARGET%に普段しているように%K17_RS("自慰/オナニー/秘密の研究")%をしてほしい、と伝えた
	
	IF FIRSTTIME(@"{SELECTCOM}口上")
		;初回
		PRINTFORML 「っ！？　うぅ……人の弱みに付け込んでぇ……」
		PRINTFORMDL 恥ずかしそうにむくれながらも、ちゃんと応えてくれる%CALLNAME:TARGET%だった
		PRINTW
		PRINTFORML 「聞いて、あのね……するのはいいんだけれど……ひとりで…っえっち、すると、ね……すごく寂しくなっちゃうの……」
		PRINTFORML 「だからこの後も、ちゃんといっぱい可愛がってくれなきゃ、いやよ？」
		PRINTFORMDL %CALLNAME:MASTER%は%CALLNAME:TARGET%の頭を優しく撫でながら、膝の上に乗せて座らせた
	ELSE
		;二回目以降
		PRINTFORML 「また……させたいの？　もうっ……%K17_RS("えっちっ/変態っ/すけべっ")%」
		PRINTFORMDL %K17_RS("恥ずかしそうにむくれながらも/口ではそう言いながらも")%、ちゃんと応えてくれる%CALLNAME:TARGET%だった
		PRINTFORML 「この後、ちゃんといっぱい%K17_CNAME()%からも%K17_RS("可愛がって/気持ちよくして")%くれなきゃ、%K17_RS("イヤだから/許さないから/だめだから")%ね%K17_HEART()%」
		PRINTFORMDL %CALLNAME:MASTER%は%CALLNAME:TARGET%%K17_RS("の頭を優しく撫でながら/を優しく抱きしめながら")%、すっかり定位置になった膝の上へ%K17_RS("乗せた/座らせた")%
	ENDIF
	
	PRINTW
	CALL K17_MESSAGE
	CALL K17_CLL_COUNTER_REACTIONMESSAGE
	PRINTFORML 「%K17_AUTO_PVOICE()%」
	PRINTFORMDL "研究熱心"　な%CALLNAME:TARGET%は、目を瞑って妄想自慰に耽っていた……
	PRINTW
	PRINTFORMDL %CALLNAME:MASTER%は%CALLNAME:TARGET%に一体どんな妄想でしているのか、耳元で囁いて聞いてみた
	PRINTFORML 「%cText%%K17_RS("妄想/想像")%でっ、%K17_RS("して/しちゃい")%ました%K17_BLUSH(1)%」
	PRINTW
	PRINTFORMDL %CALLNAME:TARGET%は真っ赤になりながらも、少し興奮した様子でそう告白した
	PRINTFORMDL すっかり妄想を暴かれることがプレイの一種になってしまっているようだ……
	CFLAG:K17C_CF_喘ぎタイプ = iType
	RETURN 1
	
;-------------------------------------------------
;9,自慰
;汎用
;-------------------------------------------------
ELSE
	;性癖なしor襲い系
	CALL K17_MESSAGE
	CALL K17_CLL_COUNTER_REACTIONMESSAGE
	PRINTFORML 「%K17_AUTO_PVOICE()%」
	IF !(CFLAG:K17C_CF_喘ぎタイプ & K17C_喘ぎF_襲い)
		;ねだり系
		PRINTFORMDL 快楽を貪る%CALLNAME:TARGET%の手つきは随分と手慣れているように感じた……
	ELSE
		;襲い系
		PRINTFORMDL 嫌がりながらも言われた街道従う%CALLNAME:TARGET%だったが、手つきは手慣れているように感じた……
	ENDIF
	RETURN 1
ENDIF



