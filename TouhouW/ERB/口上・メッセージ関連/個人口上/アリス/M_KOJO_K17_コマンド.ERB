;==================================================
;189,新妻PLAY
;==================================================
@M_KOJO_MESSAGE_COM_K17_189
IF FLAG:時間停止 || CFLAG:TARGET:睡眠
	CALL TRAIN_MESSAGE
	CALL M_KOJO_MESSAGE_COM_K17_睡眠
ELSE
	CALL TRAIN_MESSAGE
	CALL M_KOJO_MESSAGE_COM_K17_189_1
	PRINTW
ENDIF
RETURN 1

@M_KOJO_MESSAGE_COM_K17_189_1
#DIM iType = 0
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;タイプ保存
	;-------------------------------------------------
	IF TEQUIP:新妻PLAY
		;拘束のみが入った時　つまり装着した時タイプ一時保存
		iType = CFLAG:K17C_ID:K17C_CF_喘ぎタイプ
	ELSE
		;拘束が入ってないでここ来た場合は外した時
		;縄ありなら先に解除しないとこっちもできないみたい
		CFLAG:K17C_ID:K17C_CF_喘ぎタイプ = iType
	ENDIF
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初回　装着時
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(@"{SELECTCOM}口上") && TEQUIP:新妻PLAY
		;初回　付けた時
		PRINTFORML 「囲裙？　これつければいいのかしら？」
		
		PRINTFORMDL %CALLNAME:MASTER%は%CALLNAME:TARGET%に新妻になってくれ、と頼んだ
		PRINTFORML 「新妻って……%K17_BLUSH()%　わかったわよ、やってあげる」
		
	ELSEIF TEQUIP:新妻PLAY
		;二回目以降　付けた時
		PRINTFORML 「また%K17_RS("囲裙つけるの/新妻になればいいの")%ね……%K17_HEART(2)%」
	ELSE
		;外した時
		PRINTFORML 「ふぅ……%K17_RS("もうおしまい？/もういいの？")%」
		RETURN 1
	ENDIF
	
	;現在の服装検査
	CALLF K17_GET_CLOTHNAME_TOP(TARGET,1)
	IF RESULT == 2
		;服着てる
		PRINTFORMDL %CALLNAME:TARGET%は%K17_GET_CLOTHNAME_TOP()%の上に囲裙をつけた
		PRINTFORML 「うん、%K17_RS("普通/いつも家事してる格好")%ね」
	ELSEIF RESULT == 1
		;内袴のみ
		PRINTFORMDL %CALLNAME:TARGET%は%K17_GET_CLOTHNAME_TOP_UNDER()%の上に囲裙をつけた
		PRINTFORML 「な、なんだか、%K17_RS("すーすーする/変な感じだ")%わ……こういうのが好きなの？」
		PRINTFORML （なんだか、裸よりえっちな気がする……）
	ELSE
		;上半身裸
		PRINTFORMDL %CALLNAME:TARGET%は裸囲裙になった
		PRINTFORML 「%K17_RS("本で読んだこと/こっちの人、これ好きって聞いたこと")%あるけど……本当にする、のね……」
		PRINTFORML （なんだか、裸よりえっちな気がする……）
	ENDIF
	
	
ENDIF
RETURN 1
