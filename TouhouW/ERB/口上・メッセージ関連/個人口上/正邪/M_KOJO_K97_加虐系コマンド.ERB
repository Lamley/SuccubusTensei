		IF RAND:3 == 0
			PRINTFORMW 「…全く」
			PRINTFORMW 「…こういうの…好きじゃないんだがね…」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「あ…」
			PRINTFORMW 「…終わりか…」
			RETURN 1
		ELSE
			PRINTFORMW 「…これ…」
			PRINTFORMW 「…腕も痺れるんだよな…」
			RETURN 1
		ENDIF
	ENDIF
