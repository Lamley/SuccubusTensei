;==================================================
;100,打屁股
;==================================================
@M_KOJO_MESSAGE_COM_K97_100
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K97_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K97_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
			;時姦中
			IF FLAG:70
				PRINTFORMW 凍てついた時の中で正邪の尻を叩き始めた。
				RETURN 1
			ELSE
				PRINTFORMW 「…んはっ！？」
				PRINTFORMW 「あっ…んあっ！？…おい！？」
				RETURN 1
			ENDIF
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 凍てついた時の中で跨った正邪の尻を叩き始めた。
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		IF RAND:3 == 0
			PRINTFORMW 「ん…あひっ…！？」
			PRINTFORMW 「…尻痛え…」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「あっ…んあ…！？」
			PRINTFORMW 「…お前さぁ…」
			RETURN 1
		ELSE
			PRINTFORMW 「いぎぃ！？…あがっ！？」
			PRINTFORMW 「ん…もう…」
			RETURN 1
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		IF RAND:3 == 0
			PRINTFORMW 「ん！？」
			PRINTFORMW 「…あ…全く…」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「あが！？…ん…」
			PRINTFORMW 「…これ…やめろって…」
			RETURN 1
		ELSE
			PRINTFORMW 「…んあ！？…ひぎぃ！？」
			PRINTFORMW 「痛…！」
			RETURN 1
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「ん…あ！？」
			PRINTFORMW 「…全く…この…」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「あ！？…叩くなって…」
			PRINTFORMW 「…痛いんだからな…」
			RETURN 1
		ELSE
			PRINTFORMW 「…んあ！？」
			PRINTFORMW 「痛いってば…」
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

