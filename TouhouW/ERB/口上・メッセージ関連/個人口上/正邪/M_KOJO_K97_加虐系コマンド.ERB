CALL M_KOJO_MESSAGE_COM_K97_104_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K97_104_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
			;時姦中
			IF FLAG:70
				PRINTFORMW 凍てついた時の中で顔に眼罩を着けた。
				RETURN 1
			ELSE
				PRINTFORMW 「ん…眼罩ね…」
				PRINTFORMW 「…何してくれるんだ？」
				RETURN 1
			ENDIF
	ENDIF
	;基本セット
	;装着
	IF TEQUIP:眼罩
		;時姦中
		IF FLAG:70
			PRINTFORMW 凍てついた時の中で正邪の顔に眼罩を着けた。
			RETURN 1
		;恋慕
		ELSEIF TALENT:恋慕
			IF RAND:3 == 0
				PRINTFORMW 「ん…これ…」
				PRINTFORMW 「…中々興奮するな…」
				RETURN 1
			ELSEIF RAND:2 == 0
				PRINTFORMW 「あ…」
				PRINTFORMW 「…趣向凝らしてくれるじゃないか…」
				RETURN 1
			ELSE
				PRINTFORMW 「…全く…」
				PRINTFORMW 「楽しませてくれるのな…」
				RETURN 1
			ENDIF
		;屈服3
		ELSEIF MARK:不埒刻印 == 3
			IF RAND:3 == 0
				PRINTFORMW 「ん…お？」
				PRINTFORMW 「…これ…いいな…」
				RETURN 1
			ELSEIF RAND:2 == 0
				PRINTFORMW 「…んあ？」
				PRINTFORMW 「…まぁ好きにしろ…」
				RETURN 1
			ELSE
				PRINTFORMW 「…ん？」
				PRINTFORMW 「…これも悪くはないけど…」
				RETURN 1
			ENDIF
		;それ以外
		ELSE
			IF RAND:3 == 0
				PRINTFORMW 「…あ？」
				PRINTFORMW 「…この…」
				RETURN 1
			ELSEIF RAND:2 == 0
				PRINTFORMW 「お…？」
				PRINTFORMW 「…倒錯的だな…」
				RETURN 1
			ELSE
				PRINTFORMW 「…ん？」
				PRINTFORMW 「構わないけどもね…」
				RETURN 1
			ENDIF
		ENDIF
	;解除
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「ん…」
			PRINTFORMW 「…終わりか…」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「あ…」
			PRINTFORMW 「…いいのか…」
			RETURN 1
		ELSE
			PRINTFORMW 「…」
			PRINTFORMW 「外すのか…」
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

