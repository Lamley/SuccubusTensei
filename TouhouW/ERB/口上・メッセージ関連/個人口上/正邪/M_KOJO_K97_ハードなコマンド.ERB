;==================================================
;140,強制口交
;==================================================
@M_KOJO_MESSAGE_COM_K97_140
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K97_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K97_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
			;時姦中
			IF FLAG:70
				PRINTFORMW 凍てついた時の中で正邪の口腔を深く犯し始めた。
				RETURN 1
			ELSE
				PRINTFORMW 「…んご…ぐ…」
				PRINTFORMW 「ん…じゅる…」
			ENDIF
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 凍てついた時の中で正邪の口腔を深く犯し始めた。
	;恋慕
	ELSEIF TALENT:恋慕
		IF RAND:3 == 0
			PRINTFORMW 「んぐ！？…あ…」
			PRINTFORMW 「…ん…ちゅる…」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「っ…！？…じゅる…」
			PRINTFORMW 「…ん…んぐ…！」
		ELSE
			PRINTFORMW 「…ん…！？」
			PRINTFORMW 「んぶ…ぐっ…！」
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		IF RAND:3 == 0
			PRINTFORMW 「あがっ！？」
			PRINTFORMW 「…んご…ぐ…！」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「んが…！」
			PRINTFORMW 「ん…っ…！」
		ELSE
			PRINTFORMW 「っ…！？」
			PRINTFORMW 「…じゅぶ…」
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「がっ…！？」
			PRINTFORMW 「…じゅる…あ…」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「…んぶ！？」
			PRINTFORMW 「…んっ…あ…」
		ELSE
			PRINTFORMW 「…んあ！？」
		ENDIF
	ENDIF
ENDIF
RETURN 1

