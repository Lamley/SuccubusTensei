		ELSEIF RAND:2 == 0
			PRINTFORMW 「いや、ねぇ…」
			PRINTFORMW 「…話うまいな、お前」
			RETURN 1
		ELSE
			PRINTFORMW 「…口達者なことで」
			PRINTFORMW 「悪い気はしないけどな」
			RETURN 1
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「…」
			PRINTFORMW 「そう…」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「ん…」
			PRINTFORMW 「…こういうの、いいな」
			RETURN 1
		ELSE
			PRINTFORMW 「…」
			PRINTFORMW 正邪は顔を赤らめている。
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1