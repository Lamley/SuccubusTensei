;特殊コマンド
;=================================================
;-------------------------------------------------
;180,潤滑液
;-------------------------------------------------
@M_KOJO_MESSAGE_COM_K97_180
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K97_180_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K97_180_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF FLAG:70
			PRINTFORMW 凍てついた時の中で正邪の体に潤滑液を塗りたくった。
			RETURN 1
		ELSE
			PRINTFORMW 「いや、濡らしてくれるのはありがたいが、面倒だな」
			RETURN 1
		ENDIF
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 凍てついた時の中で正邪の体に潤滑液を塗りたくった。
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		IF RAND:3 == 0
			PRINTFORMW 「…ん…」
			PRINTFORMW 「…冷たいな…」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「あ…これ…」
			PRINTFORMW 「…ん…」
			RETURN 1
		ELSE
			PRINTFORMW 「…あ…」
			PRINTFORMW 「…慣れないな…冷たい…」
			RETURN 1
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		IF RAND:3 == 0
			PRINTFORMW 「…ん…」
			PRINTFORMW 「…頼む…」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「あ…」
			PRINTFORMW 「…まぁ頼むよ…」
			RETURN 1
		ELSE
			PRINTFORMW 「…これ…」
			PRINTFORMW 「…あまり…好きじゃないな」
			RETURN 1
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「…全く…」
			PRINTFORMW 「…そんなに濡れてないか…」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「あ…」
			PRINTFORMW 「…いいけどさ…」
			RETURN 1
		ELSE
			PRINTFORMW 「ん…」
			PRINTFORMW 「…気持ち悪…」
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

