@M_KOJO_MESSAGE_COM_K97_189_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「ん…どうした？似合わないってか？」
		RETURN 1
	ENDIF
	;基本セット
	;開始
	IF TEQUIP:新妻PLAY
		;恋慕
		IF TALENT:恋慕
			IF RAND:3 == 0
				PRINTFORMW 「どうした？」
				PRINTFORMW 「…好きな事してやるよ」
				RETURN 1
			ELSEIF RAND:2 == 0
				PRINTFORMW 「あ？」
				PRINTFORMW 「囲裙に合わないってか？」
				RETURN 1
			ELSE
				PRINTFORMW 「ん…貴方…」
				PRINTFORMW 「…何にする？」
				RETURN 1
			ENDIF
		;屈服3
		ELSEIF MARK:不埒刻印 == 3
			IF RAND:3 == 0
				PRINTFORMW 「…構わないけどもな…」
				PRINTFORMW 「…似合わんだろ、私…」
				RETURN 1
			ELSEIF RAND:2 == 0
				PRINTFORMW 「…どうした？」
				PRINTFORMW 「…囲裙引っ張るなって…」
				RETURN 1
			ELSE
				PRINTFORMW 「…あ？」
				PRINTFORMW 「…裸囲裙で興奮するのか？」
				RETURN 1
			ENDIF
		;それ以外
		ELSE
			IF RAND:3 == 0
				PRINTFORMW 「…ん…」
				PRINTFORMW 「…あまりジロジロ見んなって言ってるだろ」
				RETURN 1
			ELSEIF RAND:2 == 0
				PRINTFORMW 「…あ…なんだ？」
				PRINTFORMW 「…ふん、似合っていると言われてもな」
				RETURN 1
			ELSE
				PRINTFORMW 「…いいけどもな」
				PRINTFORMW 「やっぱり好きじゃないな…」
				RETURN 1
			ENDIF
		ENDIF
	;終了
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「…ん」
			PRINTFORMW 「…脱ぐのか…」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「…あ…」
			PRINTFORMW 「…このままでも構わなかったんだけどな…」
			RETURN 1
		ELSE
			PRINTFORMW 「あ、囲裙脱ぐのか」
			PRINTFORMW 「…少し残念だな…」
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1
