;==================================================
;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K97_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K97_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K97_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
			;時姦中
			IF FLAG:70
				PRINTFORMW 凍てついた時の中で正邪の手に陰茎を握らせた。
			ELSE
				PRINTFORMW 「…あ？」
				PRINTFORMW 「…わかった、ほら、イカせてやるよ」
			ENDIF
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 凍てついた時の中で正邪の手に陰茎を握らせた。
	ELSEIF BASE:PLAYER:2 > 8000
		IF RAND:3 == 0
			PRINTFORMW 「お？もう出そうだな」
			PRINTFORMW そう言うと正邪は一層激しく扱き始めた。
			PRINTFORMW 「ほらほら、イッちまいな！」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「おーおーでかくなってきやがった」
			PRINTFORMW そう言うと正邪は一層激しく扱き始めた。
			PRINTFORMW 「オラッ！ザーメンぶちまけなって！」
		ELSE
			PRINTFORMW 「ん？カウパー溢れて来たな」
			PRINTFORMW そう言うと正邪は一層激しく好き始めた。
			PRINTFORMW 「ふうん…こういうの好きなのか」
		ENDIF
	;恋慕
	ELSEIF TALENT:恋慕
		IF RAND:3 == 0
			PRINTFORMW 「ん…ほら、」
			PRINTFORMW 「さっさと出しな」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「激しくした方がいいか？」
			PRINTFORMW 「…どんどん固くなってる」
		ELSE
			PRINTFORMW 「おや？手でイクのか？」
			PRINTFORMW 「暴れるなよ、出そうになったら言え」
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		IF RAND:3 == 0
			PRINTFORMW 「ほら、シコシコしてやるよ」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「ん…いつ見ても気味悪いな」
			PRINTFORMW 「あんまり動くなよ、ほら」
		ELSE
			PRINTFORMW 「いっぱい出せよ」
			PRINTFORMW 「…オナニーと変わらないんじゃないかこれ」
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「汚えな」
			PRINTFORMW 「…ちゃんと洗ってんのか？」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「私がイイって言うまで出すんじゃないよ」
			PRINTFORMW 「…勝手に出したら罰ゲームだ」
		ELSE
			PRINTFORMW 「…ん…ここか？」
			PRINTFORMW 「ビクビクしてる…気持ち悪いな」
		ENDIF
	ENDIF
ENDIF
RETURN 1

