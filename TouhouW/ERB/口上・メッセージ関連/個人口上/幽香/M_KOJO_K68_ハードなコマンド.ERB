;==================================================
;150,双乳間抽插
;==================================================
@M_KOJO_MESSAGE_COM_K68_150
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K68_150_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K68_150_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;時姦中
	IF FLAG:70
		RETURN 1
	;睡姦中
	ELSEIF CFLAG:MASTER:イタズラ == 2
		RETURN 1
	ELSEIF TCVAR:68:泥酔 == 1
		
	;恋慕or恋人
	ELSEIF TALENT:68:恋慕 || TALENT:68:恋人
		SELECTCASE RAND:2
			CASE 0
				PRINTFORM 「んっ、ぁ、あっ
				CALL HEARTCB_K68(0xD02830)
				PRINTFORML 　もう、こんなのが気持ちいいの……？」
			CASEELSE
				PRINTFORM 「んっ
				CALL HEARTCB_K68(0xD02830)
				PRINTFORML ！　はぁ、胸とセックスしてるつもり……？」
		ENDSELECT
		PRINTFORMDW 胸で快感を貪る%CALLNAME:MASTER%に呆れつつも、幽香は突き上げに合わせて声を上げている
	;それ以外
	ELSE
		SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「うぅ……こんな、恥ずかしいこと……」
			CASEELSE
				PRINTFORML 「んっ……早くイきなさいよ……！」
		ENDSELECT
		PRINTFORMDW 胸の中で性交しているかのように厭らしい音を立てる%CALLNAME:MASTER%のペニスから、幽香は目をそらしている
	ENDIF
ENDIF

RETURN 1
