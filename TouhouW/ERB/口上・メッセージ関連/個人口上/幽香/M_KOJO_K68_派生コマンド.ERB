	;恋慕or恋人
	ELSEIF TALENT:68:恋慕 || TALENT:68:恋人
		IF EXP:68:子宮口開発経験 >= 50
			IF NOWEX:MASTER:射精
				PRINTFORM 「出してッ、私の中に、直接注ぎ込んでぇええええッ
				CALL HEARTCB_K68(0xD02830,3)
				PRINTFORMW 」
			ELSE
				SELECTCASE RAND:2
					CASE 0
						PRINTFORM 「ぅん、ぁ、あはぁッ
						CALL HEARTCB_K68(0xD02830,2)
						PRINTFORM 　許して、もう許してぇ、主人公ぁああッ
						CALL HEARTCB_K68(0xD02830,2)
						PRINTFORML 」
					CASEELSE
						PRINTFORM 「ぁあああッ
						CALL HEARTCB_K68(0xD02830,2)
						PRINTFORM ！　大好きぃッ、主人公ぁああッ
						CALL HEARTCB_K68(0xD02830,3)
						PRINTFORML 」
				ENDSELECT
				PRINTFORMDW 雌としての本能を刺激され、幽香はすっかり%CALLNAME:MASTER%に服従してしまっている
			ENDIF
		ELSE
			SELECTCASE RAND:2
				CASE 0
					PRINTFORM 「ぅん、あぁ……ッ！　\@ TALENT:MASTER:性別 == 2 ? 貴方 # 貴女 \@、興奮しすぎよ……ぉッ
					CALL HEARTCB_K68(0xD02830)
					PRINTFORML 」
				CASEELSE
					PRINTFORM 「ぃ、っぐう……！　ま、待って、主人公ぁ……ッ
					CALL HEARTCB_K68(0xD02830)
					PRINTFORML ！」
			ENDSELECT
			PRINTFORMDW 幽香は苦しそうにしているが、雌としての本能に目覚めつつある
		ENDIF
	;それ以外
	ELSE
		IF EXP:68:子宮口開発経験 >= 50
			IF NOWEX:MASTER:射精
				PRINTFORM 「いや、いやぁあああああッ
				CALL HEARTCB_K68(0xD02830,2)
				PRINTFORML ！」
				PRINTFORM （逆らえない……このまま、種付けされちゃう……ッ
				CALL HEARTCB_K68(0xD02830)
				PRINTFORMW ）
			ELSE
				SELECTCASE RAND:2
					CASE 0
						PRINTFORM 「ひぃん、ぁあ
						CALL HEARTCB_K68(0xD02830)
						PRINTFORM ！　いや、こんなの、いやぁあああッ
						CALL HEARTCB_K68(0xD02830,2)
						PRINTFORML ！」
					CASEELSE
						PRINTFORM 「ッはぁああ
						CALL HEARTCB_K68(0xD02830)
						PRINTFORM ！　ひぃッ、こんなのぉ……ッ
						CALL HEARTCB_K68(0xD02830,2)
						PRINTFORML 」
				ENDSELECT
				PRINTFORMDW 幽香は何とか流されまいとしているが、子宮を犯される度にその抵抗が弱まっていく
			ENDIF
		ELSE
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「っ、ぐぅ！　ちょっと、やめなさい！」
				CASEELSE
					PRINTFORML 「うう……、く、ぁっ……！」
			ENDSELECT
			IF EXP:68:子宮口開発経験 >= 25
				PRINTFORM 「こ、んな……きゃああッ
				CALL HEARTCB_K68(0xD02830)
				PRINTFORMW ！？」
			ELSE
				PRINTFORMW 「こ、んなの、痛いだけ、よ……！」
			ENDIF
		ENDIF
	ENDIF
ENDIF
