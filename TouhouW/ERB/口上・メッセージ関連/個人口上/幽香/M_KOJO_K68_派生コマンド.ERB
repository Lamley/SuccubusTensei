;==================================================
;14,玩弄Ｓ状結腸
;==================================================
@M_KOJO_MESSAGE_SCOM_K68_14
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K68_14_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K68_14_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;時姦中
	IF FLAG:70
		RETURN 1
	;睡姦中
	ELSEIF CFLAG:MASTER:イタズラ == 2
		RETURN 1
	ELSEIF TCVAR:68:泥酔 == 1
		RETURN 1
	;A感覚Lv3以下andA経験100以下
	ELSEIF ABL:68:2 < 3 && EXP:68:Ａ経験 < 100
		PRINTFORML 「くうっ、あぁ、はぁあ……ッ！」
		PRINTFORMDW 幽香は尻穴を責められるのにまだ慣れていないようで、苦しそうにしている
	;恋慕or恋人
	ELSEIF TALENT:68:恋慕 || TALENT:68:恋人
		SELECTCASE RAND:2
			CASE 0
				PRINTFORM 「あっ、あぁああああッ
				CALL HEARTCB_K68(0xD02830,2)
				PRINTFORM ！　もっと、もっとぉッ
				CALL HEARTCB_K68(0xD02830,2)
				PRINTFORML 」
			CASEELSE
				PRINTFORM 「そこ、いいぃッ
				CALL HEARTCB_K68(0xD02830,2)
				PRINTFORM ！　主人公っ、好きっ、大好きぃッ
				CALL HEARTCB_K68(0xD02830,2)
				PRINTFORML 」
		ENDSELECT
		PRINTFORMDW %CALLNAME:MASTER%のペニスで感じているのを隠す様子もなく、幽香は髪を振り乱して悶えている
	;それ以外
	ELSE
		SELECTCASE RAND:2
			CASE 0
				PRINTFORM 「ひっ！？　いやっ、ぁああああッ
				CALL HEARTCB_K68(0xD02830,2)
				PRINTFORML ！？　なに、これぇえええッ！？」
			CASEELSE
				PRINTFORM 「やめっ、そこは、いやぁああああッ
				CALL HEARTCB_K68(0xD02830,2)
				PRINTFORML ！」
		ENDSELECT
		PRINTFORMDW 弱点への責めに声を我慢する余裕はないらしく、幽香は髪を振り乱して悶えている
	ENDIF
ENDIF
;未記入
;CALLFORM M_KOJO_MESSAGE_COM_K68_{SELECTCOM}_1
RETURN 1
