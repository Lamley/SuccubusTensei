;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K68_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K68_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K68_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;時姦中
	IF FLAG:70
		RETURN 1
	;睡姦中
	ELSEIF CFLAG:MASTER:イタズラ == 2
		RETURN 1
	ELSEIF TCVAR:68:泥酔 == 1
		RETURN 1
	;恋慕or恋人
	ELSEIF TALENT:68:恋慕 || TALENT:68:恋人
		IF NOWEX:MASTER:射精
			IF ABL:68:技巧 > 3
				PRINTFORM 「\@ TALENT:MASTER:性別 == 2 ? 貴方 # 貴女 \@の弱点、ぜーんぶ知ってるのよ……？　ほらっ、我慢しないで、この手の中に出しなさい……
				CALL HEARTCB_K68(0xD02830)
				PRINTFORMW 」
			ELSE
				PRINTFORMW 「え、出すの……？　んっ……あの、いつ出てくるの……？」
			ENDIF
		ELSE
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「ん……気持ちいい？　ふふっ、\@ TALENT:MASTER:性別 == 2 ? 貴方 # 貴女 \@の顔を見ればわかるわよ」
				CASEELSE
					PRINTFORM 「\@ BASE:MASTER:勃起 >= 1000 ? はぁ……もう、こんなに硬くして # すぐに大きくしてあげるからね \@……ふふっ
					CALL HEARTCB_K68(0xD02830)
					PRINTFORML 」
			ENDSELECT
			PRINTFORMDW 幽香は楽しげな様子で%CALLNAME:MASTER%のペニスを扱きながら、上目遣いで見つめてくる
		ENDIF
	;それ以外
	ELSE
		IF NOWEX:MASTER:射精
			PRINTFORMW 「えっ、イきそう……？　ちょ、ちょっと待って……ごくり」
		ELSE
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「まぁ、手ならいいけど……出すときはちゃんと言ってね……？」
				CASEELSE
					IF BASE:MASTER:勃起 >= 1000
						PRINTFORML 「あ、\@ TALENT:MASTER:性別 == 2 ? 貴方 # 貴女 \@の、\@ TALENT:MASTER:形状 == 6 ? こうして近くで見ると、本当に大きいわね # もうこんなに硬くて、熱い \@……」
					ELSE
						PRINTFORML 「これが、あんなに大きくなるのよね……わっ、どんどん硬くなってる……」
					ENDIF
			ENDSELECT
			PRINTFORMDW 幽香は顔を朱に染めながら、%CALLNAME:MASTER%が気持ちよくなるようにペニスを扱いている
		ENDIF
	ENDIF
ENDIF

RETURN 1

