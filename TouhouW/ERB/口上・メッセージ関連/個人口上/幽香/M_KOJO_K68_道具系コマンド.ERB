;40,跳蛋
;==================================================
@M_KOJO_MESSAGE_COM_K68_40
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K68_40_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K68_40_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;時姦中
	IF FLAG:70
		RETURN 1
	;睡姦中
	ELSEIF CFLAG:MASTER:イタズラ == 2
		RETURN 1
	ELSEIF TCVAR:68:泥酔 == 1
		RETURN 1
	;恋慕or恋人
	ELSEIF TALENT:68:恋慕 || TALENT:68:恋人
		SELECTCASE RAND:2
			CASE 0
				PRINTFORM 「ぁん……ふぁああッ
				CALL HEARTCB_K68(0xD02830)
				PRINTFORMW 　\@ TALENT:MASTER:性別 == 2 ? 貴方 # 貴女 \@より、気持ちいいかも……なんて」
			CASEELSE
				PRINTFORM 「うう……ふっ、ぅううッ
				CALL HEARTCB_K68(0xD02830)
				PRINTFORMW 　はぁ、やっぱり、声我慢できない……」
		ENDSELECT
	;それ以外
	ELSE
		SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「うぁっ……んんっ！　やっ、こんな小さい玩具で……」
			CASEELSE
				PRINTFORMW 「くぅっ……ぅん！　ねぇ、もういいでしょう……？　いつまで当て……んっ！」
		ENDSELECT
	ENDIF
ENDIF

RETURN 1
