			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;Futanari
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
