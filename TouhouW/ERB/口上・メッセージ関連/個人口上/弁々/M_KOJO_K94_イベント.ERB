;==================================================
;デート帰りに告白（恋人が付く）
;==================================================
@M_KOJO_SPEVENT_K94_2(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;告白内容
	;この告白内容表示後に受けるかどうかの選択分が表示されます
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
			;初回
			IF FIRSTTIME(1001)
				PRINTFORMW 「ねえ、%CALLNAME:MASTER%、」
				PRINTFORMW 「%CALLNAME:MASTER%は好きな人とかいる？」
				PRINTFORMW 「私、%CALLNAME:MASTER%に恋しちゃった」
				PRINTFORMW 「%CALLNAME:MASTER%と一緒にいるとドキドキするの」
				PRINTFORMW 「%CALLNAME:MASTER%のことを考えると夜眠れなくなって」
				PRINTFORMW 「八橋や雷鼓さんと一緒に演奏している時も%CALLNAME:MASTER%のことが頭から出口ないの」
				PRINTFORMW 「%CALLNAME:MASTER%が他の女と一緒にいると嫉妬しちゃうし」
				PRINTFORMW 「%CALLNAME:MASTER%が誰かと話をしていると私も%CALLNAME:MASTER%ともっと話をしたいと思っちゃう」
				PRINTFORMW 「%CALLNAME:MASTER%と一緒になりたい」
				PRINTFORMW 「%CALLNAME:MASTER%のモノになりたい」
				PRINTFORMW 「%CALLNAME:MASTER%を私のモノにしたい」
				PRINTFORMW 「そんな想いが毎晩%CALLNAME:MASTER%の顔と一緒に夢に出て来るの」
				PRINTFORMW 「私を」
				PRINTFORMW 「%CALLNAME:MASTER%の恋人にしてください」
				PRINTFORMW 弁弁は%CALLNAME:MASTER%の袖を掴みながらそう告白した。
			;二回目の告白
			ELSE
				PRINTFORMW 「%CALLNAME:MASTER%、」
				PRINTFORMW 帰路に着くと弁弁は意を決したように口を開いた。
				PRINTFORMW 「私、やっぱり%CALLNAME:MASTER%のこと好きだわ」
				PRINTFORMW 弁弁はそう言って%CALLNAME:MASTER%に告白した。
				PRINTFORMW 「%CALLNAME:MASTER%のこと、諦めきれない」
				PRINTFORMW 「独占欲が強いのね、私」
				PRINTFORMW 「私とは付き合ってもらえないかしら？」
				PRINTFORMW 「…駄目？」
			ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白オッケー（恋人が付く）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORMW 「…」
		PRINTFORMW 弁弁は黙って顔を近づけると
		PRINTFORMW 「ちゅっ…」
		PRINTFORMW 優しくキスをしてきた。
		PRINTFORMW 「ふふっ、私、%CALLNAME:MASTER%のモノになっちゃった…」
		PRINTFORMW %CALLNAME:MASTER%はあっけに取られながらも弁弁を抱きとめた。
		PRINTFORMW （下克上だなんだと言ってたこともあったけど…これも幸せね）
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白失敗
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 「…」
		PRINTFORMW 「諦めないから」
		PRINTFORMW 弁弁はそれだけ言ってその場を後にした。
		PRINTFORMW （これで諦めるぐらいなら…）
		RETURN 1
	ENDIF

ENDIF
RETURN 1

;==================================================
;デート帰り
;==================================================
@M_KOJO_SPEVENT_K94_3(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;通常帰宅
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF CFLAG:318 == 1
			PRINTFORMW 「ふんっ…！」
			PRINTFORMW 弁弁は憤怒ながらぷいっと顔を背けて帰っていった。
		ELSEIF TALENT:恋人
			PRINTFORM 「じゃあね、
			CALL MASTER_CALLNAME_K94
			PRINTFORMW 」
			PRINTFORMW 弁弁はそう言いながら%CALLNAME:MASTER%の方へ向き直って礼を言った。
			PRINTFORM 「また、明日、
			CALL MASTER_CALLNAME_K94
			PRINTFORMW とデートできるの、楽しみにしているわよ」
		ELSEIF TALENT:恋慕
			PRINTFORMW 「今日はもうお開きね」
			PRINTFORM 「
			CALL MASTER_CALLNAME_K94
			PRINTFORMW 、貴方も早く帰りなさい」
			PRINTFORMW 「…じゃあね、バイバイ」
			PRINTFORMW 弁弁はそう言って名残惜しげに手を振って去っていった。
		ELSEIF TALENT:思慕
			PRINTFORM 「それじゃあ、また明日ね、
			CALL MASTER_CALLNAME_K94
			PRINTFORMW も元気でね」
			PRINTFORMW 弁弁はそう言ってそそくさと帰っていった。
			PRINTW
		ELSE
			PRINTFORMW 「そろそろ帰るわ」
			PRINTFORM 「
			CALL MASTER_CALLNAME_K94
			PRINTFORMW もあんまり遊んでると痛い目見るわよ」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印消去イベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:恋人
			PRINTFORMW 「…いいわ、許してあげる」
			PRINTFORMW 「でもね…あんまり変なことしちゃ駄目よ？」
			PRINTFORM 「
			CALL MASTER_CALLNAME_K94
			PRINTFORMW のこと、信頼してあげるからね」
		ELSE
			PRINTFORMW 「ん、わかった」
			PRINTFORMW 「あんまり人を怒らせることをしちゃ駄目よ」
			PRINTFORMW 「私は優しいからね、許してあげる」
			PRINTFORMW 弁弁はどこか怒気を含んだ笑顔でそう言った。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印2>1レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:恋人
			PRINTFORMW 「いいわ、少しだけ許してあげる」
			PRINTFORMW 「まあまだ怒ってるけどね」
			PRINTFORM 「
			CALL MASTER_CALLNAME_K94
			PRINTFORMW 、もう少し誠意を見せなさい」
		ELSE
			PRINTFORMW 「…」
			PRINTFORMW 「そう簡単に許してあげないわ」
			PRINTFORMW 「少しだけなら許してあげてもいいけど」
			PRINTFORMW 弁弁はどこかぎこちない笑顔でそう言った。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印3>2レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF TALENT:恋人
			PRINTFORMW 「…わかったわ」
			PRINTFORMW 「でもまだ怒ってるからね」
			PRINTFORM 「
			CALL MASTER_CALLNAME_K94
			PRINTFORMW 、もうちょっと信用できる態度を取りなさい」
		ELSE
			PRINTFORMW 「…」
			PRINTFORMW 「まだ怒ってるんだけど」
			PRINTFORMW 弁弁は%CALLNAME:MASTER%を睨みつけながらそう言った。
		ENDIF
	ENDIF
	PRINTFORML
ENDIF
RETURN 1

