@M_KOJO_MESSAGE_SCOM_K94_1_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		IF RAND:3 == 0
			PRINTFORMW 「んむ%UNICODE(0x2665) *1%んっ%UNICODE(0x2665) *1%ちゅっ%UNICODE(0x2665) *1%ちゅっ%UNICODE(0x2665) *1%ずぶっ%UNICODE(0x2665) *1%んぷっ%UNICODE(0x2665) *1%ずぶっ%UNICODE(0x2665) *1%」
			PRINTFORMW 「気持ちいいお汁…%UNICODE(0x2665) *1%…早く出して…%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「ずっ%UNICODE(0x2665) *1%じゅぷっ%UNICODE(0x2665) *1%ずっ%UNICODE(0x2665) *1%んぷっ%UNICODE(0x2665) *1%ずずっ%UNICODE(0x2665) *1%ずっ%UNICODE(0x2665) *1%」
