			PRINTFORMW 「ちゅっ%UNICODE(0x2665) *1%んふずっ%UNICODE(0x2665) *1%んぷっ…%UNICODE(0x2665) *1%…んっ%UNICODE(0x2665) *1%ずっ%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「んぷっ%UNICODE(0x2665) *1%んぐっ%UNICODE(0x2665) *1%んちゅ%UNICODE(0x2665) *1%じゅぷっ%UNICODE(0x2665) *1%じゅぷっ%UNICODE(0x2665) *1%ずぶっ%UNICODE(0x2665) *1%んっ%UNICODE(0x2665) *1%んむ%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「じゅぷっ%UNICODE(0x2665) *1%じゅぷっ%UNICODE(0x2665) *1%」
			PRINTFORM 「他の女に
			CALL MASTER_CALLNAME_K94
			PRINTFORMW のザーメンは渡さないんだから…%UNICODE(0x2665) *1%」
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
