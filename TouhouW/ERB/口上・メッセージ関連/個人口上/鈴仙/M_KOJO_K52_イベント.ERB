;デート帰り
;==================================================
@M_KOJO_SPEVENT_K52_3(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;通常帰宅
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
	;住み込み
		IF FLAG:15 == 52
			PRINTFORMW 「うーん、遊んだ遊んだ！」
			IF RAND:2
			PRINTFORMW 「楽しかったわよ、%CALLNAME:MASTER%%UNICODE(0x2665)%」
			ELSE
			PRINTFORMW 「また今度誘ってね？」
			ENDIF
		;恋人
		ELSEIF TALENT:MASTER:恋人 == NO:63 && TALENT:63:恋人 == NO:63
			PRINTFORMW 「うーん、遊んだ遊んだ！」
			IF RAND:2
			PRINTFORMW 「%CALLNAME:MASTER%と一緒にいると時間が飛ぶね」
			ELSE
			PRINTFORMW 「もう帰らないといけないのは残念ね」
			ENDIF
	;	恋慕
		ELSEIF TALENT:恋慕
			IF RAND:2
			PRINTFORMW 「はぁ、もう帰ってきちゃった」
			PRINTFORMW 「……あの、また誘ってね？」
			ELSE
			PRINTFORMW 「ふぅ、お疲れ様」
			PRINTFORMW 「ちょっと疲れたかも……」
			ENDIF
		;思慕
			ELSEIF TALENT:思慕
			IF RAND:2
			PRINTFORMW 「もうちょっと一緒に居たいけど、疲れは目に悪いのよね」
			PRINTFORMW 「また連れてってくれる？」
			ELSE
			PRINTFORMW 「ふぅ、遊び疲れたー！」
			PRINTFORMW 「仕事以外であまり外出ことがないから、こういうのもたまにいいわね」
			ENDIF
		;思慕じゃない
		ELSE
			PRINTFORMW 「……その、また誘ってくれると、うれしいです」
		ENDIF
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印消去イベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF GETBIT (CFLAG:1490,0)
			PRINTFORML 「も～……本当に」
			PRINTFORML 「人の憤怒なんだと思ってるのよ」
			PRINTFORMW 少女はそう言うと%CALLNAME:MASTER%の腕に絡みついた。
		ELSE
			PRINTFORML 「人を怒らせといて～」
			PRINTFORML 頬を膨らませた鈴仙は、ぷいっ！とそっぽ向き、
			PRINTFORML 「今度は……許さないからね？」
			PRINTFORMW 流し目で%CALLNAME:MASTER%を見つめてはそう言った。
		ENDIF
		PRINTFORML 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印2>1レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF GETBIT (CFLAG:1490,0)
			PRINTFORML 「ホントにもぉー、そんなに、優しくしないでよ」
			PRINTFORML デートから戻った鈴仙はムズムズするこの気持ちをどうにもできずにいた。
		ELSE
			PRINTFORML 「まだ……許してないからね？」
			PRINTFORML そう言うも鈴仙はご心情そうに見えた。
		ENDIF
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印3>2レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF GETBIT (CFLAG:1490,0)
			PRINTFORML 「……あれくらいで許すと思ったら大間違いなんだからね！」
			PRINTFORML そう言いながらも、目の端に浮かんでいた険が僅かに緩んだ。
		ELSE
			PRINTFORML 「絶対に、絶対に許さないから……！」
			PRINTFORMW 鈴仙はそう言っては%CALLNAME:MASTER%から出口ていった。そして、聞こえるか聞こえないかの声で、
			PRINTFORML 「……たぶん」
			PRINTFORML とつぶやいた。
		ENDIF
		PRINTFORML
		RETURN 1
	ENDIF
	PRINTFORML
ENDIF
RETURN 1

