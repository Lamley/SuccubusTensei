;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K11_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K11_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K11_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM) && !FLAG:時間停止
		PRINTFORML 「手でしごいてくれって？わかった」
		PRINTFORMW 「うーん、こんな感じか？」
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMW 
		RETURN 1
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		PRINTFORML 「ふふふ、こんなにビクビクさせちゃって…」
		PRINTFORML 魔理沙が%CALLNAME:MASTER%の一物を小さな手で擦りあげている…
		PRINTFORML 「ほらほら、気持ちいいか%CALLNAME:MASTER%？」
		PRINTFORMW 「我慢は体に悪いぞ？遠慮なくイっちゃっていいんだぜ？」
		RETURN 1
	ENDIF
ENDIF
RETURN 1

