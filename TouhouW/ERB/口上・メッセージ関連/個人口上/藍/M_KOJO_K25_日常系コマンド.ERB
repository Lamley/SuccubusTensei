@RAN_NORMALTALK
	;DAY:2で季節を選択。会話の数が増えてきたのでそれぞれへ適当に散らす形
	SELECTCASE DAY:2
	;春
	CASE 1
		SELECTCASE RAND:12
		CASE 0
			IF TALENT:恋人
				PRINTFORML 「真是個好男人呢、%CALLNAME:MASTER%」
				PRINTFORML 突然被表揚感到不知所措的%CALLNAME:MASTER%。藍看着静静的笑了
				PRINTFORMW 「不過、動作還不夠典雅。我会徹底磨練你的」
			ELSE
				PRINTFORML  「……………」
				PRINTFORML 在対談途中突然沈默、藍的目光盯着%CALLNAME:MASTER%
				PRINTFORML 然後、被用食指弾了額頭
				PRINTFORML 「給你的行為一個懲罰。不用向我道謝」
				PRINTFORMW 望着竊笑的藍%CALLNAME:MASTER%嘗試反撃回去、不過軽易的被逃脱了
			ENDIF
		CASE 1
			IF TCVAR:25:352 == 1
				PRINTFORML 「自古以来都有狐狸迷惑人類的傳説」
				PRINTFORML 「%CALLNAME:MASTER%難道不認為我在欺騙你嗎？」
				PRINTFORMW 「……這樣阿。問了個笨問題」
			ELSE
				PRINTFORML 「前陣子我教橙的小知識。就考考你」
				PRINTFORMW 「%CALLNAME:MASTER%這石頭到底是什麼呢？」
				$INPUT_LOOP_01
				PRINTL [0] 鑽石
				PRINTL [1] 碳結晶
				PRINTL [2] 葉子
				INPUT
				SELECTCASE RESULT
				CASE 0
					PRINTFORML 「可惜。答案只是一片葉子」
					PRINTFORML 「回答錯誤也沒関係。這本来就只是考験内心本質的問題」
					PRINTFORMW 「你直率的説出心中所想。是個善与傾聽的人」
				CASE 1
					PRINTFORML 「可惜。這是玻璃製的假物」
					PRINTFORML 「察覚到正確答案是不可能的。哈哈幹嘛突然從我後面搔癢」
					PRINTFORMW 「答錯就悩羞成怒阿」
				CASE 2
					PRINTFORML 「可惜。其実是真的」
					PRINTFORML 「雖説如此、正確与否。真真假假有什麼意義呢？」
					PRINTFORMW 「哎啊、鑽石被風吹走了」
				CASEELSE
				GOTO INPUT_LOOP_01
				ENDSELECT
				TCVAR:25:352 = 1
			ENDIF
		CASE 2
			IF TALENT:恋人
				PRINTFORML 「八雲家的仇家大概会瞄準你下手吧」
				PRINTFORML 「我還要変的更強。力量与智慧還遠遠不夠」
				PRINTFORMW 藍小声的嘟噥着自言自語
			ELSE
				PRINTFORML 「雖然是自殺行為、但有能力破坏大結界的人偶爾会出現」
				PRINTFORML 「在幻想鄉初生的年軽妖怪。一般都在想饞食着人類」
				PRINTFORML 「在和平時代成長的傢伙可不明白那價值觀。来踏入危険之中、追尋着試煉吧」
				PRINTFORML 「我可会好好傳授給你」
				PRINTFORMW 微微嗜虐的的表情。莫非和平太久讓她積存着圧力
			ENDIF
		CASE 3
			IF TALENT:恋人
				PRINTFORML 「唔、這是普莉茲姆利巴音楽会的傳単」
				PRINTFORML 「總是在春天精力充沛的活動着。找個時間去吧」
				PRINTFORMW 難得被藍邀請。%CALLNAME:MASTER%爽快点頭答応
			ELSE
				PRINTFORML 「愛麗斯的人偶劇真不錯呢」
				PRINTFORML 「操演的技術越来越好、劇本跟演出看的出来有下功夫」
				PRINTFORML 「在約会中觀看的話肯定很開心」
				PRINTFORMW %CALLNAME:MASTER%聽着微笑点点頭
			ENDIF
		CASE 4
			IF TALENT:恋人
				PRINTFORML 「今天都在訓練橙練習術式」
				PRINTFORMW 「当她成為独当一面的式神、約定好会親手縫製道士服奨勵她」
			ELSE
				PRINTFORML 「想送給橙跟我一樣的道士服」
				PRINTFORML 「雖然早就縫製好了、但該用什麼理由当奨勵呢」
				PRINTFORMW 「生日嗎、還是做個困難点的課題。在奨勵呢」
			ENDIF
		CASE 5
			PRINTFORML 「你有聽過、狐狸是怎麼叫的嗎」
			PRINTFORMW 「要我表演看看嗎 ……嘸嘸」
		CASE 6
			PRINTFORML 「呼、把要清洗的衣服都拿進来了……」
			PRINTFORML 「嗯？ 辛苦？。家事這種小事可累不倒我」
			PRINTFORMW 深深感覚到她真是高規格的万能式神
		CASE 7
			PRINTFORML 「善狐悪狐怎麼區分？」
			PRINTFORMW 「我不過是一個式神。只為聽從紫様的命令行事」
		CASE 8
			PRINTFORML 「在春天就要去白玉樓賞花」
			PRINTFORMW 「在這時期可以看到被一片櫻花覆蓋。美到連霊魂都被撫慰的程度」
		CASE 9
			PRINTFORML 「附近一帯的雪都完全融化了」
			PRINTFORML 「春天再度到来。從今以後也将継續多次迎接春天的到来吧」
			PRINTFORMW 藍温柔的瞇着双眼的
		CASE 10
			PRINTFORML 「春天是花妖跟蟲妖活躍的時候。在這季節這些笨蛋可控制不住妖力」
			PRINTFORML 「現在是適合繁殖的季節、無論是傳粉還是産卵」
			PRINTFORMW 如果被逆推的話……出神妄想着的%CALLNAME:MASTER%、被藍盯着
		CASE 11
			PRINTFORML 「在春天的這個季節使人変的懶洋洋的呢」
			PRINTFORMW 「不過身為最強式神、可不能如此堕落」
		ENDSELECT
	;夏
	CASE 2
		SELECTCASE RAND:12
		CASE 0
			IF TALENT:恋人
				PRINTFORML 「跟八雲家関係親密的你、你会被視為八雲家的一份子」
