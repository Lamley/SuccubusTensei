			CALL PANTS_GET(TARGET,"もらう")
		CASE 4
			PRINTFORML 「確実收到了……但是現在」
			PRINTFORML 「好吧許可了就沒辦法了」
			PRINTFORML 聽到答応的%CALLNAME:MASTER%将身体邁入九尾中
			PRINTFORML 就想在夢裡般、%CALLNAME:MASTER%的活力満満上升
			PRINTL 
			PRINTFORML 体力・気力＋150 愛情経験＋5
			BASE:MASTER:体力 = MIN(MAXBASE:MASTER:体力,BASE:MASTER:体力 + 150)
			BASE:MASTER:気力 = MIN(MAXBASE:MASTER:気力,BASE:MASTER:気力 + 150)
			EXP:TARGET:愛情経験 += 5
		CASEELSE
		GOTO INPUT_LOOP_04
		ENDSELECT
	ELSE
		PRINTFORML 「到底有什麼好処……」
		PRINTFORML 藍一臉感到可疑的樣子
	ENDIF
	PRINTW 
ENDIF
RETURN 1
