IF LOCAL
	;同時射精
	IF NOWEX:11 && NOWEX:MASTER:11
		PRINTFORMW
		RETURN 1
	;TARGETだけ射精
	ELSEIF NOWEX:11
		;PLAYERについてる
		IF TALENT:PLAYER & 2
			PRINTFORMW
			RETURN 1
		;付いてない
		ELSE
			PRINTFORMW
			RETURN 1
		ENDIF
	;PLAYERだけ射精
	ELSE
		;TARGETについてる
		IF TALENT:2 & 2
			PRINTFORMW
			RETURN 1
		;付いてない
		ELSE
			PRINTFORMW
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

