				PRINTFORMW 「ねえ、二人でデートなんてどうかしら%UNICODE(0x2665)%//」
			;恋慕縛りが解除されたときのために
			ELSE
				PRINTFORMW 「%CALLNAME:MASTER%、もし暇だったら一緒に%LOCALS%でも行かない？」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 1
		;受けた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人 && TALENT:恋慕
				PRINTFORMW 「やった%UNICODE(0x2665)% それじゃあ行きましょっ%UNICODE(0x2665)%%UNICODE(0x2665)%」
			ELSEIF TALENT:恋慕 || CFLAG:1014 == 1
				PRINTFORMW 「やった%UNICODE(0x2665)%//」
			ELSE
				PRINTFORMW 「それじゃあ早速行きましょう%UNICODE(0x2665)%」
			ENDIF
			;デートに誘われたフラグを立てる
			TCVAR:356 = 1
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 2
		;断った
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「それじゃあ別の日にデートしましょうね%UNICODE(0x2665)%」
			ELSE
				PRINTFORMW 「あやや、まあ%CALLNAME:MASTER%が乗り気じゃないならいいわ」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 3
		;部屋に誘われた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人 && TALENT:恋慕
				PRINTFORMW 「わたし的家でのんびりしましょう%UNICODE(0x2665)%」
			ELSEIF TALENT:恋慕 || CFLAG:1014 == 1 || TALENT:恋人
				PRINTFORMW 「よかったらわたし的家に寄ってかない？//」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 4
		;部屋に誘われたのを受けた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 5
		;部屋に誘われたのを断った
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1
			RETURN 1
		ENDIF
	ENDSELECT
ENDIF

