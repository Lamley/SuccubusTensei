@M_KOJO_EVENT_K44_20(ARG,ARG:1)
;口上主はLOCALS or RESULT
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
IF LOCAL && !CFLAG:ブチギレ
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;通常帰宅
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;採取終了後の帰宅
		IF TIME < 1260
			;恋人かつ恋慕時専用
			IF TALENT:恋人 && TALENT:恋慕
				;仕事手伝ったなら
				SIF TCVAR:364
				PRINTFORMW 「ありがとう%UNICODE(0x2665)% おかげでたくさん採れたわ%UNICODE(0x2665)%」
				PRINTFORMW 「わたしは家に戻るけど%CALLNAME:MASTER%も暇だったら来てね%UNICODE(0x2665)%」
			;恋慕or準恋慕or恋人時専用
			ELSEIF TALENT:恋慕 || CFLAG:1014 == 1 || TALENT:恋人
				IF TCVAR:364
					PRINTFORMW 「手伝ってくれてありがとう%UNICODE(0x2665)%// また後で会いましょう%UNICODE(0x2665)%//」
				ELSE
					PRINTFORMW （朝から%CALLNAME:MASTER%に会えるなんてしあわせー%UNICODE(0x2665)%//）
				ENDIF
			;思慕
			ELSEIF TALENT:思慕
				IF TCVAR:364
					PRINTFORMW 「手伝ってくれてありがとう%UNICODE(0x2665)%」
				ELSE
					PRINTFORMW 「採取したものを片付けてくるわ」
				ENDIF
			;特になし
			ELSE
				PRINTFORMW 「\@ TCVAR:364 ? 助かったわ%UNICODE(0x2665)% #  \@それじゃあね」
			ENDIF
		ELSE
			;恋人かつ恋慕時専用
			IF TALENT:恋人 && TALENT:恋慕
				PRINTFORMW 「もう遅いからわたしは帰るわね%UNICODE(0x2665)%　おやすみなさい%UNICODE(0x2665)%%UNICODE(0x2665)%」
			;恋慕or準恋慕or恋人時専用
			ELSEIF TALENT:恋慕 || CFLAG:1014 == 1 || TALENT:恋人
				PRINTFORMW 「もうこんな時間！　残念だけどもう帰らなきゃ」
				PRINTFORMW 「また今度ね%UNICODE(0x2665)%」
			;其他
			ELSE
				PRINT 「あらもうこんな時間！　そろそろ家に帰らないと、
				IF TALENT:思慕
					PRINTFORMW バイバイ%UNICODE(0x2665)%」
				ELSE
					PRINTW それじゃあね」
				ENDIF
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;デート中帰宅
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		;仕事帰りからのデート
		IF TCVAR:356 == 1
			IF TALENT:恋人 || TALENT:恋慕
				PRINTFORMW 「もうこんな時間なの…　残念だけど明日もあるしわたしは帰るわ」
				PRINTFORMW 「誘っておいてごめんね、この埋め合わせはするから」
			ELSE
				PRINTFORMW 「もうこんな時間！　まだ一緒にいたいけどもう帰らないと」
				PRINTFORMW 「わたしから誘っておいてごめんね」
			ENDIF
		;恋人かつ恋慕時専用
		ELSEIF TALENT:恋人 && TALENT:恋慕
			PRINTFORMW 「もうこんな時間なの…　残念だけどもう遅いからわたしは帰るわ、また明日ね%UNICODE(0x2665)%」
		;恋慕or準恋慕or恋人時専用
		ELSEIF TALENT:恋慕 || CFLAG:1014 == 1 || TALENT:恋人
			PRINTFORMW 「もうこんな時間！　まだデートしていたいけどもう帰らないと」
			PRINTFORMW 「せっかく誘って貰ったのにごめんね」
		;其他
		ELSE
			PRINTFORML 「もうこんな時間！　そろそろ家に帰らないと」
			IF TALENT:思慕
				PRINTFORMW 「バイバイ%UNICODE(0x2665)%」
			ELSE
				PRINTFORMW 「それじゃあ%UNICODE(0x2665)%」
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;玄関先でお見送り
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		;恋人かつ恋慕時専用
		IF TALENT:恋人 && TALENT:恋慕
			PRINTFORMW 「もう遅いからわたしは帰るわ%UNICODE(0x2665)%　またね%UNICODE(0x2665)%%UNICODE(0x2665)%」
		;恋慕or準恋慕or恋人時専用
		ELSEIF TALENT:恋慕 || CFLAG:1014 == 1 || TALENT:恋人
			PRINTFORMW 「もうこんな時間！　残念だけどもう帰らなきゃ」
			PRINTFORMW 「また今度ね%UNICODE(0x2665)%」
		;其他
		ELSE
			PRINT 「あらもうこんな時間！　そろそろ家に帰らないと、
			IF TALENT:思慕
				PRINTFORMW バイバイ%UNICODE(0x2665)%」
			ELSE
				PRINTW それじゃあね」
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

