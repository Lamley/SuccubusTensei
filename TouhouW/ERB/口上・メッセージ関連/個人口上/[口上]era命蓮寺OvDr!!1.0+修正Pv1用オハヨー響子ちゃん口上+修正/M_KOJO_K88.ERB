;===========================================================
; "EVENT"系イベント
;===========================================================
; 部屋で遭遇
;=================================================
@M_KOJO_EVENT_K88_1(ARG, ARG:1)
;口上主はTARGET
LOCAL = 1
IF LOCAL
	; 互換処理呼び出し
	SIF !TCVAR:351
		CALL K88_KYOKO_UPDATE
	; 書き分けが大変なのでどっちが入ってきた場合もまとめとく
	; 内部ではARGで分岐する可能性もある
	LOCAL:1 = 1
	IF LOCAL:1 && ARG
		; 仕事中(まだいっぱい)
		IF CFLAG:行動 == 5 && BASE:工作量 > MAXBASE:工作量 / 2
			PRINTFORML 響子はリズムよくお経を唱えながら掃除をしている
			PRINTFORMW 仕事が終わるまでには、もう少し時間がかかりそうに見える
			TCVAR:351++
		; 仕事中(もうすぐ終わる)
		ELSEIF CFLAG:行動 == 5 && BASE:工作量 > 0
			PRINTFORML 響子は鼻歌を歌いながら掃除をしている
			PRINTFORMW 仕事はあと少しで終わりそうだ
			TCVAR:351++
		; それ以外
		ELSE
			; まだ朝の挨拶してない
			IF TIME:2 == 1  && GETBIT(TCVAR:350, 0)
				IF TALENT:恋慕 && RAND:2
					PRINTFORML 「おはよーございます
					PRINTFORMW 　おはよーございます！」
					PRINTFORML 元気な挨拶と共に子犬がじゃれつくかのごとく響子が飛びかかってきた
					PRINTFORMW おはよう犬とでも名付けようか？心の中で
					SETBIT TCVAR:350, 0
				ELSE
					PRINTFORML 「おはよーございます！」
					PRINTFORML おお、おはようおはよう
					PRINTFORMW 今日も朝からハイテンションなようだ
					SETBIT TCVAR:350, 0
				ENDIF
			; まだ昼の挨拶してない
			ELSEIF TIME:2 == 2 && GETBIT(TCVAR:350, 1)
				IF TALENT:恋慕 && RAND:2
					PRINTFORMW 「ちわーっす！！」
					PRINTFORML ……その挨拶はどうかと思うと苦言を呈したら、
					PRINTFORMW 参拝に来ていた里の人がそういってたので真似をしたのだそうだ
					PRINTFORMW おのれ純真な子になんて挨拶を吹き込むのだ
					SETBIT TCVAR:350, 1
				ELSE
					PRINTFORML 「こんにちは！！」
					PRINTFORMW 今日もお寺に皆を笑顔にさせるような挨拶が響く
					SETBIT TCVAR:350, 1
				ENDIF
			; まだ夜の挨拶してない
			ELSEIF TIME:2 == 3 && GETBIT(TCVAR:350, 2)
				IF TALENT:恋慕 && RAND:2
					PRINTFORML 「こんばんは！」
					SIF GETBIT(TCVAR:350, 0) && GETBIT(TCVAR:350, 1)
						PRINTFORML 「今日はおはようからこんばんはまでコンプリートね！」
					PRINTFORMW ニコニコと笑う響子の姿についつい見惚れてしまった
					SETBIT TCVAR:350, 2
				ELSE
					PRINTFORML 「こんばんワおーん！」
					PRINTFORML 挨拶の途中に聞こえてきた遠吠えを律儀に返すのはいいが、
					PRINTFORML ヤマビコと挨拶は混ぜない方が良いと思うのだ
					PRINTFORMW もちろん山彦のアイデンティティを奪うわけにはいかないので口には出せない
					SETBIT TCVAR:350, 2
				ENDIF
			; 既に挨拶済み
			ELSE
				IF TCVAR:351 > 8
					PRINTFORML 「なんだか今日はよく会うわね」
					IF ARG == 2
						PRINTFORML 響子は首を傾げてるが、その原因は響子が動き回っているからじゃないだろうか？
					ELSE
						PRINTFORML (もしかして後をつけられてるのかしら？)
						PRINTFORML 何だか響子は\@ TALENT:恋慕 ? 嬉しそうな # 複雑な\@表情をしている
					ENDIF
				ELSEIF TCVAR:351 > 4
					; <工事中> 何か思いついたら後で書き直すよ
					PRINTFORML ＼Google!／
				ELSEIF TCVAR:351 > 2
					; <工事中> 何か思いついたら後で書き直すよ
					PRINTFORML ＼Excite!／
				ELSE
					; <工事中> 何か思いついたら後で書き直すよ
					PRINTFORML ＼YAHOO!／
				ENDIF
			ENDIF
			TCVAR:351++
		ENDIF
		RETURN 1
	ENDIF
	;其他
	PRINTFORML  
ENDIF
RETURN 1

