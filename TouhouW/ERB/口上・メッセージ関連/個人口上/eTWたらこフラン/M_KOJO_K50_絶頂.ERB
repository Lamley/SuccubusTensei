@M_KOJO_MESSAGE_PALAMCNG_A_K50_1(ARG)
CALL PALAM_MESSAGE_A
;-------------------------------------------------
;膣内
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	SIF !SHIRAHU(TARGET)
		RETURN 0
	IF 充填率(TARGET,1) >= 100
		;カリスマは株だからおNISAまは優遇されるの
		PRINTFORML 「おにぃさまぁ、%CALLNAME:TARGET%もうおなかいっぱいだよぉ」
		PRINTFORMW 「ご飯食べられなくなっちゃうよぉっ」
	ELSE
		PRINTFORMW 「んっ……\@ TALENT:恋慕 ? %UNICODE(0x2661)% #  \@」
		PRINTFORML 「お兄様のおちんちんがお腹の奥で、どくんどくんってしてる」
		IF GETBIT(CFLAG:1500,1)
			PRINTFORMW 「%CALLNAME:TARGET%のなかでしゃせーしてるのね」
		ENDIF
		IF ABL:Ｖ感覚 > 2
			PRINTFORMW 「うふふ%UNICODE(0x2661)%どくんどくんする度にお兄様のおちんちんが暴れて、%CALLNAME:TARGET%も気持ちいいよ」
		ELSE
			PRINTFORMW 「お兄様気持ちよさそう」
		ENDIF
		IF GETBIT (CFLAG:1500,3)
			PRINTFORMW 「お兄様嬉しい？%CALLNAME:TARGET%のお腹の中で\@ GETBIT (CFLAG:1500,1) ? 射精 # どくんどくん \@するの嬉しい？」
			SIF 充填率(TARGET,1) < 50
				PRINTFORMW 「コンティニュー、してもいいよ」
		ENDIF
	ENDIF
ENDIF
RETURN 1

