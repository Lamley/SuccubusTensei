;==================================================
;9,自慰
;==================================================
@M_KOJO_MESSAGE_COM_K50_9
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K50_9_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K50_9_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「おにゃにー？」
		PRINTFORMW 「おまたをこすればいいの？うん、やってみるね」
		RETURN 1
	ENDIF
	;IF TALENT:恋慕 && TALENT:処女 == 1
		;小さくても大きいと言わせそちん主人公をより情けなくしてみる試み
	;	PRINTFORMW 「ねぇおにいさまぁ。%CALLNAME:TARGET%のちっちゃなおまんこをお兄様のおっきなおちんちんで、ごしごししてください」
	;	RETURN 1
	;ENDIF
	;IF ABL:自慰中毒 >= 5
	;	PRINTFORMW 「おなにーっていけない事なの？たらこきちゃうの？」
	;	PRINTFORMW 「そんな事言われても、指止まんないよぉ」
	;	PRINTFORMW 「酷いよぉお兄様」
	;	PRINTFORMW 「酷いよぉ」
	;	PRINTFORMW 「ごめんなさいたらこさんごめんなさい」
	;	PRINTFORMDW それでも%CALLNAME:TARGET%は自慰をやめられないようだ
	;	RETURN 1
	;ELSEIF ABL:自慰中毒 >= 2
	IF ABL:自慰中毒 >= 2
		PRINTDATAL
			DATAFORM 「おまたごしごしきもちいいよぉ…」
			DATAFORM 「きもちよくって、あたまがおばかになっちゃうよぉ…」
			DATALIST
				DATAFORM 「はぁはぁ」
				DATAFORM 「おにゃにーきもちいぃ……おにゃにーきもちいいよぉ……おにぃさまぁ……」
			ENDLIST
		ENDDATA
	ELSEIF ABL:自慰中毒 >= 1
		IF BASE:MASTER:勃起 > 1000
			PRINTFORMW 「お兄様のおまた、すっごくなってる」
			PRINTFORMDW %CALLNAME:TARGET%は%CALLNAME:MASTER%の股間を食い入るように観察しながら涎をたらしながら指の動きを速めていった
			PRINTFORMDW %CALLNAME:MASTER%は%CALLNAME:TARGET%のお口の回りを拭いてあげた
		ELSE
			PRINTFORMW 「お兄様ぁ」
			PRINTFORMW 「%CALLNAME:TARGET%、お兄様にごしごしして欲しい」
			SIF CFLAG:TARGET:溜まってる度 / 10 > 750
				PRINTFORMW 「ねぇお兄様ぁ……駄目？」
		ENDIF
	ELSE
		IF !RAND:5
			PRINTFORML 「んしょんしょんっんっ」
			PRINTFORMW 「んしょんしょんっんっ」
			PRINTFORMW 「ねぇお兄様、これ気持ちいいね」
			PRINTFORMW 「お兄様もおまたごしごし、しないの？」
		ELSEIF !RAND:4
			IF !TALENT:処女 && BASE:MASTER:勃起 > 1000
				PRINTFORMW 「ねぇお兄様、しないの？」
				PRINTFORMW 「%CALLNAME:TARGET%のおまんこで、お兄様のおちんちんごしごし。ごしごししないの？」
				SIF CFLAG:TARGET:溜まってる度 / 10 > 500
					PRINTFORMW 「しないの？」
				SIF CFLAG:TARGET:溜まってる度 / 10 > 600
					PRINTFORMW 「しよ？」
				SIF CFLAG:TARGET:溜まってる度 / 10 > 750
					PRINTFORMW 「して」
			ELSE
				PRINTFORMW 「ねぇお兄様。お兄様は見てるだけで退屈じゃないの？」
				PRINTFORMW 「%CALLNAME:TARGET%、お兄様と遊びたいな」
			ENDIF
		ELSEIF !RAND:3
			;おまたごしごしじゃなくてオナニーだよウフフ
			PRINTFORMW 「ねえお兄様、ぱっちぇはおまたごしごしするの？」
			PRINTFORML 「うん」
			PRINTFORMW 「おにゃにー」
			PRINTFORMW 「ぱっちぇはおにゃにーってするの？」
		ELSEIF !RAND:2
			PRINTFORMW 「ねえお兄様、咲夜もおまたごしごし」
			PRINTFORML 「うん」
			PRINTFORMW 「おにゃにー」
			PRINTFORMW 「咲夜もおにゃにーってするのかしら」
		ELSE
			PRINTFORMW 「ねえお兄様、お姉様はこの事は知っているのかしら？」
			PRINTFORMW 「秘密なの？」
			PRINTFORML 「うん」
			PRINTFORMW 「ゆびきりげんまんね」
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1
