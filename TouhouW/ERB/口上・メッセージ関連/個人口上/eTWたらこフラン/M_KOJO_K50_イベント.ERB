		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 「おはよおにいさま」
			PRINTFORMW 「お兄様。夜はおはようだよ」
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;同棲中
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 「おはよおにいさま」
			PRINTFORMW 「お兄様。夜はおはようだよ」
			PRINTFORMW 
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
