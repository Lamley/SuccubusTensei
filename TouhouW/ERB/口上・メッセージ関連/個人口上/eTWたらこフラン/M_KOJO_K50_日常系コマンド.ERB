	;ENDIF
	;舞茄子になってもいいさと考えるんだ
	;IF !CFLAG:1502 && CFLAG:信頼度 >= 500 && ABL:教養 > 2 && MONEY >= 300000
	;IF !CFLAG:1502 && CFLAG:信頼度 >= 500 && ABL:教養 > 2
	;	TRYCALLFORM M_KOJO_MESSAGE_COM_K50_進学
	;	RETURN 1
	;ENDIF
	
	IF 家庭菜園可(CFLAG:MASTER:現在位置) && !GETBIT (TCVAR:350,1)
		SETBIT TCVAR:350,1
		;SELECTCASE PLANT_TYPE_0
		SELECTCASE PLANT_TYPE
			CASE 151
				PRINTFORMW 「あ、捲心菜畑。」
				PRINTFORML 「家庭菜園をこんなにしちゃうなんて%UNICODE(0x2661)%」
				PRINTFORMW 「お兄様、赤ちゃん欲しいの？」
				IF CFLAG:生解禁 <= 2 && TALENT:恋人
					IF CFLAG:16:信頼度 < 500
						PRINTFORML 「でもお姉様が駄目だって」
					ELSE
						PRINTFORML 
						PRINTFORML 「いいよ」
						PRINTFORMW 「だって%CALLNAME:TARGET%もお兄様の赤ちゃん欲しいもの」
						PRINTFORMW 「うふふっ！ほらお兄様！捲心菜畑で捕まえてっ！」
						CFLAG:生解禁 = 2
					ENDIF
				ELSE
					PRINTDATAL
						DATALIST
							DATAFORM 「%CALLNAME:TARGET%にはまだ早いと思うの」
							;ばっちゃに語呂を合わせぱっちぇ呼称
							DATAFORM 「お赤飯が来ないと無理なんだってぱっちぇが言ってたもん」
						ENDLIST
						DATALIST
							DATAFORM 「お兄様、おあずけ」
							DATAFORM 「むりやりはやぁよ？うふふ♪」
						ENDLIST
						DATALIST
							DATAFORM 「欲・し・い・の・よ・ね？お・兄・様？」
							DATAFORM 「%CALLNAME:TARGET%にはお見通しなんだから」
							DATAFORM 「うふふ。まだだぁめ%UNICODE(0x2661)%」
						ENDLIST
					ENDDATA
				ENDIF
			CASE 163
				PRINTFORML 「お姉様も昔は青かったのかしら」
			CASE 164
				PRINTFORML 「西瓜さん～西瓜さん～おぉきくなぁれー西瓜さん～」
				PRINTFORML 「西瓜さんみんなで食べようね！」
				PRINTFORMDW %CALLNAME:TARGET%はにぱっと笑った
			CASE 177
				PRINTFORML 「この葡萄が取れたら葡萄酒を作りましょ」
				PRINTFORML 「%CALLNAME:TARGET%ね。葡萄踏みしてみたかったの」
				PRINTFORML 「うふふ。楽しみね♪」
				PRINTFORMD %CALLNAME:TARGET%は裙子をたくし上げ
				
				IF TALENT:MASTER:受虐狂
					PRINTFORMDL ると、%CALLNAME:MASTER%の股間に視線を送りながら足踏みをする仕草をした
				ELSE
					PRINTFORMDL ながらはしゃいでいる
				ENDIF
			CASE 187
				PRINTFORML 「ゆり～♪ゆりゆり～♪」
				PRINTFORML 「ねえねえお兄様。百合の根っこは食べられるんだよ！」
				SIF TALENT:MASTER:性別 == 3
					PRINTFORMDL %CALLNAME:TARGET%の%CALLNAME:MASTER%の股間を見る目がおかしい…
			CASE 188
				IF TCVAR:媚薬
					PRINTFORML 「ねえねえお兄様」
					PRINTFORML 「にんじんにレモンをかけると媚薬になるんだって！でも媚薬ってなぁに？」
				ELSE
					PRINTFORML 「にんじんさん♪だいこんさん♪しいたけさん♪ごぼうさん♪あな～の空いたぁれんこんさん♪」
					PRINTFORML 「すじーの通ったふぅーっきっ♪」
				ENDIF
		ENDSELECT
	ENDIF
	;-------------------------------------------------
	;以下日常 とりあえずランダム三枠
	;-------------------------------------------------
	
	IF TFLAG:193 == 1
		PRINTFORMDW %CALLNAME:TARGET%は%CALLNAME:MASTER%の言葉にうんうんと大きく頷いている
		RETURN 1
	ELSEIF TFLAG:193 == -1
		PRINTFORMDW %CALLNAME:TARGET%は%CALLNAME:MASTER%の言葉に小さく首を傾げている
		RETURN 1
	ENDIF
	;恋慕 後期
	IF TALENT:恋慕
		IF !RAND:3
			;PRINTFORML 「」
		ELSEIF !RAND:2
			;PRINTFORML 「」
		ELSE
			;PRINTFORML 「」
		ENDIF
		RETURN 1
	;思慕 中期
	ELSEIF TALENT:思慕
