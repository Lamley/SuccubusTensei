			CASE 4
				PRINTFORMW 
				RETURN 0
			;Ｖ＆Ｂ絶頂
			CASE 5
				PRINTFORMW 
				RETURN 0
			;Ａ＆Ｂ絶頂
			CASE 6
				PRINTFORMW 
				RETURN 0
		ENDSELECT
	ENDIF
	IF NOWEX:Ｃ絶頂
		;強絶頂Ｃ
		IF NOWEX:Ｃ絶頂 == 2
			PRINTFORMW 
		;絶頂Ｃ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:Ｖ絶頂
		;強絶頂Ｖ
		IF NOWEX:Ｖ絶頂 == 2
			PRINTFORMW 
		;絶頂Ｖ
		ELSE
			PRINTFORMW 
