;==================================================
@M_KOJO_EVENT_K95_14(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;持っていた食物を食べられた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == -1
		IF TALENT:恋人
			PRINTFORM 「それ、頂戴、
			CALL MASTER_CALLNAME_K95
			PRINTFORMW ！」
			PRINTFORMW 「ん…ごめん…お腹空いてたの…」
			PRINTFORMW 八橋は%CALLNAME:MASTER%の持っていたものを食べてしまった。
		ELSEIF TALENT:恋慕
			PRINTFORMW 「それ…頂戴！」
			PRINTFORMW そう言うと八橋は%CALLNAME:MASTER%の持っているものを食べてしまった。
			PRINTFORMW 「…ごめん…お腹空いちゃってて…」
		ELSEIF TALENT:思慕
			PRINTFORMW 「…もぐもぐ…ごめん…」
			PRINTFORMW 八橋は%CALLNAME:MASTER%の持っていたものを食べてしまった！
		ELSE
			PRINTFORMW 「…んぐっ…」
			PRINTFORMW 八橋はいつの間にか%CALLNAME:MASTER%の持っていたものを食べてしまった！
		ENDIF
		RETURN 1
	ENDIF
	;其他
	PRINTFORML  
ENDIF
RETURN 1

