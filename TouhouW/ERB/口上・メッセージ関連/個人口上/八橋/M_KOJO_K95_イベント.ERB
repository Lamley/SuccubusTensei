;==================================================
;デート帰りに告白（恋人が付く）
;==================================================
@M_KOJO_SPEVENT_K95_2(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;告白内容
	;この告白内容表示後に受けるかどうかの選択分が表示されます
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		;初回
			IF FIRSTTIME(1001)
				PRINTFORMW 「ねえ%CALLNAME:MASTER%、」
				PRINTFORMW 「私ね、好きな人が出来たの」
				PRINTFORMW 「その人はね、優しくて、格好良くて、真面目で」
				PRINTFORMW 「落ち込んでる時もその人と話してると元気になれたの」
				PRINTFORMW 「こういう時どうしたらいいのって姉さんに聞いたんだけど」
				PRINTFORMW 「『それは自分で決めなさい』って言われちゃった」
				PRINTFORMW 「私ね、付喪神になってからも一人じゃ何も出来なかったの」
				PRINTFORMW 「姉さんや雷鼓さんにおんぶにだっこ」
				PRINTFORMW 「でもね、いつまでもそういう訳にもいかないし」
				PRINTFORMW 「二人よりも大切な人が出来たんだ」
				PRINTFORMW 「%CALLNAME:MASTER%、」
				PRINTFORMW 「私は%CALLNAME:MASTER%のことが好き」
				PRINTFORMW 「世界中の誰よりも、%CALLNAME:MASTER%のことが大好き！」
;				PRINTFORMW 「わ、私は…その…姉さんや雷鼓さんや他のみんなみたいに華は無いけど…」
				PRINTFORMW 「私、主人公の一番になりたい！」
				PRINTFORMW 「私と恋人になって下さい！」
			;二回目の告白
			ELSE
				PRINTFORMW 「あのね、%CALLNAME:MASTER%」
				PRINTFORMW 帰路に着くと八橋は意を決したように口を開いた。
				PRINTFORMW 「ほんの少しだけでもいいから私を見て」
				PRINTFORMW 八橋はそう言って%CALLNAME:MASTER%を振り向かせた。
				PRINTFORMW 「%CALLNAME:MASTER%と会えば会うほど好きになって」
				PRINTFORMW 「%CALLNAME:MASTER%と会わないと寂しくて」
				PRINTFORMW 「こんなのどうしていいかなんて…分からなくて」
				PRINTFORMW 「どうしても…恋人になってもらえませんか…？」
			ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白オッケー（恋人が付く）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORMW 「…ふぇ…」
		PRINTFORMW 八橋は顔を真っ赤にしながら嗚咽を漏らした。
		PRINTFORMW 「んっ…ありがとう…」
		PRINTFORMW そう言うと八橋は抱きついてきた。
		PRINTFORMW 「%CALLNAME:MASTER%…大好き…」
		PRINTFORMW %CALLNAME:MASTER%は八橋の頭を撫でながらしばらくそのままでいることにした。
		PRINTFORMW （もう少し…このまま…）
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白失敗
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW  「む…ぅ…」
		PRINTFORMW  「わ、わかったわ」
		PRINTFORMW  「じゃあね、%CALLNAME:MASTER%」
		PRINTFORMW  八橋はそう言って去っていった。
		PRINTFORMW  （もし、もしもだけど…）
		PRINTFORMW  （誰よりも早く…%CALLNAME:MASTER%に…会ってたら…）
		PRINTFORMW  （いっそ…）
		RETURN 1
	ENDIF

ENDIF
RETURN 1

;==================================================
;デート帰り
;==================================================
@M_KOJO_SPEVENT_K95_3(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;通常帰宅
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF CFLAG:318 == 1
			PRINTFORMW 「…ばいばい」
		ELSEIF TALENT:恋人
			PRINTFORM 「…今日は楽しかったわ、ありがとうね、
			CALL MASTER_CALLNAME_K95
			PRINTFORMW ！」
			PRINTFORMW 八橋はそう言いながら%CALLNAME:MASTER%の方へ向き直って礼を言った。
			PRINTFORM 「やっぱり
			CALL MASTER_CALLNAME_K95
			PRINTFORMW と一緒に過ごすのっていいわね」
			PRINTFORMW 「明日もまた、デートしましょ！…約束よ」
		ELSEIF TALENT:恋慕
			PRINTFORMW 「ふふ、今日は楽しかった」
			PRINTFORM 「
			CALL MASTER_CALLNAME_K95
			PRINTFORMW 、貴方はどうだった？」
			PRINTFORMW 「…もし、%CALLNAME:MASTER%さえよければ明日も―」
			PRINTFORMW 「…やっぱりなんでもないわ、じゃあね！」
			PRINTFORMW 八橋はそう言って名残惜しげに手を振って去っていった。
		ELSEIF TALENT:思慕
			PRINTFORMW 「じゃあね、また明日！」
			PRINTFORMW 八橋はそう言ってそそくさと帰っていった。
			PRINTFORMW （…また明日…会えたら…）
			PRINTW
		ELSE
			PRINTFORMW 「ん…もう帰るね」
			PRINTFORM 「
			CALL MASTER_CALLNAME_K95
			PRINTFORMW も早く寝たほうがいいわよ、夜更かしは体に悪いからね」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印消去イベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:恋人
			PRINTFORMW 「…わかった、許したげる」
			PRINTFORMW 「でもね…あんまり悪いことしないでよ？」
			PRINTFORM 「
			CALL MASTER_CALLNAME_K95
			PRINTFORMW のこと、信じてるからね？」
		ELSE
			PRINTFORMW 「ん、いいけどさ」
			PRINTFORMW 「あんまり人怒らせること、しないほうがいいよ」
			PRINTFORMW 「私は優しいから許してあげるけど」
			PRINTFORMW 八橋はどこかぎこちない笑顔でそう言った。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印2>1レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:恋人
			PRINTFORMW 「…ふん！」
			PRINTFORMW 「まだ怒ってるからね！」
			PRINTFORM 「
			CALL MASTER_CALLNAME_K95
			PRINTFORMW のことそう簡単に信用してあげないんだから！」
		ELSE
			PRINTFORMW 「…まだ嫌い」
			PRINTFORM 「だけど
			CALL MASTER_CALLNAME_K95
			PRINTFORMW だからちょっと許したげる…」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印3>2レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF TALENT:恋人
			PRINTFORMW 「…まだ嫌い」
			PRINTFORM 「だけど
			CALL MASTER_CALLNAME_K95
			PRINTFORMW だからちょっと許したげる…」
			PRINTFORMW 「…別に！」
			PRINTFORMW 「いつまでも怒ってるってわけじゃないわ…」
		ELSE
			PRINTFORMW 「…いいわよ」
			PRINTFORMW 「少しだけ…許したげる！」
		ENDIF
		RETURN 1
	ENDIF
	PRINTFORML
ENDIF
RETURN 1

