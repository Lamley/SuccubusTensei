;==================================================
;時止め終了時
;==================================================
@M_KOJO_EVENT_K95_17(ARG,ARG:1)
;口上主はTARGET
;排他と明記されていないモノは状況によっては連続で表示されるので注意
;ARGに射精状況
;ARG:1に射精状態
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;ARG=1～3は排他
	;謎の快感
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:恋慕
			PRINTFORMW 「～～～！？」
			PRINTFORMW 八橋は唐突な快感に身悶えしている。
			PRINTFORMW 「なんでもない…！何でもないから…！」
			PRINTFORMW 八橋は快楽に耐えながら必死に何でもない風を装っている。
			PRINTFORMW （こんなの…%CALLNAME:MASTER%に見られたくない…！）
		ELSE
			PRINTFORMW 「～～～！？」
			PRINTFORMW 八橋はビクッと体を震わせ
			PRINTFORMW 「何でも…ないわ…」
			PRINTFORMW （…やだ…何よ…もう…）
			PRINTFORMW 八橋はあまり経験のない快楽に涙目になっている。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;絶頂
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:恋慕
			PRINTFORMW 「！？！？！？」
			PRINTFORMW 八橋は一瞬でへたり込んでしまい何があったのか理解できないようだ。
			PRINTFORMW 「…ふぇ…やだぁ…」
			PRINTFORMW 彼女は差し出された手を取りながらもう片方の手で涙を拭っている。
		ELSE
			PRINTFORMW 「んあっ%UNICODE(0x2665) *1%」
			PRINTFORMW 「…や…ぁ…」
			PRINTFORMW 突然快楽を叩き込まれた八橋はうずくまってしまった。
			PRINTFORMW 「何…？…何！？」
			PRINTFORMW 八橋は困惑しながら体に手を回して泣いている。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;パンツ紛失
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 「…んん！？」
		PRINTFORMW 八橋は急に裙子を両手で押さえつけた！
		PRINTFORMW どうも内袴を盗ったため違和感を覚えたようだ。
		PRINTFORMW 「…や、やだ…！？何よこれ…！」
		PRINTFORMW 八橋は内袴が無いことに困惑している。
		PRINTFORMW 少なくとも彼女にとってノーパンは初めてだったようだ。
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;【無自覚口内射精】
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;精液中毒低精飲
		IF ARG:1 == 1
			IF EXP:精液経験 == 0
				PRINTFORMW 「んえぇ！？」
				PRINTFORMW 八橋は口の中に流し込まれた子種に驚いている。
				PRINTFORMW 「うぇっ…何…何…！？」
				PRINTFORMW 「何なのよ…もう…」
				PRINTFORMW 八橋は至極不快そうな顔をしながらゆっくりとどうにか精液を飲み下した。
			ELSE
				PRINTFORMW 「…ん！？」
				PRINTFORMW 八橋は突然口を押さえ体を震わせながら膝から崩れ落ちた。
				PRINTFORMW 「うぇ…んん…」
				PRINTFORMW 口内に残る子種をどうにかして処理しようか寸瞬迷った後
				PRINTFORMW 「ん…んっく…」
				PRINTFORMW 飲み込むことにしたようだ。
				PRINTFORMW 「何よ…もう…」
			ENDIF
		;精液中毒低吐き出し
		ELSEIF ARG:1 == 2
			PRINTFORMW 「ん！？」
			PRINTFORMW 「げほっおぇ…」
			PRINTFORMW 八橋は突然口内に現れたザーメンの臭いと苦味に顔をしかめ咳き込んだ。
			PRINTFORMW 「何なのよもう…」
		;精液中毒低手で覆う
		ELSEIF ARG:1 == 3
			PRINTFORMW 「！？」
			PRINTFORMW 「ん…ぐぇ…うっ…」
			PRINTFORMW 八橋は涙目になりながら口を押さえている…
		;精液中毒高精飲
		ELSEIF ARG:1 == 4
			PRINTFORMW 「…ん！？」
			PRINTFORMW 八橋は突然口の中に流し込まれたザーメンを困惑しながら飲み下した。
			PRINTFORMW 「…んんっ…なんなの…」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;【無自覚顔面射精】
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		;幼稚持ち＆精液経験100未満
		IF ARG:1 == 1
		;通常ぬぐい取り
		ELSEIF ARG:1 == 2
			PRINTFORMW 「…んん！？」
			PRINTFORMW 八橋は顔面にへばりついた精液に気がつくと
			PRINTFORMW 「何よ…これ…」
			PRINTFORMW 取り乱しながら拭い取った。
		;顔射精液舐めとり
		ELSEIF ARG:1 == 3
			PRINTFORMW 「…んぇ！？」
			PRINTFORMW 八橋は顔面にへばりついた精液に気がつくと
			PRINTFORMW 「…ん…れろ…美味し…%UNICODE(0x2665) *1%」
			PRINTFORMW 美味しそうに舐めとった。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;【無自覚手淫射精】
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		;ぬぐい取り
		IF ARG:1 == 1
			PRINTFORMW 「…！？」
			PRINTFORMW 八橋は手にへばりついた精液に気がつくと
			PRINTFORMW 「…な…何よこれ！？」
			PRINTFORMW 取り乱しながら拭い取った。
		;舐めとり
		ELSEIF ARG:1 == 2
			PRINTFORMW 「…うぇ！？」
			PRINTFORMW 八橋は手にへばりついた精液に気がつくと
			PRINTFORMW 「ん…れろ…ちゅ…%UNICODE(0x2665) *1%」
			PRINTFORMW 妖艶に舐めとった。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;【無自覚アナル射精】
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		;Ａ感覚0
		IF ARG:1 == 1
			PRINTFORMW 「ひぎぃ！何！？何！？」
			PRINTFORMW 八橋はアナルに違和感を覚えたが、その正体まではわからないようだ。
		;ソレ以外
		ELSEIF ARG:1 == 2
			PRINTFORMW 「…んひ！？」
			PRINTFORMW 八橋はアナルの違和感に気づくと
			PRINTFORMW 「…何！？何なのよ…もう…」
			PRINTFORMW お尻を押さえながら辺りを睨みつけている。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;【無自覚処女喪失】
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 8
		PRINTFORMW 「…うっ！？」
		PRINTFORMW 八橋は下腹部の鈍痛に顔をしかめると
		PRINTFORMW 「………？」
		PRINTFORMW 唇を噛み締めながら悪寒に冷や汗をかいている。
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;【無自覚膣内射精】
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 9
		;非処女膣内射精口服避孕薬なし
		IF ARG:1 == 1
			PRINTFORMW 「…ん！？」
			PRINTFORMW 八橋は膣内の違和感に気づくと
			PRINTFORMW 「何よ…やだ…もう…」
			PRINTFORMW 取り乱しながら下腹部を押さえている。
		;非処女膣内射精口服避孕薬あり
		ELSEIF ARG:1 == 2
			PRINTFORMW 「んあっ！？」
			PRINTFORMW 八橋は下半身の違和感に気づくと
			PRINTFORMW 「…これ…何…大丈夫だよね…」
			PRINTFORMW 何やら薬を飲んでいるようだ。
		;処女膣内射精
		ELSEIF ARG:1 == 3
			PRINTFORMW 「…っ！？」
			PRINTFORMW 八橋は下半身の鈍痛と違和感に気づくと
			PRINTFORMW 「やだやだ…何よこれ…知らない…！」
			PRINTFORMW 下半身から漏れ出る精液に取り乱しながら半泣きになっている。
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1
;==================================================
;ナマでやらせろ時
;==================================================
@M_KOJO_EVENT_K95_18(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;避妊要求台詞
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:恋人
			PRINTFORMW 「生？」
			PRINTFORMW 「えーと…兒童作りたいってことかな…？」
			PRINTFORMW 八橋は生エッチの意味をあまり良く分かっていないようだ。
			PRINTFORMW 「まだ…そういうのは早いかなって…」
		ELSEIF TALENT:恋慕
			PRINTFORMW 「…えっと…」
			PRINTFORMW 「駄目…やっぱそういうのはダメ！」
			PRINTFORM 「
			CALL MASTER_CALLNAME_K95
			PRINTFORMW がキライとかじゃなくて…そういうのはその…早いよ…」
		ELSE
			PRINTFORMW 「…生えっち？」
			PRINTFORMW 「…えーと…そういうのは…」
			PRINTFORMW 八橋は少し考えた後、
			PRINTFORMW 「駄目…かな…」
			PRINTFORMW 目を逸らしながら断った。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;断った
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:恋人
			PRINTFORMW 「駄目だって…」
			PRINTFORMW 「しつこいな…もう…！」
			PRINTFORMW 八橋はなおも拒否してきた。
		ELSEIF TALENT:恋慕
			PRINTFORMW 「…もう…！」
			PRINTFORMW 「駄目だって言ってるの…！」
			PRINTFORMW 八橋は半泣きになりながら拒否してきた。
		ELSE
			PRINTFORMW 「…いやダメだって」
			PRINTFORMW 八橋はキッパリと断った。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;断ったのに粘られて切れた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 「止めて！」
		PRINTFORMW 八橋は耳を塞いで%CALLNAME:MASTER%を拒絶した！
		PRINTFORM 「やだ…止めてよ…
		CALL MASTER_CALLNAME_K95
		PRINTFORMW のこと…嫌いになりたくないのに…！」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;危険日以外許可
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		IF TALENT:恋人
			PRINTFORMW 「わかったわ！」
			PRINTFORMW 「安全日はいいわ！」
			PRINTFORMW 八橋は妥協した。
			PRINTFORMW 「でもね…危険日はダメだよ？」
		ELSEIF TALENT:恋慕
			PRINTFORMW 「…いいよ」
			PRINTFORMW 「でもね、ダメって言ったときはダメだよ？」
		ELSE
			PRINTFORMW 「…うん」
			PRINTFORMW 「…分かった、いいよ」
			PRINTFORMW 八橋は少し考えた後妥協することにした。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;危険日許可
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		IF TALENT:恋人
			PRINTFORMW 「うん、分かった」
			PRINTFORMW 「兒童、作ろっか」
			PRINTFORMW 八橋は覚悟を決めたように表情を強張らせながらも笑顔を作ってみせた。
		ELSEIF TALENT:恋慕
			PRINTFORMW 「…分かった」
			PRINTFORMW 「ちゃんと…責任取ってね…？」
		ELSE
			PRINTFORMW 「………うん」
			PRINTFORMW 「分かった、生でも、いいよ」
			PRINTFORMW 八橋は少し表情を緩めながらそう言った。
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1
