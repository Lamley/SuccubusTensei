;概　要:言葉でなく心で理解しろ
;引　数:ARG…キャラ登録番号
;戻り値:夢
;備　考:文中関数
;使用例:%OPPAI_DESCRIPTION(TARGET)%
;例文:胸
;ARG:1を1にすると状態に応じた形容詞がつく
;使用例:%OPPAI_DESCRIPTION(TARGET,1)%
;例文:はちきれんばかりの乳塊
;条件文に TEQUIP:50 == PLAYER || TEQUIP:51 == PLAYER
;を指定しているのは合体中の描写
;-------------------------------------------------
@OPPAI_DESCRIPTION(ARG,ARG:1)
#FUNCTIONS
VARSET LOCALS
IF ARG:1
	IF FLAG:Look表示用乳房
		;爆乳
		IF TALENT:ARG:胸囲 == 2
			LOCALS += "快要装不下的"
		;巨乳
		ELSEIF TALENT:ARG:胸囲 == 1
			LOCALS += "豊満的"
		;並乳
		ELSEIF TALENT:ARG:胸囲 == 0
			LOCALS += "外形良好的"
		;貧乳
		ELSEIF TALENT:ARG:胸囲 == -1
			LOCALS += "小巧的"
		;絶壁
		ELSEIF TALENT:ARG:胸囲 == -2
			LOCALS += "可愛的"
		ELSE
			LOCALS += "错误報告、出现了预想之外的数値――以上"
		ENDIF
	ELSE
		;爆乳
		IF TALENT:ARG:胸囲 == 2
			IF TEQUIP:50 == PLAYER || TEQUIP:51 == PLAYER
				SELECTCASE RAND:4
					CASE 0
						LOCALS += "縦横乱晃的"
					CASE 1
						LOCALS += "像要甩出去一樣搖晃着的"
					CASE 2
						LOCALS += "噗尼噗尼的跳動着的"
					CASE 3
						LOCALS += "累累垂垂的摇晃着的"
				ENDSELECT
			ENDIF
			IF TALENT:PLAYER:胸囲 == -2 && RAND:5 == 0
				LOCALS += "令人嫉妬的巨大成熟的"
			ELSEIF RAND:4 == 0
				LOCALS += "乳牛一般的"
			ELSEIF RAND:3 == 0
				LOCALS += "快要装不下的"
			ELSE
				LOCALS += "无法掌控的"
			ENDIF
		;巨乳
		ELSEIF TALENT:ARG:胸囲 == 1
			IF RAND:3 == 0
				IF TEQUIP:50 == PLAYER || TEQUIP:51 == PLAYER
					LOCALS += "沈甸甸的大到手掌放不下的"
				ELSE
					LOCALS += "大到手掌放不下的"
				ENDIF
			ELSEIF RAND:2 == 0
				IF TEQUIP:50 == PLAYER || TEQUIP:51 == PLAYER
					LOCALS += "累累垂垂的像摇晃的果実那样的"
				ELSE
					LOCALS += "像成熟的果実那样的"
				ENDIF
			ELSE
				IF TEQUIP:50 == PLAYER || TEQUIP:51 == PLAYER
					LOCALS += "晃動不止大到手掌放不下的"
				ELSE
					LOCALS += "会從手中溢出的"
				ENDIF
			ENDIF
		;並乳
		ELSEIF TALENT:ARG:胸囲 == 0
			IF TEQUIP:ARG:50 == PLAYER || TEQUIP:ARG:51 == PLAYER
				IF !RAND:4
					LOCALS += "像果凍一般顫動的"
				ELSEIF !RAND:3
					LOCALS += "小幅度晃動的"
				ELSE
					LOCALS += "軟綿綿顫動的"
				ENDIF
			ENDIF
			IF TALENT:ARG:体型 < 0
				IF !RAND:2
					LOCALS += "超出体格発育良好的"
				ELSE
					LOCALS += "和小小的身体完全不相称的"
				ENDIF
			ELSEIF RAND:4 == 0
					LOCALS += "新鮮発育的"
			ELSEIF RAND:3 == 0
					LOCALS += "一手可以掌握的"
			ELSEIF RAND:2 == 0
					LOCALS += "左右匀称的"
			ELSE
				LOCALS += "外形良好的"
			ENDIF
		;絶壁
		ELSEIF TALENT:ARG:胸囲 == -2
			IF RAND:4 == 0
				LOCALS += "隠隠約約有一点点的"
			ELSEIF RAND:2 == 0
				LOCALS += "大致上没有凸起的"
			ELSE
				LOCALS += "只有一点点凸起感覚的"
			ENDIF
		;貧乳
		ELSEIF TALENT:ARG:胸囲 == -1
			IF TALENT:ARG:体型 < 0
				LOCALS += "和外表相応的"
			ELSEIF RAND:2 == 0
				LOCALS += "圓潤的"
			ELSE
				LOCALS += "可愛的"
			ENDIF
		;想定外
		ELSE
			LOCALS += "错误報告、出现了预想之外的数値――以上"
		ENDIF
	ENDIF
