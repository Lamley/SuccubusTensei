	;乳
	ELSEIF TALENT:ARG:胸囲 == 0 && RAND:5 == 0
		LOCALS += "美乳"
	ELSEIF TALENT:ARG:胸囲 == 0 && RAND:4 == 0
		LOCALS += "胸部"
	ELSEIF TALENT:ARG:胸囲 >= 0 && RAND:3 == 0
		LOCALS += "胸乳"
	ELSEIF TALENT:ARG:胸囲 >= 0 && RAND:2 == 0
		LOCALS += "乳房"
	ELSE
		;念のため男の乳書いとく
		IF TALENT:ARG:胸囲 == -2 && TALENT:ARG:性別 == 2
			IF ABL:ARG:戦闘能力 >= 6
				LOCALS += "大胸筋峡谷"
			ELSEIF ABL:ARG:戦闘能力 >= 4
				LOCALS += "厚実胸膛"
			ELSEIF ABL:ARG:戦闘能力 <= 1
				LOCALS += "単薄胸板"
