		LOCALS += "淫爆乳"
	ELSEIF TALENT:ARG:胸囲 == 2 && TALENT:ARG:体型 == 1 && RAND:23 == 0
		LOCALS += "超乳"
	ELSEIF TALENT:ARG:胸囲 == 2 && RAND:22 == 0
		LOCALS += "乳塊"
	ELSEIF TALENT:ARG:胸囲 == 2 && RAND:20 == 0
		LOCALS += "爆乳"
	ELSEIF TALENT:ARG:胸囲 == 2 && RAND:19 == 0
		LOCALS += "乳肉"
	;巨乳
	ELSEIF TALENT:ARG:胸囲 == 1 && RAND:18 == 0
		LOCALS += "豊乳"
	ELSEIF TALENT:ARG:胸囲 == 1 && RAND:17 == 0
		LOCALS += "巨乳"
	ELSEIF TALENT:ARG:胸囲 == 1 && RAND:16 == 0
		LOCALS += "豊満的胸"
	ELSEIF TALENT:ARG:胸囲 == 1 && RAND:15 == 0
		LOCALS += "柔肉"
	ELSEIF TALENT:ARG:胸囲 >= 1 && RAND:14 == 0
		LOCALS += "柔乳"
	;絶壁
	ELSEIF TALENT:ARG:胸囲 == -2 && TALENT:ARG:性別 != 2 && RAND:13 == 0
		LOCALS += "絶壁"
	ELSEIF TALENT:ARG:胸囲 == -2 && TALENT:ARG:性別 != 2 && RAND:12 == 0
		LOCALS += "洗衣板"
	ELSEIF TALENT:ARG:胸囲 == -2 && TALENT:ARG:性別 != 2 && RAND:11 == 0
		LOCALS += "平坦胸部"
	;貧乳
	ELSEIF TALENT:ARG:胸囲 == -1 && RAND:10 == 0
		LOCALS += "貧乳"
	ELSEIF TALENT:ARG:胸囲 == -1 && RAND:9 == 0
		LOCALS += "贫胸"
	ELSEIF TALENT:ARG:胸囲 == -1 && RAND:8 == 0
		LOCALS += "小巧胸部"
	ELSEIF TALENT:ARG:胸囲 == -1 && RAND:7 == 0
		LOCALS += "単薄的胸"
	ELSEIF TALENT:ARG:胸囲 == -1 && RAND:6 == 0
