IF ABL:ARG:指 < 6
	LOCAL = EXP:ARG:手淫経験
	LOCAL:1 = 10 + ABL:ARG:技巧 / 2 + TALENT:ARG:霊活手指 * 2 + TALENT:ARG:習得速度
	IF LOCAL * LOCAL:1 / 10 > EXPLV:(ABL:ARG:指 + 3) / 2; && EXP_UP(24,ARG)
		ABL:ARG:指 ++
		PRINTFORMW %CALLNAME:ARG%的手指変得霊活了
		IF ABL:ARG:指 >= 5
			PRINTFORMW %CALLNAME:ARG%获得了[霊活手指]
			TALENT:ARG:霊活手指 = 1
		ENDIF
	ENDIF
ENDIF
IF ABL:ARG:舌 < 6
	LOCAL = EXP:ARG:口淫経験
	LOCAL:1 = 10 + ABL:ARG:技巧 / 2 + TALENT:ARG:舌頭霊活 * 2 + TALENT:ARG:習得速度
	IF  LOCAL * LOCAL:1 / 10 > EXPLV:(ABL:ARG:舌 + 3); && EXP_UP(25,ARG)
		ABL:ARG:舌 ++
		PRINTFORMW %CALLNAME:ARG%的舌头変得霊活了
		IF ABL:ARG:舌 >= 5
			PRINTFORMW %CALLNAME:ARG%获得了[舌頭霊活]
			TALENT:ARG:舌頭霊活 = 1
		ENDIF
	ENDIF
ENDIF
IF ABL:ARG:胸 < 6
	LOCAL = EXP:ARG:乳交経験 * 2 +  EXP:ARG:Ｂ経験 / 5 + EXP:ARG:噴乳経験  / 4
	LOCAL:1 = 10 + ABL:ARG:技巧 / 2 + TALENT:ARG:胸囲 + TALENT:ARG:習得速度
	SIF FLAG:抱負 == 5
		LOCAL:1 ++
	SIF GETBIT (CFLAG:ARG:引き継ぎ, 5)
		LOCAL:1 *= 3
	IF LOCAL * LOCAL:1 / 10 > EXPLV:(ABL:ARG:胸 + 3) && EXP:ARG:乳交経験 + EXP:ARG:噴乳経験  / 5 > EXPLV:(ABL:ARG:胸 + 1); && EXP_UP(26,ARG)
		ABL:ARG:胸 ++
		PRINTFORMW %CALLNAME:ARG%在使用胸部的性技上的技術提高了
		IF ABL:ARG:胸 >= 5 && TALENT:ARG:2 != 2
			PRINTFORMW %CALLNAME:ARG%获得了[淫乳]
			TALENT:ARG:淫乳 = 1
		ENDIF
	ENDIF
ENDIF
IF ABL:ARG:腰 < 6
	LOCAL = EXP:ARG:Ｖ性交経験 + EXP:ARG:Ａ性交経験 + EXP:ARG:挿入経験
	LOCAL:1 = 10 + ABL:ARG:技巧 / 2 + TALENT:ARG:習得速度
	IF LOCAL * LOCAL:1 / 10 > EXPLV:(ABL:ARG:腰 + 3)
		ABL:ARG:腰 ++
		PRINTFORMW %CALLNAME:ARG%的腰技変得更高明了
	ENDIF
ENDIF
IF ABL:ARG:膣 < 6
	;広げすぎるとガバガバになっちゃいますよ
	LOCAL = EXP:ARG:Ｖ性交経験  * 2 +  EXP:ARG:Ｖ経験 / 5 + (EXP:ARG:Ｖ拡張経験 <= 10 ? EXP:ARG:Ｖ拡張経験 * 5 # 100 - EXP:ARG:Ｖ拡張経験 * 5)
	LOCAL:1 = 10 + ABL:ARG:技巧 / 2 + TALENT:ARG:習得速度
	SIF FLAG:抱負 == 3
		LOCAL:1 ++
	SIF GETBIT (CFLAG:ARG:引き継ぎ, 3)
		LOCAL:1 *= 3
	IF LOCAL * LOCAL:1 / 10 > EXPLV:(ABL:ARG:膣 + 3) && EXP:ARG:Ｖ性交経験 >  EXPLV:(ABL:ARG:膣 + 1); && EXP_UP(20,ARG)
		ABL:ARG:膣 ++
		PRINTFORMW %CALLNAME:ARG%的性交技術提高了
		IF ABL:ARG:膣 >= 5 && TALENT:ARG:2 != 2
			PRINTFORMW %CALLNAME:ARG%获得了[淫壺]
			TALENT:ARG:淫壺 = 1
		ENDIF
	ENDIF
ENDIF
IF ABL:ARG:肛 < 6
	LOCAL = EXP:ARG:Ａ性交経験  * 2 +  EXP:ARG:Ａ経験 / 5 + (EXP:ARG:Ａ拡張経験 <= 25 ? EXP:ARG:Ａ拡張経験 * 2 # 100 - 2 * EXP:ARG:Ａ拡張経験)
	LOCAL:1 = 10 + ABL:ARG:技巧 / 2 + TALENT:ARG:習得速度
	SIF FLAG:抱負 == 4
		LOCAL:1 ++
	SIF GETBIT (CFLAG:ARG:引き継ぎ, 4)
		LOCAL:1 *= 3
	IF LOCAL * LOCAL:1 / 10 > EXPLV:(ABL:ARG:肛 + 3) && EXP:ARG:Ａ性交経験 > EXPLV:(ABL:ARG:肛 + 1); && EXP_UP(21,ARG)
		ABL:ARG:肛 ++
		PRINTFORMW %CALLNAME:ARG%用肛门性交的技術提高了
		IF ABL:ARG:肛 >= 5
			PRINTFORMW %CALLNAME:ARG%获得了[尻穴狂]
			TALENT:ARG:尻穴狂 = 1
		ENDIF
	ENDIF
ENDIF
