	IF (EXP:ARG:歌唱経験 > EXPLV:(ABL:ARG:音楽技能 + 3) * 2 || (EXP:ARG:歌唱経験 > 20 && ABL:ARG:音楽技能 == 0)) || (EXP:ARG:演奏経験 > EXPLV:(ABL:ARG:音楽技能 + 3) * 2 || (EXP:ARG:演奏経験 > 20 && ABL:ARG:音楽技能 == 0))
