	IF EXP:ARG:釣魚経験 > EXPLV:(ABL:ARG:釣魚技能 + 3) || (EXP:ARG:釣魚経験 > 5 && ABL:ARG:釣魚技能 == 0)
		ABL:ARG:釣魚技能 ++
		PRINTFORMW %CALLNAME:ARG%の釣魚技能がLV{ABL:ARG:釣魚技能}成為了
	ENDIF
