@EVENT_COUNTER_PUNISHMENT(ARG,ARG:1)
VARSET LOCAL
LOCAL = 1
WHILE LOCAL
LOCAL:1 = RAND:5
RESULT = 1
TRYCALLFORM PUNISHMENT_{LOCAL:1}(ARG,ARG:1)
SIF CFLAG:ARG:弱み握り <= 0
	CFLAG:ARG:弱み握り = 0
LOCAL = RESULT
WEND
SIF !CFLAG:ARG:弱み握り
	PRINTFORML  这之后被%CALLNAME:ARG%饶过了的样子…
DRAWLINE
WAIT
;パンツを脱がせる
@PUNISHMENT_0(ARG,ARG:1)
SIF !CFLAG:PLAYER:206
	RETURN 1
CFLAG:PLAYER:206 = 0

PRINTFORML %CALLNAME:ARG%奪走了%CALLNAME:PLAYER%的内袴
PRINTFORML 下令今天一天都得不穿内袴渡过并将凌乱的衣服整理好…
CFLAG:ARG:弱み握り -= 1
EXP:ARG:嗜虐快楽経験 += 1
RETURN 0

;跳蛋挿入
@PUNISHMENT_1(ARG,ARG:1)
SIF !(TALENT:PLAYER:性別 & 1)
	RETURN 1
CFLAG:PLAYER:跳蛋挿入 = 240
PRINTFORML %CALLNAME:ARG%把跳蛋塞进%CALLNAME:PLAYER%的私処并固定好
PRINTFORML 下令就这样呆着一段时间并将凌乱的衣服整理好…
CFLAG:ARG:弱み握り -= 2
EXP:ARG:嗜虐快楽経験 += 10

RETURN 0

;跳蛋A挿入
@PUNISHMENT_2(ARG,ARG:1)
;応急処置としてコメントアウトしておきます
;SIF CFLAG:PLAYER:跳蛋A挿入
;	RETURN 1
CFLAG:PLAYER:跳蛋A挿入 = 240
PRINTFORML %CALLNAME:ARG%把跳蛋塞进%CALLNAME:PLAYER%的肛门并固定好
PRINTFORML 下令就这样呆着一段时间并将凌乱的衣服整理好…
CFLAG:ARG:弱み握り -= 2
EXP:ARG:嗜虐快楽経験 += 10

RETURN 0

;キスマーク
@PUNISHMENT_3(ARG,ARG:1)
SIF TFLAG:キスマーク
	RETURN 1
SIF !TALENT:ARG:恋慕
	RETURN 1
PRINTFORML %CALLNAME:ARG%抱住%CALLNAME:PLAYER%、用力吸着后頸留下了吻痕…
PRINTFORML 在它消失之前似乎别让其他的女孩子看到比较好…
TFLAG:キスマーク = ARG
CFLAG:ARG:弱み握り -= 1
EXP:ARG:嗜虐快楽経験 += 2
RETURN 0
[SKIPSTART]
@PUNISHMENT_4(ARG,ARG:1)
PRINTFORMW %CALLNAME:ARG%対%CALLNAME:PLAYER%、下達了現在当場自慰的命令……
$LOOP
CALL ASK_M("学習地做",1,"展示自慰的樣子",ABL:MASTER:露出癖 >= 3,"辺説喜歡辺做",ABL:MASTER:露出癖 >= 5)
SELECTCASE RESULT
	CASE 1
		PRINTFORMW %CALLNAME:MASTER%不情不願的開始了自慰、但是由於是在注視之下所以\@ TALENT:MASTER:性別 == 1? 完全沒有湿# 萎靡不振完全沒有勃起的樣子\@……
		IF ABL:ARG:欲望 < 5
			IF ABL:ARG:施虐属性 >= 3 && (TALENT:MASTER:性別 & 2)
				PRINTFORML %CALLNAME:ARG%用難聽的話罵着%CALLNAME:MASTER%
				PRINTFORMW %CALLNAME:MASTER%的心霊受到了深深地傷害
				BASE:MASTER:気力 -= 1000
				BASE:MASTER:勃起 = 0
				SOURCE:ARG:強要 += 300
				SOURCE:ARG:侮辱 += 300
				SOURCE:ARG:加虐 += 500
				CFLAG:ARG:弱み握り -= 2
				TIME += 3
			ELSEIF (TALENT:ARG:思慕 || TALENT:ARG:恋慕)
				PRINTFORML %CALLNAME:ARG%説道会有這種時候的、進行安慰
				PRINTFORMW %CALLNAME:MASTER%的心霊受到了傷害
				BASE:MASTER:気力 -= 300
				BASE:MASTER:勃起 = 0
				SOURCE:ARG:強要 += 200
				SOURCE:ARG:侮辱 += 200
				SOURCE:ARG:加虐 += 100
				SOURCE:ARG:情愛 += 50
				CFLAG:ARG:弱み握り -= 2
				TIME += 3
			ELSE
				PRINTFORML %CALLNAME:ARG%非常無聊的樣子、説道已経可以了
				PRINTFORMW %CALLNAME:MASTER%的心霊受到了傷害
				BASE:MASTER:気力 -= 500
				BASE:MASTER:勃起 = 0
				SOURCE:ARG:強要 += 200
				SOURCE:ARG:侮辱 += 200
				SOURCE:ARG:加虐 += 300
				CFLAG:ARG:弱み握り -= 1
				TIME += 3
			ENDIF
		ELSEIF ABL:ARG:施虐属性 >= 3
			PRINTFORML %CALLNAME:ARG%説着我来幇你吧、開始用脚摩擦着%CALLNAME:MASTER%的股間……
			PRINTFORML 於是、%CALLNAME:MASTER%的\@ TALENT:MASTER:性別 == 1? 私処立刻就変得湿潤了# 肉棒很快就恢復了硬度\@
			PRINTFORML 被這種樣子煽動起嗜虐心的%CALLNAME:ARG%、言辞和脚下的動作更加激烈了
			PRINTFORML 不久%CALLNAME:MASTER%発出了沒出息的呻吟達到了絶頂
			IF TALENT:MASTER:性別 & 2
				BASE:MASTER:勃起 = 500
				BASE:MASTER:精力 = MAX(BASE:MASTER:精力 - 100,0)
				EXP:MASTER:射精経験 ++
			ENDIF
			BASE:MASTER:気力 = MAX(BASE:MASTER:気力 - 300,0)
			TIME += 10
			EXP:MASTER:絶頂経験 ++
			EXP:MASTER:自慰経験 ++
			SOURCE:ARG:強要 += 500
			SOURCE:ARG:侮辱 += 500
			SOURCE:ARG:加虐 += 700
			SOURCE:ARG:征服 += 700
			SOURCE:ARG:達成 += 300
			CFLAG:ARG:弱み握り -= 5
		ELSE
			PRINTFORML %CALLNAME:ARG%掀起了%SHOW_BOTTOM(ARG,1)%、讓%UNDER_SKIRT_DESCRIPTION(ARG)%暴露出来…
			PRINTFORML 於是、%CALLNAME:MASTER%的\@ TALENT:MASTER:性別 == 1? 私処立刻就変得湿潤了# 肉棒很快就恢復了硬度\@
			PRINTFORML 被命令重新開始的%CALLNAME:MASTER%、在高潮一次之後還在持續着自慰
			IF TALENT:MASTER:性別 & 2
				BASE:MASTER:勃起 = 500
				EXP:MASTER:射精経験 ++
				BASE:MASTER:精力 = MAX(BASE:MASTER:精力 - 50,0)
			ENDIF
			BASE:MASTER:気力 = MAX(BASE:MASTER:気力 - 300,0)
			TIME += 10
			EXP:MASTER:絶頂経験 ++
			EXP:MASTER:自慰経験 ++
			SOURCE:ARG:強要 += 300
			SOURCE:ARG:侮辱 += 300
			SOURCE:ARG:挑発 += 300
			SOURCE:ARG:征服 += 200
			SOURCE:ARG:誘惑 += 100
			CFLAG:ARG:弱み握り -= 3
			TIME += 10 
		ENDIF
	CASE 2
		PRINTFORMW まだ未実装なんだぜ
		GOTO LOOP
	CASE 3
		PRINTFORMW まだ未実装なんだぜ
		GOTO LOOP
ENDSELECT
RETURN 0
PRINTFORML %CALLNAME:ARG%用充満憐憫的視線看着%CALLNAME:MASTER%
PRINTFORML %CALLNAME:MASTER%的心霊受到了傷害


;[SKIPSTART]
PRINTFORML %CALLNAME:MASTER%非常喜歡%CALLNAME:ARG%、一辺高喊着最喜歡%CALLNAME:ARG%一辺開始了自慰
IF ABL:ARG:親密 < 5
	PRINTFORML %CALLNAME:ARG%慌張的命令停止
	BASE:MASTER:気力 -= 100
	SOURCE:ARG:強要 += 100
	SOURCE:ARG:侮辱 += 150
	SOURCE:ARG:情愛 += 50
	CFLAG:ARG:弱み握り -= 2
ELSE
	PRINTFORML %CALLNAME:ARG%有一点吃驚、但是並沒有感到討厭
	SIF ABL:ARG:施虐属性 >= 3
		PRINTFORML %CALLNAME:ARG%罵起了恬不知恥開始沈溺于快感中的%CALLNAME:MASTER%……
	IF ABL:MASTER:露出癖 >= 3 || ABL:MASTER:受虐属性 >= 3
		PRINTFORML 在%CALLNAME:ARG%的注視下感到興奮的%CALLNAME:MASTER%的自慰動作愈発激烈了……
		CFLAG:ARG:溜まってる度 += 150
	ENDIF
	IF ABL:ARG:欲望 > 5 && CFLAG:ARG:溜まってる度 > 500
		PRINTFORML 看着%CALLNAME:MASTER%自慰的樣子、%CALLNAME:ARG%也把手伸向了自己的私処……
		
ENDIF
[SKIPEND]
