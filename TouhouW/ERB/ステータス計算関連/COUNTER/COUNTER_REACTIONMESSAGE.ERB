;乳交
@EVENT_COUNTE_MESSAGE504
SELECTCASE TCVAR:21
	CASE 1
		IF TALENT:TARGET:胸囲 > 0
			PRINTFORMW %CALLNAME:TARGET%用胸部挟住了%CALLNAME:MASTER%的%TINKO(PLAYER)%…
		ELSE
			PRINTFORMW %CALLNAME:TARGET%用乳头咯哩咯哩地摩擦着%TINKO(PLAYER)%的环状沟…
		ENDIF
	;縦乳交,乳ファック
	CASE 2
		PRINTFORMW %CALLNAME:PLAYER%用%TINKO(PLAYER)%突入%CALLNAME:TARGET%豊満的胸部的间隙晃起了腰部…
	;谷間を作る
	CASE 3
		IF TALENT:TARGET:胸囲 < 0
			PRINTFORMW %CALLNAME:TARGET%拼命贴上平平的胸部想法设法来挟住%TINKO(PLAYER)%…
		ELSE
			PRINTFORMW %CALLNAME:TARGET%用挤圧胸部做出来的乳沟挟住%TINKO(PLAYER)%并激烈地動起了上半身…
		ENDIF
	;乳夾口交
	CASE 4
		PRINTFORMW 另外%CALLNAME:TARGET%还用舌头愛撫并吮吸起了露出来的亀頭…
ENDSELECT
RETURN 1

