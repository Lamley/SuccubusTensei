@挿入不可(ARG)
;-2 = ガバガバで気持ちよくない -1 = 刺激が弱い 1= 経験が必要 2= 経験と知識が必要 3= 小人飛機杯
#FUNCTION
#DIM 主人公のサイズ
#DIM サイズ差
IF ARG == PLAYER
	RETURNF 0
ENDIF

SELECTCASE TALENT:PLAYER:形状
	CASE 0,3,4
		主人公のサイズ = 0
	CASE 1
		主人公のサイズ = -1
	CASE 6
		主人公のサイズ = 1
	CASE 5,10
		主人公のサイズ = 2
ENDSELECT

サイズ差 = (TALENT:PLAYER:体型 - TALENT:ARG:体型) * 2 + 主人公のサイズ

SELECTCASE サイズ差
	CASE IS > 8
		RETURNF 3
	CASE 5 TO 7
		RETURNF 2
	CASE 3 TO 4
		RETURNF 1
	CASE -2 TO 2
		RETURNF 0
	CASE -3 TO -6
		RETURNF -1
	CASE IS < -6
		RETURNF -2
ENDSELECT
	