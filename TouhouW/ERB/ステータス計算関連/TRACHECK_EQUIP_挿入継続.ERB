SIF !FLAG:70 && !CFLAG:ARG:イタズラ
	EXP:ARG:Ａ性交経験 ++
SIF TEQUIP:ARG:体位 == 6
	EXP:ARG:Ａ拡張経験 ++
RETURN 1


@挿入継続Ｖ苦痛処理(ARG)
;EXP:Ｖ経験をみる（処女はありえない）
LOCAL = 200
LOCAL:1 = 150
IF EXP:ARG:Ｖ経験 < EXPLV:2
	TIMES LOCAL , 0.80
	TIMES LOCAL:1 , 1.20
ELSEIF EXP:ARG:Ｖ経験 < EXPLV:3
	TIMES LOCAL , 0.40
ELSEIF EXP:ARG:Ｖ経験 < EXPLV:4
	TIMES LOCAL , 0.10
ELSEIF EXP:ARG:Ｖ経験 < EXPLV:5
	TIMES LOCAL , 0.00
ELSEIF EXP:ARG:Ｖ経験 >= EXPLV:5
	TIMES LOCAL , 0.00
ENDIF

;PALAM:潤滑をみる
