;挿入継続の処理
@EQUIP_INCERT_V(ARG)
#DIM 竿役

竿役 = TEQUIP:ARG:50
;-------------------------------------------------
;行為者
;-------------------------------------------------
TCVAR:ARG:116 = 竿役
;TCVAR:2 射精箇所フラグ（-1=避孕套 1=膣内 2=アナル 3=手淫 4=口淫 5=乳交 6=素股 7=足交 8=体表 9=肛門奉仕 10=道具 11=触手 12=Ａ責め 13=Ｖ責め）
;TCVAR:4 射精快感強度

CSTR:竿役:10 = @"/{ARG}/"
TCVAR:竿役:射精箇所 = 1

CSTR:ARG:10 += @"{TEQUIP:ARG:50}/"
SIF TCVAR:ARG:射精箇所 == 0 || TCVAR:ARG:射精箇所 >= 8
	TCVAR:ARG:射精箇所 = 13


IF ARG == MASTER
	PRINTFORML ＜%CALLNAME:竿役%插入Ｖ中＞

	TCVAR:竿役:射精快感強度 = 850
	TCVAR:ARG:射精快感強度 = 500 + (100 * ABL:ARG:Ｖ感覚)

	DOWNBASE:竿役:体力 += 20
	DOWNBASE:竿役:気力 += 40

	SOURCE:ARG:快Ｖ += 200
	SOURCE:竿役:快Ｃ += 200
	SOURCE:竿役:情愛 += 25
	SOURCE:竿役:性行動 += 250
	SOURCE:竿役:達成 += 25
	SOURCE:竿役:征服 += 500
	SOURCE:竿役:逸脱 += 50

ELSE
	PRINTFORML ＜%CALLNAME:ARG%Ｖ被插入中＞

	TCVAR:竿役:射精快感強度 = 700 + TEQUIP:ARG:子宮 * 100
	TCVAR:ARG:射精快感強度 = 750

	;時間停止中
	IF FLAG:70
		SOURCE:ARG:快Ｖ += 150
		CALL 挿入継続Ｖ苦痛処理(ARG)

	ELSE
		DOWNBASE:ARG:体力 += 10
		DOWNBASE:ARG:気力 += 30
		SIF TEQUIP:ARG:体位 == 6
			SOURCE:ARG:苦痛 += 300
		SIF TALENT:竿役:形状 == 5
			SOURCE:ARG:苦痛 += 200
		SOURCE:ARG:快Ｖ += 150
		SOURCE:ARG:情愛 += 75
		SOURCE:ARG:露出 += 25
		SIF TCVAR:ARG:無理矢理
			SOURCE:ARG:反感 += 1000
		;体位ボー茄子
		IF TEQUIP:ARG:体位 == CFLAG:ARG:好きな体位
			SOURCE:ARG:情愛 += 100
			SOURCE:ARG:恭順 += 50
		ENDIF
		CALL 挿入継続Ｖ苦痛処理(ARG)
	ENDIF

	IF CFLAG:ARG:うふふ == 2
		SOURCE:ARG:快Ｖ += 150
		SOURCE:ARG:情愛 += 100
		TCVAR:竿役:射精快感強度 += 150
	ENDIF
ENDIF

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
EXP:竿役:挿入経験 ++
EXP:ARG:Ｖ経験 ++
SIF !FLAG:70 && !CFLAG:ARG:イタズラ
EXP:ARG:Ｖ性交経験 ++
SIF TEQUIP:ARG:体位 == 6
	EXP:ARG:Ｖ拡張経験 ++
SIF FLAG:70 == 1
	EXP:MASTER:時姦経験 += 1
IF CFLAG:MASTER:イタズラ == 2
	EXP:MASTER:睡眠姦経験 += 1
	EXP:ARG:睡眠姦経験 += 1
ENDIF
RETURN 1


