;────────────────────────────────────
;絶頂系の処理など
;────────────────────────────────────
;-------------------------------------------------
;絶頂処理
;-------------------------------------------------
@ORGASM_ADD(ARG)
#DIM 合計絶頂数
#DIM 焦らし中

VARSET LOCAL
焦らし中 = 0
LOCAL = ((CUP:ARG:快Ｃ + PALAM:ARG:快Ｃ) >= PALAMLV:4) + (CUP:ARG:快Ｖ + PALAM:ARG:快Ｖ >= PALAMLV:4) + (CUP:ARG:快Ａ + PALAM:ARG:快Ａ >= PALAMLV:4) + (CUP:ARG:快Ｂ + PALAM:ARG:快Ｂ >= PALAMLV:4) + (CUP:ARG:快Ｍ + PALAM:ARG:快Ｍ >= PALAMLV:4)
SELECTCASE LOCAL
	CASE 5
		LOCAL:1 = 5000
	CASE 4
		LOCAL:1 = 3000
	CASE 3
		LOCAL:1 = 2000
	CASE 2
		LOCAL:1 = 1500
	CASE 1
		LOCAL:1 = 1000
ENDSELECT

;視認性の問題で残しておく
C = 0
V = 0
A = 0
B = 0
M = 0
;絶頂Ｃ
;うっかり絶頂
CALL 絶頂強度判定(ARG,0,LOCAL:1)
C = RESULT
;絶頂Ｖ
CALL 絶頂強度判定(ARG,1,LOCAL:1)
V = RESULT

;絶頂Ａ
CALL 絶頂強度判定(ARG,2,LOCAL:1)
A = RESULT

;絶頂Ｂ
CALL 絶頂強度判定(ARG,3,LOCAL:1)
B = RESULT
	
;絶頂Ｍ
CALL 絶頂強度判定(ARG,4,LOCAL:1)
M = RESULT
IF TEQUIP:ARG:焦らしモード
	IF 焦らし発生(ARG,C + V + A + B + M)
		EX:ARG:Ｃ寸止め += C
		EX:ARG:Ｖ寸止め += V
		EX:ARG:Ａ寸止め += A
		EX:ARG:Ｂ寸止め += B
		EX:ARG:Ｍ寸止め += M
		TCVAR:ARG:焦らし回数 ++
		焦らし中 = 1
	ENDIF
ENDIF
SIF 焦らし中
	GOTO JIRASI
IF CUP:ARG:快Ｃ
	C += EX:ARG:Ｃ寸止め
	EX:ARG:Ｃ寸止め = 0
ENDIF
IF CUP:ARG:快Ｖ
	V += EX:ARG:Ｖ寸止め
	EX:ARG:Ｖ寸止め = 0
ENDIF
IF CUP:ARG:快Ａ
	A += EX:ARG:Ａ寸止め
	EX:ARG:Ａ寸止め = 0
ENDIF
IF CUP:ARG:快Ｂ
	B += EX:ARG:Ｂ寸止め
	EX:ARG:Ｂ寸止め = 0
ENDIF
IF CUP:ARG:快Ｍ
	M += EX:ARG:Ｍ寸止め
	EX:ARG:Ｍ寸止め = 0
ENDIF
;-------------------------------------------------
;絶頂時の追加処理
;-------------------------------------------------
IF C && V && A && B && M
	C *= 16
	V *= 16
	A *= 16
	B *= 16
	M *= 16
	NOWEX:ARG:五重絶頂 = 1
ELSEIF C && V && A && B
	C *= 8
	V *= 8
	A *= 8
	B *= 8
	NOWEX:ARG:四重絶頂 = 1
ELSEIF C && V && A && M
	C *= 8
	V *= 8
	A *= 8
	M *= 8
	NOWEX:ARG:四重絶頂 = 2
ELSEIF C && V && B && M
	C *= 8
	V *= 8
	B *= 8
	M *= 8
	NOWEX:ARG:四重絶頂 = 3
ELSEIF C && A && B && M
	C *= 8
	A *= 8
	B *= 8
	M *= 8
	NOWEX:ARG:四重絶頂 = 4
ELSEIF V && A && B && M
	V *= 8
	A *= 8
	B *= 8
	M *= 8
	NOWEX:ARG:四重絶頂 = 5
ELSEIF C && V && A
	C *= 4
	V *= 4
	A *= 4
	NOWEX:ARG:三重絶頂 = 1
ELSEIF B && V && A
	B *= 4
	V *= 4
	A *= 4
	NOWEX:ARG:三重絶頂 = 2
ELSEIF C && B && A
	C *= 4
	B *= 4
	A *= 4
	NOWEX:ARG:三重絶頂 = 3
ELSEIF C && V && B
	C *= 4
	V *= 4
	B *= 4
	NOWEX:ARG:三重絶頂 = 4
ELSEIF C && V && M
	C *= 4
	V *= 4
	M *= 4
	NOWEX:ARG:三重絶頂 = 5
ELSEIF B && V && M
	B *= 4
	V *= 4
	M *= 4
	NOWEX:ARG:三重絶頂 = 6
ELSEIF A && V && M
	A *= 4
	V *= 4
	M *= 4
	NOWEX:ARG:三重絶頂 = 7
ELSEIF B && C && M
	C *= 4
	B *= 4
	M *= 4
	NOWEX:ARG:三重絶頂 = 8
ELSEIF A && B && M
	A *= 4
	B *= 4
	M *= 4
	NOWEX:ARG:三重絶頂 = 9
ELSEIF C && A && M
	C *= 4
	A *= 4
	M *= 4
	NOWEX:ARG:三重絶頂 = 10
ELSEIF C && V
	C *= 2
	V *= 2
	NOWEX:ARG:二重絶頂 = 1
ELSEIF C && A
	C *= 2
	A *= 2
	NOWEX:ARG:二重絶頂 = 2
ELSEIF V && A
	V *= 2
	A *= 2
	NOWEX:ARG:二重絶頂 = 3
ELSEIF C && B
	C *= 2
	B *= 2
	NOWEX:ARG:二重絶頂 = 4
ELSEIF V && B
	V *= 2
	B *= 2
	NOWEX:ARG:二重絶頂 = 5
ELSEIF A && B
	A *= 2
	B *= 2
	NOWEX:ARG:二重絶頂 = 6
ELSEIF A && M
	A *= 2
	M *= 2
	NOWEX:ARG:二重絶頂 = 7
ELSEIF V && M
	V *= 2
	M *= 2
	NOWEX:ARG:二重絶頂 = 8
ELSEIF C && M
	C *= 2
	M *= 2
	NOWEX:ARG:二重絶頂 = 9
ELSEIF B && M
	B *= 2
	M *= 2
	NOWEX:ARG:二重絶頂 = 10
ENDIF


IF (C + V + A + B + M) > 0
	TCVAR:ARG:焦らし回数 = 0
	TEQUIP:ARG:焦らしモード = 0
ENDIF
合計絶頂数 = C + V + A + B + M
;時姦刻印
IF FLAG:70 && ARG
	IF 合計絶頂数 >= 16 && MARK:ARG:時姦刻印 < 3
		TCVAR:ARG:時姦刻印取得用 = 3
	ELSEIF 合計絶頂数 >= 8 && MARK:ARG:時姦刻印 < 2
		TCVAR:ARG:時姦刻印取得用 = 2
	ELSEIF  合計絶頂数 >= 1 && MARK:ARG:時姦刻印 < 1
		TCVAR:ARG:時姦刻印取得用 = 1
	ENDIF
ELSE
;快楽刻印の処理
;絶頂の部位と強度の合計が一定以上で刻印を取得する
;合計絶頂数が8以上で快楽刻印３に相当
	IF 合計絶頂数 >= 24 && MARK:ARG:快楽刻印 <= 3
		TCVAR:ARG:快楽強度 = 3
	;合計絶頂数が5以上で快楽刻印２に相当
	ELSEIF 合計絶頂数 >= 16 && MARK:ARG:快楽刻印 < 2
		TCVAR:ARG:快楽強度 = 2
	;合計絶頂数が2以上で快楽刻印１に相当
	ELSEIF 合計絶頂数 >= 2 && MARK:ARG:快楽刻印 < 1
		TCVAR:ARG:快楽強度 = 1
	ELSE
		TCVAR:ARG:快楽強度 = 0
	ENDIF
ENDIF
IF FLAG:70 == 1
ELSE

IF C
	SOURCE:ARG:露出 += 500 * C
	SOURCE:ARG:屈従 += 200 * C
	;SOURCE:ARG:反感 += 50 * C
	SOURCE:ARG:反感 += 40 * C * (CFLAG:ARG:うふふ ? 1 # 3)
	DOWNBASE:ARG:体力 += 20
	DOWNBASE:ARG:気力 += 10
ENDIF
IF V
	SOURCE:ARG:欲情 += 800 * V
	SOURCE:ARG:恭順 += 500 * V
	SOURCE:ARG:露出 += 700 * V
	SOURCE:ARG:屈従 += 400 * V
	SOURCE:ARG:反感 += 80 * V * (CFLAG:ARG:うふふ ? 1 # 3)
	DOWNBASE:ARG:体力 += 30
	DOWNBASE:ARG:気力 += 20
ENDIF
IF A
	SOURCE:ARG:欲情 += 500 * A
	SOURCE:ARG:恭順 += 700 * A
	SOURCE:ARG:露出 += 900 * A
	SOURCE:ARG:屈従 += 500 * A
	SOURCE:ARG:反感 += 120 * A * (CFLAG:ARG:うふふ ? 1 # 3)
	DOWNBASE:ARG:体力 += 40
	DOWNBASE:ARG:気力 += 20
ENDIF
IF B
	SOURCE:ARG:露出 += 500 * B
	SOURCE:ARG:屈従 += 200 * B
	SOURCE:ARG:反感 += 50 * B * (CFLAG:ARG:うふふ ? 1 # 3)
	DOWNBASE:ARG:体力 += 20
	DOWNBASE:ARG:気力 += 10
ENDIF
IF M
	SOURCE:ARG:情愛 += 1500 * M
	SOURCE:ARG:恭順 += 1000 * M
	DOWNBASE:ARG:体力 += 10
	DOWNBASE:ARG:気力 += 5
ENDIF
IF C && V && A && B && M
	DOWNBASE:ARG:体力 -= 80
	DOWNBASE:ARG:気力 -= 70
ELSEIF (C && V && A && B) || (C && V && A && M) || (B && V && A && M) || (C && B && A && M) || (C && V && B && M)
	DOWNBASE:ARG:体力 -= 40
	DOWNBASE:ARG:気力 -= 30

ELSEIF (C && V && A) || (B && V && A) || (C && B && A) || (C && V && B) || (C && V && M) || (B && V && M) || (C && B && M) || (C && A && M) || (V && A && M) || (B && A && M)
	DOWNBASE:ARG:体力 -= 20
	DOWNBASE:ARG:気力 -= 10
ENDIF
ENDIF
;NOWEXにデータを入れる（絶頂時口上に使う）
NOWEX:ARG:Ｃ絶頂 = C
NOWEX:ARG:Ｖ絶頂 = V
NOWEX:ARG:Ａ絶頂 = A
NOWEX:ARG:Ｂ絶頂 = B
NOWEX:ARG:Ｍ絶頂 = M
;絶頂回数を増やす
EX:ARG:Ｃ絶頂 += C
EX:ARG:Ｖ絶頂 += V
EX:ARG:Ａ絶頂 += A
EX:ARG:Ｂ絶頂 += B
EX:ARG:Ｍ絶頂 += M

;時間停止中もしくは睡眠中なら、無自覚絶頂回数を増やす（ただし、主人公を除く）
IF FLAG:70 > 0 || CFLAG:ARG:睡眠 && ARG
	EX:ARG:無自覚Ｃ絶頂 += C
	EX:ARG:無自覚Ｖ絶頂 += V
	EX:ARG:無自覚Ａ絶頂 += A
	EX:ARG:無自覚Ｂ絶頂 += B
	EX:ARG:無自覚Ｍ絶頂 += M
ENDIF

;絶頂経験を増やす
EXP:ARG:絶頂経験 += 合計絶頂数

;体位別絶頂経験
IF TEQUIP:ARG:50 > -1
	LOCAL:3 = TEQUIP:ARG:体位
ELSEIF TEQUIP:ARG:51 > -1
	LOCAL:3 = TEQUIP:ARG:体位 + 5
ELSE
	LOCAL:3 = 0
ENDIF
SIF LOCAL:3
	EXP:ARG:(LOCAL:3 + 100) += 1

;絶頂経験の追加、無自覚のみ主人公に加算されない
SIF FLAG:70 == 1 || CFLAG:ARG:睡眠 && ARG
	EXP:ARG:無自覚絶頂経験 += C + V + A + B + M
EXP:ARG:Ｃ絶頂経験 += C
EXP:ARG:Ｖ絶頂経験 += V
EXP:ARG:Ａ絶頂経験 += A
EXP:ARG:Ｂ絶頂経験 += B
EXP:ARG:Ｍ絶頂経験 += M

;ビデオ
IF TEQUIP:ARG:28
	IF NOWEX:ARG:8
		LOCALS = O2/O3/O4/
		TSTR:1 += LOCALS
	ENDIF
	IF NOWEX:ARG:7
		LOCALS = O2/O3/
		TSTR:1 += LOCALS
	ENDIF
	IF NOWEX:ARG:6
		LOCALS = O2/
		TSTR:1 += LOCALS
	ENDIF
	IF NOWEX:ARG:0 == 2
		LOCALS = C/C/
		TSTR:1 += LOCALS
	ELSEIF NOWEX:ARG:0 == 1
		LOCALS = C/
		TSTR:1 += LOCALS
	ENDIF
	IF NOWEX:ARG:1 == 2
		LOCALS = V/V/
		TSTR:1 += LOCALS
	ELSEIF NOWEX:ARG:1 == 1
		LOCALS = V/
		TSTR:1 += LOCALS
	ENDIF
	IF NOWEX:ARG:2 == 2
		LOCALS = A/A/
		TSTR:1 += LOCALS
	ELSEIF NOWEX:ARG:2 == 1
		LOCALS = A/
		TSTR:1 += LOCALS
	ENDIF
	IF NOWEX:ARG:3 == 2
		LOCALS = B/B/
		TSTR:1 += LOCALS
	ELSEIF NOWEX:ARG:3 == 1
		LOCALS = B/
		TSTR:1 += LOCALS
	ENDIF
ENDIF
$JIRASI
;IF 焦らし中

RETURN 1

