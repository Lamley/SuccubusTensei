;-------------------------------------------------
;調教対象の噴乳チェック
;-------------------------------------------------
@TARGET_MILK_CHECK(ARG)
SIF !TALENT:ARG:母乳体質
	RETURN 0

LOCAL = CUP:ARG:快Ｃ / 10 + CUP:ARG:快Ｖ / 10 + CUP:ARG:快Ａ / 10 + CUP:ARG:快Ｂ * 3
SIF LOCAL == 0
	RETURN 0

;自制心
SIF TALENT:ARG:自制心
	LOCAL /= 2

;接受快感
SIF TALENT:ARG:快感応答 > 0
	TIMES LOCAL , 1.20

;否定快感（淫乱、娼婦で無効）
SIF TALENT:ARG:快感応答 < 0 && !TALENT:ARG:淫乱
	TIMES LOCAL , 0.80

;B敏感
SIF TALENT:ARG:Ｂ感度 > 0
	TIMES LOCAL , 1.20

;媚薬
SIF TCVAR:ARG:媚薬 || TCVAR:ARG:発情
	LOCAL *= 2

;利尿剤
SIF TCVAR:ARG:利尿剤
	LOCAL /= 2

;睡眠薬
SIF TCVAR:ARG:睡眠薬強度
	LOCAL /= 2

;調教者が幼児退行
SIF TALENT:PLAYER:幼児／幼児退行
	TIMES LOCAL , 1.20

;調教者が幼稚
SIF TALENT:PLAYER:幼稚
	TIMES LOCAL , 1.20

;貧乳
SIF TALENT:ARG:胸囲 < 0
	TIMES LOCAL , 0.50

LOCAL = 100 + LOCAL / 2
SIF TEQUIP:ARG:焦らしモード
	LOCAL = LOCAL * 8 / 10
BASE:ARG:母乳 += LOCAL
IF TEQUIP:ARG:焦らしモード && BASE:ARG:母乳 > MAXBASE:ARG:母乳 && ARG
	TCVAR:ARG:焦らし回数 ++
	RETURN 0
ELSEIF  BASE:ARG:母乳 > MAXBASE:ARG:母乳 * 2
	LOCAL = 1
ELSEIF BASE:ARG:母乳 > MAXBASE:ARG:母乳
	LOCAL = 0
ELSE
	RETURN 0
ENDIF
LOCAL ++
DOWNBASE:ARG:体力 += 20 * LOCAL
DOWNBASE:ARG:気力 += 40 * LOCAL
IF EXP:ARG:噴乳経験 < EXPLV:1
	SOURCE:ARG:露出 += 250 * LOCAL
	SOURCE:ARG:屈従 += 150 * LOCAL
ELSEIF EXP:ARG:噴乳経験 < EXPLV:2
