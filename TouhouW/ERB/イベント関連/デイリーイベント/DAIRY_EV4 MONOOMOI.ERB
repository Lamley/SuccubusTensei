@DAIRY_CONDITION_EV4

SIF RAND:7 > 0
	RETURN -1
CALL DAIRY_EV4
RETURN 1

@DAIRY_EV4
CALL FISHER_YATES_SHAFFLE(CHARANUM - 1)
FOR LOCAL:10,1,CHARANUM
	LOCAL = SHAFFLE_ARRAY:(LOCAL:10) + 1
	IF ABL:LOCAL:親密 >= 5 && !CFLAG:LOCAL:神社在住
		PRINTFORMW 結束了思考的%CALLNAME:LOCAL%、注意到自己会在不知不覚中就想着%CALLNAME:MASTER%
		PRINTFORMW %CALLNAME:LOCAL%決定明天去見%CALLNAME:MASTER%
		CFLAG:LOCAL:翌日来訪 = 1
		BREAK
	ENDIF
NEXT

