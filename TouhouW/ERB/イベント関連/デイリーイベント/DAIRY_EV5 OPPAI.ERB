@GOD_FUTANARI
CALL COLORMESSAGE("嚯、沒想到人類察覚到了吾的正体",C_D_RED,2,1)
PRINTFORML 
PRINTFORMW 声音主人的气息変強大了……
CALL COLORMESSAGE("你対扶她肉棒的愛真是了不起",C_D_RED,1,1)
CALL COLORMESSAGE("給你奨勵、説出你的願望吧",C_D_RED,1,1)
$LOOP
CALL ASK_M("沒什麼",1,"長出扶她肉棒",1,"強化扶她肉棒",1,"想吃扶她肉棒",1,"請給我内袴",1)
SELECTCASE RESULT
	CASE 0
		CALL COLORMESSAGE("不懂風趣……你応該展示出光榮接受恩惠的樣子",C_D_RED,1,1)
		CALL COLORMESSAGE("就這樣吧",C_D_RED,1,1)
	CASE 1
		PRINTFORML 選誰？
		DRAWLINE
		FOR LOCAL,0,CHARANUM
			SIF TALENT:LOCAL:性別 == 1
				PRINTFORMLC [{LOCAL}] %CALLNAME:LOCAL,15,LEFT%
		NEXT
		DRAWLINE
		PRINTFORML [999] 算了
		INPUT
		IF INRANGE(RESULT,0,CHARANUM) && TALENT:RESULT:性別 == 1
			CALL COLORMESSAGE("這個簡単",C_D_RED,1,1)
			PRINTFORMW %CALLNAME:RESULT%変成了扶她
			TALENT:RESULT:性別 = 3
			MAXBASE:RESULT:精力 = 1000
		ELSE
			GOTO LOOP
		ENDIF
	CASE 2
		PRINTFORML 誰的？
		DRAWLINE
		FOR LOCAL,1,CHARANUM
			SIF TALENT:LOCAL:性別 == 3 && MAXBASE:LOCAL:精力 < 1500
				PRINTFORMLC [{LOCAL}] %CALLNAME:LOCAL,15,LEFT%
		NEXT
		DRAWLINE
		PRINTFORML [999] 算了
		INPUT
		IF INRANGE(RESULT,1,CHARANUM) && TALENT:RESULT:性別 == 3 && MAXBASE:LOCAL:精力 < 1500
			CALL COLORMESSAGE("這個簡単",C_D_RED,1,1)
			PRINTFORMW %CALLNAME:RESULT%的精力最大値増加
			MAXBASE:RESULT:精力 += 100
		ELSE
			GOTO LOOP
		ENDIF
	CASE 3
		CALL COLORMESSAGE("呵呵、対扶她肉棒的愛讓吾非常愉快",C_D_RED,1,1)
		CALL COLORMESSAGE("就在這收下它、尽情的品嘗吧",C_D_RED,1,1)
		PRINTFORML 突然%CALLNAME:MASTER%的口中出現了巨大的質量
		PRINTFORML 從那既堅挺又柔軟的肉棒上、能感到圧潰般的力道和温暖的安心感
		PRINTFORML 片刻之後被圧倒的%CALLNAME:MASTER%反応過来、拼命的奉仕着超越人知的東西
		PRINTFORML 在無限的苦悶和至福的時間的尽頭、那超常的陽物終於迎来了絶頂
		PRINTFORML 品味着那無法言喻的芳醇気味和喉嚨深処被射入的膨大神気、%CALLNAME:MASTER%失去了意識……
		CALL BUFF_BASE(MASTER,BASE_気力,2000)
		CALL BUFF_BASE(MASTER,BASE_体力,2000)
	CASE 4
		CALL COLORMESSAGE("不穿那種東西",C_D_RED,2,1)
		GOTO LOOP
ENDSELECT
CALL COLORMESSAGE("不要忘記、扶她与您同在",C_D_RED,1,1)
PRINTFORML 強大的气息離開了……


@OPPAIANSWER(ARG,ARG:1)
IF ARG:1 == -1
	;CALL COLORMESSAGE("滅びよ悪しき者！！！","RED",1,1)
	SETCOLOR C_RED
	FONTBOLD
	PRINTFORMW 邪悪的人類毀滅吧！！！
	RESETCOLOR
	FONTREGULAR
	PRINTFORML 在淒厲的怒吼和像要掀開腦殼一樣的衝撃中、%CALLNAME:MASTER%的意識断絶了
	BASE:MASTER:体力 -= 10000
ELSEIF ARG:1 == 2
	CALL COLORMESSAGE("竟然会選那個……",C_AQUA,1,1)
	PRINTFORML 那之後声音就断絶了
ELSEIF !ARG:1
		;これが正解の分岐を後で作る
		;PRINTFORML 主人公は心底おっぱいを愛しているのですね……
		SETCOLOR C_AQUA
		FONTBOLD
		PRINTFORMW 可悲的沒有信念的人……
		RESETCOLOR
		FONTREGULAR
			;CALL COLORMESSAGE("哀れなる信念なき者……","AQUA",1,1)
		PRINTFORML 那之後声音就断絶了
ELSE
	SETCOLOR C_AQUA
	FONTBOLD
	PRINTFORML 這個想法只要你沒有忘記、\@ ARG < 0 ? 貧乳# 巨乳\@就永遠是你的夥伴…
	PRINTFORML 給你\@ ARG < 0 ? 貧乳# 巨乳\@的加護吧……
	RESETCOLOR
	FONTREGULAR
	CALL ASK_YN("想要揉搓胸部","希望祝福胸部")
	IF RESULT
		PRINTFORML 誰的？
		DRAWLINE
		FOR LOCAL,0,CHARANUM
			IF ARG > 0
				SIF TALENT:LOCAL:胸囲 < 2
					PRINTFORMLC [{LOCAL}] %CALLNAME:LOCAL%
			ELSE
				SIF TALENT:LOCAL:胸囲 > -2
					PRINTFORMLC [{LOCAL}] %CALLNAME:LOCAL%
			ENDIF
		NEXT
		PRINTFORML 
		DRAWLINE
		PRINTFORML [999] 算了
		INPUT
		SELECTCASE RESULT
			CASE 0 TO CHARANUM
				SETCOLOR C_AQUA
				FONTBOLD
				PRINTFORMW %CALLNAME:RESULT%的胸部被祝福了……
				RESETCOLOR
				FONTREGULAR
				IF ARG > 0
					TALENT:RESULT:胸囲 ++
					PRINTFORMW %CALLNAME:RESULT%的胸変大了！
				ELSE
					TALENT:RESULT:胸囲 --
					PRINTFORMW %CALLNAME:RESULT%的胸変小了！
				ENDIF
			CASEELSE
		ENDSELECT
	ELSE
		;揉ませてもらえる分岐は未完成
		SETCOLOR C_AQUA
		FONTBOLD
		PRINTFORML 当你対胸部的熱愛更強烈的時候讓你揉吧……
		IF !TALENT:MASTER:霊活手指
			PRINTFORMW 在那之前、給你更加喜歡胸部的術……
			RESETCOLOR
			FONTREGULAR
			PRINTFORMW %CALLNAME:MASTER%得到了【霊活手指】！
			TALENT:MASTER:霊活手指 = 1
		ENDIF
		RESETCOLOR
		FONTREGULAR
	ENDIF
	PRINTFORMW 在胸部包囲般的愜意中、%CALLNAME:MASTER%的意識墜入了夢寐……
ENDIF
