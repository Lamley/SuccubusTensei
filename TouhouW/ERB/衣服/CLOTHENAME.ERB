		ELSEIF EQUIP:ARG:上半身内袴１ == 8
			RETURNF "穿環"
		ELSE
			RETURNF "内袴"
		ENDIF
	ENDIF
ELSEIF LOCAL == 1
	IF EQUIP:ARG:和服
		SIF EQUIP:ARG:和服 == 2
			RETURNF "襦袢"
		RETURNF "和服"
	ELSEIF EQUIP:ARG:上半身上衣２
		IF EQUIP:ARG:上半身上衣２ == 3
			RETURNF "パジャマ"
		ELSEIF EQUIP:ARG:上半身上衣２ == 4
			RETURNF "巫女服"
