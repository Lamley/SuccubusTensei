[SKIPSTART]
;CLOTHESに移動しました
;ファイル丸ごと使用していません
;-------------------------------------------------
;関数名:SHOW_BOTTOM
;概　要:下半身服装名取得関数
;戻り値:服装名(文字列)
;引数:0 参照するキャラ番号
;引数:1裙子、2ずらし可、3ずらし不可、4下半身上衣ずらし不可の種別(省略した場合は最も露出を妨げているもの)
;備　考:文中関数
;TEQUIP:0に対応して一番上に着ている衣装の種類を返す
;-------------------------------------------------
@SHOW_BOTTOM(ARG,ARG:1)
#FUNCTIONS
LOCAL = 0
IF TEQUIP:ARG:0 & 8
	LOCAL = 4
ELSEIF TEQUIP:ARG:0 & 4
	LOCAL = 3
ELSEIF TEQUIP:ARG:0 & 2
	LOCAL = 2
ELSEIF TEQUIP:ARG:0 & 1
	LOCAL = 1
ENDIF
LOCAL = ARG:1 ? ARG:1 # LOCAL


SELECTCASE LOCAL
	CASE 4
		IF EQUIP:ARG:袴子
			SIF EQUIP:ARG:和服 == 6
				RETURNF "袴"
			RETURNF "着衣"
		ELSEIF EQUIP:ARG:連体内衣
			IF EQUIP:ARG:連体内衣 == 2
				RETURNF "膠衣"
			ELSEIF EQUIP:ARG:連体内衣 == 3
				RETURNF "機車夾克"
			ELSE
				RETURNF "連体内衣"
			ENDIF
		ENDIF
	CASE 3
		IF EQUIP:ARG:袴子
			RETURNF "着衣"
		ELSEIF EQUIP:ARG:連体内衣
			IF EQUIP:ARG:連体内衣 == 2
				RETURNF "膠衣"
			ELSEIF EQUIP:ARG:連体内衣 == 3
				RETURNF "機車夾克"
			ELSE
				RETURNF "連体内衣"
			ENDIF
		ELSEIF EQUIP:ARG:緊身内衣
			IF (EQUIP:ARG:緊身内衣 == 2 || EQUIP:ARG:緊身内衣 == 3 || EQUIP:ARG:緊身内衣 == 10 || EQUIP:ARG:緊身内衣 == 11)
				RETURNF "インナー"
			ELSEIF (EQUIP:ARG:緊身内衣 == 4 || EQUIP:ARG:緊身内衣 == 5)
				RETURNF "兔女郎服"
			ELSEIF EQUIP:ARG:緊身内衣 == 9
				RETURNF "紐水着"
			ELSEIF EQUIP:ARG:緊身内衣 >= 6
				RETURNF "水着"
			ELSE
				RETURNF "緊身内衣"
			ENDIF
		ELSEIF EQUIP:ARG:下半身内袴１
			IF EQUIP:ARG:下半身内袴１ == 1
				RETURNF "兜襠布"
			ELSEIF EQUIP:ARG:下半身内袴１ == 2
				RETURNF "Ｃ字袴"
			ELSEIF EQUIP:ARG:下半身内袴１ == 3
				RETURNF "創口貼"
			ELSEIF EQUIP:ARG:下半身内袴１ == 4
				RETURNF "安全袴"
			ELSEIF EQUIP:ARG:下半身内袴１ == 5
				RETURNF "貞操帯"
			ELSEIF (EQUIP:ARG:下半身内袴１ == 6 || EQUIP:ARG:下半身内袴１ == 7)
				RETURNF "穿環"
			ELSE
			RETURNF "内袴"
			ENDIF
		ENDIF
	CASE 2
		IF EQUIP:ARG:緊身内衣
			IF (EQUIP:ARG:緊身内衣 == 2 || EQUIP:ARG:緊身内衣 == 3 || EQUIP:ARG:緊身内衣 == 10 || EQUIP:ARG:緊身内衣 == 11)
				RETURNF "インナー"
			ELSEIF (EQUIP:ARG:緊身内衣 == 4 || EQUIP:ARG:緊身内衣 == 5)
				RETURNF "兔女郎服"
			ELSEIF EQUIP:ARG:緊身内衣 == 9
				RETURNF "紐水着"
			ELSEIF EQUIP:ARG:緊身内衣 >= 6
				RETURNF "水着"
			ELSE
				RETURNF "緊身内衣"
			ENDIF
		ELSEIF EQUIP:ARG:下半身上衣
			RETURNF "着衣"
		ELSEIF EQUIP:ARG:下半身内袴２
			IF EQUIP:ARG:下半身内袴２ == 20
				RETURNF "比基尼"
			ELSEIF EQUIP:ARG:下半身内袴２ == 21
				RETURNF "微型比基尼"
			ELSE
				RETURNF "内袴"
			ENDIF
		ENDIF
	CASE 1
		IF EQUIP:ARG:裙子
			IF EQUIP:ARG:裙子 == 2
				RETURNF "長裙"
			ELSEIF EQUIP:ARG:裙子 == 4
				RETURNF "行灯袴"
			ELSE
				RETURNF "裙子"
			ENDIF
		ELSEIF EQUIP:ARG:連衣裙
			RETURNF "裙子"
		ELSEIF EQUIP:ARG:和服
			RETURNF "和服下擺"
		ENDIF
ENDSELECT
;-------------------------------------------------
