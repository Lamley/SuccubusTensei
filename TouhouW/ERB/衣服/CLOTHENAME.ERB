		ELSEIF EQUIP:ARG:緊身内衣 >= 6
			RETURNF "水着"
		ELSE
			RETURNF "緊身内衣"
		ENDIF
	ELSEIF EQUIP:ARG:連体内衣
		IF EQUIP:ARG:連体内衣 == 2
			RETURNF "膠衣"
		ELSEIF EQUIP:ARG:連体内衣 == 3
			RETURNF "機車夾克"
		ELSE
			RETURNF "連体内衣"
		ENDIF
	ELSEIF EQUIP:ARG:上半身内袴１
		IF (EQUIP:ARG:上半身内袴１ == 1 || EQUIP:ARG:上半身内袴１ == 3)
			RETURNF "文胸"
		ELSEIF EQUIP:ARG:上半身内袴１ == 2
