@COM353
;猥褻
IF FLAG:甲斐性無
	PRINTFORMW %CALLNAME:MASTER%沒有那樣的胆量
	RETURN -1
ENDIF
FOR LOCAL,1,CHARANUM
	SIF TARGET:LOCAL <= 0
		CONTINUE
	CALL SET_PRANK(1,TARGET:LOCAL,1)
NEXT
SIF CFLAG:TARGET:睡眠姦 < 1
	CFLAG:TARGET:睡眠姦 = 1
RETURN 1

;-------------------------------------------------
;実行判定
;-------------------------------------------------
@COM_ABLE353
;イタズラ実行判定
SIF !TFLAG:100
	RETURN 0
;一括管理
SIF GLOBAL_COMABLE(353)
	RETURN RESULT
;睡眠中のみ
SIF CFLAG:睡眠 == 0
	RETURN 0
;ターゲットの私室でのみ
SIF CFLAG:MASTER:現在位置 != CFLAG:311
	RETURN 0

;時間停止中不可
SIF FLAG:70
	RETURN 0
;すでにイタズラモード
SIF CFLAG:イタズラ
	RETURN 0
;添い寝中は出来ない
SIF CFLAG:添い寝中
	RETURN 0
;他に誰か居る
FOR LOCAL,0,CHARANUM
	;TARGET:1～をさがす( MASTER [0] は見ない )
	SIF !TARGET:LOCAL
		CONTINUE
	;同室フラグチェック
	SIF CFLAG:(TARGET:LOCAL):同室 && (TARGET:LOCAL) != TARGET
		RETURN 0
NEXT
;気力0
SIF BASE:MASTER:気力 <= 0
	RETURN 0
;弱ってたら無し
SIF CFLAG:衰弱 > 0
	RETURN 0
RETURN 1

