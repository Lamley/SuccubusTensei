			CALL ニンジャセット(ARG)
			GOTO INPUT_LOOP_コスプレ
		ELSEIF RESULT == 999
			GOTO INPUT_LOOP
		ELSE
			GOTO INPUT_LOOP_コスプレ
		ENDIF
	ELSEIF RESULT == 18
		FOR 装備部位,0,23
			EQUIP:ARG:装備部位 = 0
		NEXT
		PRINTFORMW %CALLNAME:ARG%変成全裸了
		GOTO INPUT_LOOP
	ELSEIF RESULT == 100
		IF EQUIP:MASTER:0 == 0
			PRINTFORM %CALLNAME:MASTER%大人、虽然非常难以启齿、
			IF (TALENT:MASTER:2 & 2)
				PRINT 小雞雞
			ELSE
				PRINT 某些重要部位
			ENDIF
			PRINTFORMW 露出来真的好吗？
			PRINTL [0] 説的也是ー、还是算了吧
			PRINTL [1] 不明白你什么意思呢、那样不是也挺好的吗
			PRINTL [2] 能幇我系上领帯吗
			INPUT
			IF RESULT == 1
				PRINTFORMW 那真是失礼了。原来如此、这就是您的行事风格吗
				PRINTFORMW 那么就請尽情享受全裸的生活吧
				SOURCE:MASTER:露出 += 300
				EXP:MASTER:露出快楽経験 ++
				GOTO 換装終了処理
			ELSEIF RESULT == 2
				PRINTFORMW %CALLNAME:MASTER%大人、整理着装的事还請您自己做。
				PRINTFORMW …那么
				PRINTFORMW 首先請低头…将衬底朝内…
				PRINTFORML 
				PRINTFORML （摸来摸去）
				PRINTFORMW （摸来摸去）
				PRINTFORMW 您在做什么呢。
				PRINTFORML 
				PRINTFORMW 把细的那头往粗的那头…从上方囲上一圈…
				PRINTFORML 
				PRINTFORML （摸来摸去）
				PRINTFORMW （摸来摸去）
				PRINTFORMW 請不要摸我的头。
				PRINTFORML 
				PRINTFORMW 穿过里面…从上面塞进去…
				PRINTFORMW 啊。
				PRINTFORML 
				PRINTFORMW 一次…二次…穿过里面…
				PRINTFORMW 从上面塞进去…拉紧…
				PRINTFORMW 好。系好了哦%CALLNAME:MASTER%大人。
				PRINTFORML 
				PRINTFORML （摸来摸去）
				PRINTFORMW （摸来摸去）
				PRINTFORMW 所以呢、能請您不要再摸头了吗。
				PRINTFORML 
				PRINTFORMW 生疏的手法与系的不整斉的两侧领帯令%CALLNAME:MASTER%忍不住微笑起来
				PRINTFORMW %CALLNAME:MASTER%被系上了领帯！
				PRINTFORML 
				EQUIP:MASTER:其他１ = 5
				SOURCE:MASTER:露出 += 299
				EXP:MASTER:露出快楽経験 ++
				GOTO 換装終了処理
			ELSE
				GOTO INPUT_LOOP
			ENDIF
		ELSE
			CALL CLOTHES_SETTING_SAVE(ARG)
			CALL CLOTHES_SETTING_LOAD(ARG); 全裸判定を反映するために、保存した装備を現在の装備に反映しています。
			CFLAG:ARG:623 = 1
			GOTO 換装終了処理
		ENDIF
	ELSEIF RESULT == 999
		SIF FLAG:時間停止 == 1
			CFLAG:TARGET:時間停止換装 = 0
		;初期化
		FOR 装備部位,0,23
			EQUIP:ARG:装備部位 = 0
		NEXT
		CFLAG:ARG:623 = 0
		EQUIP:ARG:0 = 1
		CALL CLOTHES_SETTING(ARG)
		RETURN -1
	ELSE
		GOTO INPUT_LOOP
	ENDIF
	$換装終了処理
	;リセット用に記録する
	FOR 装備部位,0,23
		CFLAG:ARG:(200 + 装備部位) = EQUIP:ARG:装備部位
		;念のため
		;SIF EQUIP:ARG:0 == 0
			;EQUIP:ARG:0 = 0
	NEXT
	SIF INROOM(CFLAG:ARG:現在位置)
		CALL DATUI_SHOES(ARG,0)
	CALL CLOTHES_SETTING_TRAIN(ARG)
RETURN -1
