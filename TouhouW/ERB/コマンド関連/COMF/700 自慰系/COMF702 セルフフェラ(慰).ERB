;-------------------------------------------------
;セルフフェラ
;自慰系コマンド、レベル1
;-------------------------------------------------
@COM702
;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者
TCVAR:116 = PLAYER
;TCVAR:2 射精箇所フラグ（-1=避孕套 1=膣内 2=アナル 3=手淫 4=口淫 5=乳交 6=素股 7=足交 8=体表 9=肛門奉仕 10=道具 11=触手）

LOCALS = {PLAYER}/
CSTR:PLAYER:10 += LOCALS
TCVAR:PLAYER:2 = 4
;強度
TCVAR:PLAYER:4 = 1500

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
DOWNBASE:PLAYER:体力 += 5
DOWNBASE:PLAYER:気力 += 50

SOURCE:PLAYER:快Ｃ = 110
SOURCE:PLAYER:露出 = 20
SOURCE:PLAYER:逸脱 = 20
SOURCE:PLAYER:反感 = 20

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;圧抑、抵抗持ちには、抑鬱ソース追加
SIF TALENT:PLAYER:自己愛 < 0 || TALENT:PLAYER:抵抗
	SOURCE:PLAYER:鬱屈 += 20

;調教者のTALENT:舌頭霊活をみる
SIF TALENT:PLAYER:舌頭霊活
	TIMES SOURCE:PLAYER:快Ｃ , 1.50

;調教者のTALENT:猫舌をみる
SIF TALENT:PLAYER:猫舌
	TIMES SOURCE:PLAYER:快Ｃ , 0.75
;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷のＰ⇔調教者の口の汚れが移動
STAIN:PLAYER:Ｐ |= STAIN:PLAYER:口
STAIN:PLAYER:口 |= STAIN:PLAYER:Ｐ

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
SIF !FLAG:時間停止
	TIME += 5
;自慰経験
CFLAG:PLAYER:オナニーした ++
EXP:PLAYER:口淫経験 ++
RETURN 1

@CAN_COM702(PLAYER)
SKIPDISP PLAYER
PRINTL 自我口交
;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
A = 0
S = 0

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:PLAYER:欲望
	SIF S
		PRINT  + 
	A += ABL:PLAYER:欲望 * 1
	PRINT 欲望
	PRINTV 'LV,ABL:PLAYER:欲望,'(,ABL:PLAYER:欲望 * 1,')
	S = 1
ENDIF
;ABL:奉仕精神
IF ABL:PLAYER:奉仕精神
	SIF S
		PRINT  + 
	A += ABL:PLAYER:奉仕精神 * 4
	PRINT 奉仕精神
	PRINTV 'LV,ABL:PLAYER:奉仕精神,'(,ABL:PLAYER:奉仕精神 * 4,')
	S = 1
ENDIF
;ABL:精液中毒
IF ABL:PLAYER:精液中毒
	SIF S
		PRINT  + 
	A += ABL:PLAYER:精液中毒 * 3
	PRINT 精液中毒
	PRINTV 'LV,ABL:PLAYER:精液中毒,'(,ABL:PLAYER:精液中毒 * 3,')
	S = 1
ENDIF

IF MARK:PLAYER:快楽刻印
	SIF S
		PRINT  + 
	A += MARK:PLAYER:快楽刻印 * 1
	PRINT 快楽刻印
	PRINTV 'LV,MARK:PLAYER:快楽刻印,'(,MARK:PLAYER:快楽刻印 * 1,')
	S = 1
ENDIF
;PALAM:欲情
IF GETPALAMLV(PALAM:PLAYER:欲情,5)
	SIF S
		PRINT  + 
	A += GETPALAMLV(PALAM:PLAYER:欲情,5) * 1
	PRINT 欲情
	PRINTV 'LV,GETPALAMLV(PALAM:PLAYER:欲情,5),'(,GETPALAMLV(PALAM:PLAYER:欲情,5) * 1,')
	S = 1
ENDIF
;害羞
IF TALENT:PLAYER:羞恥心 > 0
	PRINT  - 
	A -= 1
	PRINT 害羞
	PRINTV '(,1,')
	S = 1
ENDIF
;汚臭鈍感
IF TALENT:PLAYER:汚臭耐性 > 0
	SIF S
		PRINT  + 
	A += 1
	PRINT 汚臭鈍感
	PRINTV '(,1,')
	S = 1
ENDIF
;汚臭敏感
IF TALENT:PLAYER:汚臭耐性 < 0
	PRINT  - 
	A -= 3
	PRINT 汚臭敏感
	PRINTV '(,3,')
	S = 1
ENDIF
;献身的
IF TALENT:PLAYER:献身的
	SIF S
		PRINT  + 
	A += 6
	PRINT 献身的
	PRINTV '(,6,')
	S = 1
ENDIF
;否定快感（淫乱、娼婦で無効）
IF TALENT:PLAYER:快感応答 < 0 && !TALENT:PLAYER:淫乱
	PRINT  - 
	A -= 1
	PRINT 否定快感
	PRINTV '(,1,')
	S = 1
ENDIF

Y = 0
SIF STAIN:PLAYER:Ｐ & 1
	Y += 1
SIF STAIN:PLAYER:Ｐ & 4
	Y += 3
SIF STAIN:PLAYER:Ｐ & 8
	Y += 7
SIF STAIN:PLAYER:Ｐ & 16
	Y += 15

Y = Y * (2 - TALENT:PLAYER:汚臭耐性) / 2

;汚れあり
IF Y
	PRINT  - 
	A -= Y
	PRINT 汚れあり
	;汚臭鈍感
	IF TALENT:PLAYER:汚臭耐性 > 0
		PRINT 、汚臭鈍感
	ELSEIF TALENT:PLAYER:汚臭耐性 < 0
		PRINT 、汚臭敏感
	ENDIF
	PRINTV '(,Y,')
	S = 1
ENDIF

;合計を表示(24以上で実行)
PRINT  = 
PRINTV A

V = 24
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 実行値
PRINTV V

WAIT
SKIPDISP 0
;実行できない
SIF A < V
	RETURN 0
RETURN 1

;-------------------------------------------------
;実行可能判定
;-------------------------------------------------
@COM_ABLE702
;セルフフェラ実行判定
SIF !TFLAG:100
	RETURN 0
;一括管理
SIF GLOBAL_COMABLE(702)
	RETURN RESULT
;性器が汚れているとMASTERの場合舐陰茎はできない（調教者が汚臭無視か愛液、Ｐ、母乳、チョコのよごれならＯＫ）
SIF STAIN:2 >= 4 && (STAIN:2 != 18 && STAIN:2 != 19 && STAIN:2 != 128 && STAIN:2 != 129 && STAIN:2 != 130 && STAIN:2 != 131 && STAIN:2 != 144 && STAIN:2 != 145 && STAIN:2 != 146 && STAIN:2 != 147) && ASSIPLAY == 0 && TALENT:MASTER:汚臭耐性 < 2
	RETURN 0
;性器が汚れており、汚臭敏感で従順3以下のASSIの場合フェラはできない（調教者が汚臭無視か愛液、Ｐ、母乳、チョコのよごれならＯＫ）
IF STAIN:2 >= 4 && (STAIN:2 != 18 && STAIN:2 != 19 && STAIN:2 != 128 && STAIN:2 != 129 && STAIN:2 != 130 && STAIN:2 != 131 && STAIN:2 != 144 && STAIN:2 != 145 && STAIN:2 != 146 && STAIN:2 != 147) && ASSIPLAY
	SIF ABL:ASSI:従順 <= 3 && TALENT:ASSI:汚臭耐性 < 0
		RETURN 0
ENDIF
;猫舌の調教者の場合は技巧Lv2以上必要
SIF TALENT:PLAYER:猫舌 && ABL:PLAYER:技巧 < 2
	RETURN 0
;男人かFutanariじゃないとダメ
SIF !(TALENT:2 & 2)
	RETURN 0
;下半身着衣
SIF TEQUIP:0 & 4
	RETURN 0
;飛機杯使用中はダメ
SIF TEQUIP:12
	RETURN 0
RETURN 1

