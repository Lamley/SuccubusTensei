;-------------------------------------------------
;自慰終了
;-------------------------------------------------
@COM751
;CALL ENDUFUFU(PLAYER)
IF FLAG:70 == 1
	;時間停止中は服装セットしない
ELSEIF CFLAG:PLAYER:イタズラ > 1
	;はだけ中は服装セットしない
ELSEIF LOCK:(CFLAG:PLAYER:311) && CFLAG:PLAYER:イタズラ < 2
	;イタズラLv1以下ならパジャマセット
	CALL CTRL_CLOTHES_SET(PLAYER, "現在衣装の変更_パジャマ")
ELSE
	;イタズラ以外なら普段着セット
	CALL CTRL_CLOTHES_SET(PLAYER, "現在衣装の変更_普段着")
	CFLAG:PLAYER:パジャマ = 0
ENDIF

;服装をTEQUIPへ（↓でやってるので削除）

;ウフフ時系装備リセット
;TEQUIP:ARG:10-TEQUIP:ARG:149を初期化する
;PLAYER:1をカウントに使うのはやめよう
VARSET TEQUIP:(PLAYER):0, 0, 10, 150

;射精関係リセット
IF TFLAG:102 < 2
	BASE:PLAYER:射精 = 0
	TCVAR:PLAYER:イきそう = 0
ENDIF
CFLAG:今忙しくて手が離せない = 0
TEQUIP:PLAYER:50 = -1
TEQUIP:PLAYER:51 = -1
TEQUIP:PLAYER:上半身はだけ状態 = 0
TEQUIP:PLAYER:下半身ずらし状態 = 0
TCVAR:PLAYER:無理矢理 = 0
IF !FLAG:70
	TFLAG:運搬 = 0
	IF CFLAG:PLAYER:口に精液
		PRINTFORML %CALLNAME:PLAYER%把嘴里残留着的精液给\@ ABL:PLAYER:精液中毒 > 2 ? 喝掉了 # 吐出来了 \@
		CFLAG:PLAYER:口に精液 = 0
	ENDIF
	IF CFLAG:PLAYER:顔に精液 || CFLAG:PLAYER:手に精液
		PRINTFORM %CALLNAME:PLAYER%
		IF CFLAG:PLAYER:顔に精液 && !CFLAG:PLAYER:手に精液
			PRINTFORM 脸上的
		ELSEIF !CFLAG:PLAYER:顔に精液 && CFLAG:PLAYER:手に精液
			PRINTFORM 手上的
		ELSE
			PRINTFORM 脸和手上的
		ENDIF
		PRINTFORM 精液を\@ ABL:PLAYER:精液中毒 > 2 ? 含进嘴里 # 擦掉了 \@
		CFLAG:PLAYER:顔に精液 = 0
		CFLAG:PLAYER:手に精液 = 0
	ENDIF
ENDIF
TEQUIP:PLAYER:乳首吸い自慰 = 0
TEQUIP:PLAYER:揉胸自慰 = 0
TFLAG:106 = 0
;はだけフラグをオフ
TEQUIP:PLAYER:2 = 0
TEQUIP:PLAYER:上半身はだけ状態 = 0
CALL CLOTHES_SETTING_TRAIN(PLAYER)
;上衣脱衣済みフラグをオフ
TEQUIP:PLAYER:5 = 0
SIF !FLAG:時間停止
	TIME += 5
;自慰経験
SIF CFLAG:PLAYER:オナニーした > 0
	EXP:PLAYER:自慰経験 ++
CFLAG:PLAYER:オナニーした = 0
RETURN 1

;-------------------------------------------------
;実行可能判定
;-------------------------------------------------
@COM_ABLE751
;自慰終了実行判定
SIF !TFLAG:100
	RETURN 0
SIF TARGET > 0 && FLAG:70 && CFLAG:PLAYER:今忙しくて手が離せない
	RETURN 1
;一括管理
SIF GLOBAL_COMABLE(751)
	RETURN RESULT
RETURN 1

