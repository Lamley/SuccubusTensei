;-------------------------------------------------
;乳頭跳蛋
;道具使用コマンド、レベル2
;-------------------------------------------------
@COM749
;-------------------------------------------------
;特殊派生処理
;-------------------------------------------------
PRINTL 乳頭跳蛋
STR:0 = 乳頭跳蛋
;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者
TCVAR:116 = PLAYER
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力

	DOWNBASE:PLAYER:体力 += 10
	DOWNBASE:PLAYER:気力 += 80

	SOURCE:PLAYER:快Ｂ = 250
	SOURCE:PLAYER:露出 = 20
	SOURCE:PLAYER:逸脱 = 30
	SOURCE:PLAYER:反感 = 30
	;-------------------------------------------------
	;特殊ソース処理
	;-------------------------------------------------
	;圧抑、抵抗持ちには、抑鬱ソース追加
	SIF TALENT:PLAYER:自己愛 < 0 || TALENT:PLAYER:抵抗
		SOURCE:PLAYER:鬱屈 += 30
;-------------------------------------------------
;経験の処理
;-------------------------------------------------
SIF !FLAG:時間停止
	TIME += 5
;自慰経験
CFLAG:PLAYER:オナニーした ++
EXP:PLAYER:道具使用経験 ++
RETURN 1

;-------------------------------------------------
;実行可能判定
;-------------------------------------------------
@COM_ABLE749
;乳頭跳蛋実行判定
SIF !TFLAG:100
	RETURN 0
;一括管理
SIF GLOBAL_COMABLE(749)
	RETURN RESULT
;道具を持っているか、道具なし設定でないとダメ
SIF ITEM:0 == 0 && NOITEM == 0
	RETURN 0
;上半身着衣
SIF (TEQUIP:1 & 2) && TEQUIP:3 == 0
	RETURN 0
;搾乳器使用中は不可
SIF TEQUIP:17
	RETURN 0
;乳房電極装着中はダメ
SIF TEQUIP:24
	RETURN 0
RETURN 1

