;-------------------------------------------------
;肛門電極
;ハードな調教コマンド、レベル4
;-------------------------------------------------
@COM148
;着脱の処理
TEQUIP:肛門電極 = !TEQUIP:肛門電極
;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者
TCVAR:116 = PLAYER

CSTR:TARGET:10 += @"{PLAYER}/"
;TCVAR:2 射精箇所フラグ（-1=避孕套 1=膣内 2=アナル 3=手淫 4=口淫 5=乳交 6=素股 7=足交 8=体表 9=肛門奉仕 10=道具 11=触手 12=Ａ責め 13=Ｖ責め）
TCVAR:TARGET:2 = 12
;強度
TCVAR:TARGET:4 = 1000

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
IF FLAG:70 == 1
BASE:MASTER:TSP -= 300
SOURCE:快Ａ = 500
SOURCE:苦痛 = 750

;フィストなどのAへの大きな苦痛は先に処理しておく
;EXP:Ａ経験をみる
IF EXP:Ａ経験 < EXPLV:1
	TIMES SOURCE:苦痛 , 3.00
ELSEIF EXP:Ａ経験 < EXPLV:2
	TIMES SOURCE:苦痛 , 2.50
ELSEIF EXP:Ａ経験 < EXPLV:3
	TIMES SOURCE:苦痛 , 2.40
ELSEIF EXP:Ａ経験 < EXPLV:4
	TIMES SOURCE:苦痛 , 2.00
ELSEIF EXP:Ａ経験 < EXPLV:5
	TIMES SOURCE:苦痛 , 1.80
ELSEIF EXP:Ａ経験 >= EXPLV:5
	TIMES SOURCE:苦痛 , 1.60
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:苦痛 , 3.00
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:苦痛 , 2.50
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:苦痛 , 2.00
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:苦痛 , 1.50
ELSEIF PALAM:潤滑 >= PALAMLV:4
	TIMES SOURCE:苦痛 , 1.00
ENDIF

ELSE

DOWNBASE:体力 += 300
DOWNBASE:気力 += 450

SOURCE:快Ａ = 500
SOURCE:苦痛 = 1500
SOURCE:露出 = 300
SOURCE:屈従 = 800
SOURCE:逸脱 = 1200
SOURCE:反感 = 1200

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;圧抑、抵抗持ちには、抑鬱ソース追加
SIF TALENT:自己愛 < 0 || TALENT:抵抗
	SOURCE:鬱屈 += 1200

;フィストなどのAへの大きな苦痛は先に処理しておく
;EXP:Ａ経験をみる
IF EXP:Ａ経験 < EXPLV:1
	TIMES SOURCE:苦痛 , 3.00
	TIMES SOURCE:反感 , 4.00
ELSEIF EXP:Ａ経験 < EXPLV:2
	TIMES SOURCE:苦痛 , 2.50
	TIMES SOURCE:反感 , 3.50
ELSEIF EXP:Ａ経験 < EXPLV:3
	TIMES SOURCE:苦痛 , 2.40
	TIMES SOURCE:反感 , 3.00
ELSEIF EXP:Ａ経験 < EXPLV:4
	TIMES SOURCE:苦痛 , 2.00
	TIMES SOURCE:反感 , 2.50
ELSEIF EXP:Ａ経験 < EXPLV:5
	TIMES SOURCE:苦痛 , 1.80
	TIMES SOURCE:反感 , 2.00
ELSEIF EXP:Ａ経験 >= EXPLV:5
	TIMES SOURCE:苦痛 , 1.60
	TIMES SOURCE:反感 , 1.50
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:苦痛 , 3.00
	TIMES SOURCE:反感 , 2.00
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:苦痛 , 2.50
	TIMES SOURCE:反感 , 1.50
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:苦痛 , 2.00
	TIMES SOURCE:反感 , 1.20
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:苦痛 , 1.50
ELSEIF PALAM:潤滑 >= PALAMLV:4
	TIMES SOURCE:苦痛 , 1.00
ENDIF
ENDIF
;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;Ａ経験
EXP:Ａ経験 += 4

SIF !GETBIT (CFLAG:3,8)
	EXP:異常経験 += 1

;同性愛　レズなら戻り値1ゲイなら戻り値2
IF FLAG:70 == 1
IF HETEROSEX(TARGET,PLAYER)
	EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 1
ENDIF
ELSE
IF HETEROSEX(TARGET,PLAYER)
	EXP:(50 + HETEROSEX(TARGET,PLAYER)) += 1
	EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 1
ENDIF
ENDIF
RETURN 1
;-------------------------------------------------
;装着継続の処理
;-------------------------------------------------
@EQUIP_COM148(ARG)
;肛門電極

PRINTL ＜肛門電極挿入中＞

CSTR:TARGET:10 += @"{PLAYER}/"
;TCVAR:2 射精箇所フラグ（-1=避孕套 1=膣内 2=アナル 3=手淫 4=口淫 5=乳交 6=素股 7=足交 8=体表 9=肛門奉仕 10=道具 11=触手 12=Ａ責め 13=Ｖ責め）
TCVAR:TARGET:2 = 12
;強度
TCVAR:TARGET:4 = 500

IF FLAG:70 == 1
BASE:MASTER:TSP -= 200
SOURCE:ARG:快Ａ += 250

;フィストなどのAへの大きな苦痛は先に処理しておく
;EXP:Ａ経験をみる
LOCAL = 750
LOCAL:1 = 600
IF EXP:ARG:Ａ経験 < EXPLV:1
	TIMES LOCAL , 3.00
ELSEIF EXP:ARG:Ａ経験 < EXPLV:2
	TIMES LOCAL , 2.50
ELSEIF EXP:ARG:Ａ経験 < EXPLV:3
	TIMES LOCAL , 2.40
ELSEIF EXP:ARG:Ａ経験 < EXPLV:4
	TIMES LOCAL , 2.00
ELSEIF EXP:ARG:Ａ経験 < EXPLV:5
	TIMES LOCAL , 1.80
ELSEIF EXP:ARG:Ａ経験 >= EXPLV:5
	TIMES LOCAL , 1.60
ENDIF

;PALAM:潤滑をみる
IF PALAM:ARG:潤滑 < PALAMLV:1
	TIMES LOCAL , 3.00
ELSEIF PALAM:ARG:潤滑 < PALAMLV:2
	TIMES LOCAL , 2.50
ELSEIF PALAM:ARG:潤滑 < PALAMLV:3
	TIMES LOCAL , 2.00
ELSEIF PALAM:ARG:潤滑 < PALAMLV:4
	TIMES LOCAL , 1.50
ELSEIF PALAM:ARG:潤滑 >= PALAMLV:4
	TIMES LOCAL , 1.00
ENDIF
SOURCE:ARG:苦痛 += LOCAL

ELSE


DOWNBASE:ARG:体力 += 200
DOWNBASE:ARG:気力 += 250

SOURCE:ARG:快Ａ += 250
SOURCE:ARG:屈従 += 400
SOURCE:ARG:逸脱 += 600


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;圧抑、抵抗持ちには、抑鬱ソース追加
SIF TALENT:ARG:自己愛 < 0 || TALENT:ARG:抵抗
	SOURCE:ARG:鬱屈 += 600

;フィストなどのAへの大きな苦痛は先に処理しておく
;EXP:Ａ経験をみる
LOCAL = 750
LOCAL:1 = 600
IF EXP:ARG:Ａ経験 < EXPLV:1
	TIMES LOCAL , 3.00
	TIMES LOCAL:1 , 4.00
ELSEIF EXP:ARG:Ａ経験 < EXPLV:2
	TIMES LOCAL , 2.50
	TIMES LOCAL:1 , 3.50
ELSEIF EXP:ARG:Ａ経験 < EXPLV:3
	TIMES LOCAL , 2.40
	TIMES LOCAL:1 , 3.00
ELSEIF EXP:ARG:Ａ経験 < EXPLV:4
	TIMES LOCAL , 2.00
	TIMES LOCAL:1 , 2.50
ELSEIF EXP:ARG:Ａ経験 < EXPLV:5
	TIMES LOCAL , 1.80
	TIMES LOCAL:1 , 2.00
ELSEIF EXP:ARG:Ａ経験 >= EXPLV:5
	TIMES LOCAL , 1.60
	TIMES LOCAL:1 , 1.50
ENDIF

;PALAM:潤滑をみる
IF PALAM:ARG:潤滑 < PALAMLV:1
	TIMES LOCAL , 3.00
	TIMES LOCAL:1 , 2.00
ELSEIF PALAM:ARG:潤滑 < PALAMLV:2
	TIMES LOCAL , 2.50
	TIMES LOCAL:1 , 1.50
ELSEIF PALAM:ARG:潤滑 < PALAMLV:3
	TIMES LOCAL , 2.00
	TIMES LOCAL:1 , 1.20
ELSEIF PALAM:ARG:潤滑 < PALAMLV:4
	TIMES LOCAL , 1.50
ELSEIF PALAM:ARG:潤滑 >= PALAMLV:4
	TIMES LOCAL , 1.00
ENDIF
SOURCE:ARG:苦痛 += LOCAL
SOURCE:ARG:反感 += LOCAL:1
ENDIF
;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;Ａ経験
EXP:ARG:Ａ経験 += 1

RETURN 1

;-------------------------------------------------
;実行判定
;-------------------------------------------------
@COM_ABLE148
;肛門電極実行判定
SIF !TFLAG:100
	RETURN 0
;一括管理
SIF GLOBAL_COMABLE(148)
	RETURN RESULT
;下半身着衣
SIF TEQUIP:0 > 1
	RETURN 0
;解除はいつでも可能
SIF TEQUIP:23
	RETURN 1
;道具を持っているか、道具なし設定でないとダメ
SIF ITEM:24 == 0 && NOITEM == 0
	RETURN 0
;肛用振動棒使用中は不可
SIF TEQUIP:14
	RETURN 0
;後庭拉珠使用中は不可
SIF TEQUIP:15
	RETURN 0
;普通の浣腸使用中は不可
SIF TEQUIP:21
	RETURN 0
;バルーン使用中は不可
SIF TEQUIP:22
	RETURN 0
;浴室PLAY中はダメ
SIF TEQUIP:31
	RETURN 0
RETURN 1

