;早苗
@CHARAMOVE_DATA_31(ARG,ARG:2)
LOCAL = 31
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;ひとんちの浴室とか廁所にはいかない
			CASE 12, 13, 14, 18, 30, 35, 39, 40, 51
				RETURN -100
			;守矢神社分社によくいる
			CASE 21
				RETURN 10
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 0, 2, 7, 8
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 1,2,4, 21,22,23,27, 71,72,73,76, 81,82,83,84,85
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

