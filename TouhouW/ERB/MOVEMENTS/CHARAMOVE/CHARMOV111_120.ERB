;キャラ別_各種データ
;111～120番までのキャラ
;CALL		USER
;ARG		0:呼出データNO, 1:移動傾向取得時的房間NO
;RETURN		キャラ別呼出データ値
;COMMENT	
;-----------------------------------------------------------
;克労恩皮絲
@CHARAMOVE_DATA_111(ARG,ARG:2)
LOCAL = 111
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;とくになし
			CASEELSE
				RETURN 0
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 2, 10
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 21,22,23, 109
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

