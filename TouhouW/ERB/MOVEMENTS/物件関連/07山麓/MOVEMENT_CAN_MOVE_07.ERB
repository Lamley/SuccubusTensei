
;地点関連性応答処理
;-----------------------------------------------------------
@CAN_MOVE_7(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
;樹海道入口
CASE 701
	SIF GROUPMATCH(ARG:1,702,734)
		RETURNF 1
	SIF ARG:1 == MAXROOM
		RETURNF 2
CASE 702
	SIF GROUPMATCH(ARG:1,701,703,715,716)
		RETURNF 1
;玄武之澤
CASE 703
	SIF GROUPMATCH(ARG:1,702,704,705,709,735)
		RETURNF 1
CASE 704,735
	SIF ARG:1 == 703
		RETURNF 1
CASE 705
	SIF GROUPMATCH(ARG:1,703,707)
		RETURNF 3
	SIF ARG:1 == 733
		RETURNF 1
CASE 706,708,710
	SIF ARG:1 == 707
		RETURNF 1
CASE 707
	SIF GROUPMATCH(ARG:1,705,706,708,710,711,731)
		RETURNF 1
CASE 709
	SIF GROUPMATCH(ARG:1,703)
		RETURNF 1
CASE 711
	SIF GROUPMATCH(ARG:1,707,712,732)
		RETURNF 1
CASE 712,732
	SIF GROUPMATCH(ARG:1,711)
		RETURNF 1
CASE 715
	SIF GROUPMATCH(ARG:1,702,716)
		RETURNF 1
CASE 716
	SIF GROUPMATCH(ARG:1,702,715,717,718,719)
		RETURNF 1
CASE 717
	SIF GROUPMATCH(ARG:1,716,718,719)
		RETURNF 1
CASE 718
	SIF GROUPMATCH(ARG:1,716,717,719)
		RETURNF 1
CASE 719
	SIF GROUPMATCH(ARG:1,715,716,717,718)
		RETURNF 1
;仙人宅邸華扇ちゃん！
;廚房からは玄関、道場と浴室、二階客間
CASE 720
	SIF GROUPMATCH(ARG:1,721,722,723,729)
		RETURNF 1
;浴室からは道場と廚房玄関、二階客間
CASE 721
	SIF GROUPMATCH(ARG:1,720,722,723,729)
		RETURNF 1
;道場からは浴室と廚房玄関、二階客間
CASE 722
	SIF GROUPMATCH(ARG:1,720,721,723,729)
		RETURNF 1
;二階客間からは道場と廚房、玄関、浴室、書斎、二階出入り口、三階居間
CASE 723
	SIF GROUPMATCH(ARG:1,720,721,722,724,725,726,729)
		RETURNF 1
;書斎
CASE 724
	SIF GROUPMATCH(ARG:1,723,725)
		RETURNF 1
CASE 725
	SIF GROUPMATCH(ARG:1,723,724,730)
		RETURNF 1
CASE 726
	SIF GROUPMATCH(ARG:1,723,727,728)
		RETURNF 1
CASE 727
	SIF GROUPMATCH(ARG:1,726,728)
		RETURNF 1
CASE 728
	SIF GROUPMATCH(ARG:1,726,727)
		RETURNF 1
;仙人の庭と直通
CASE 729
	SIF ARG:1 == 730
		RETURNF 3
	SIF GROUPMATCH(ARG:1,720,721,722,723)
		RETURNF 1
;仙人の庭
CASE 730
	SIF GROUPMATCH(ARG:1,729,731)
		RETURNF 1
;木
CASE 731
	SIF ARG:1 == 730
		RETURNF 1
	SIF ARG:1 == 707
		RETURNF 1
CASE 733
	SIF ARG:1 == 705
		RETURNF 1
CASE 734
	SIF ARG:1 == 701
		RETURNF 1
CASE MAXROOM
	SIF ARG:1 == 701
		RETURNF 1
ENDSELECT

RETURNF 0