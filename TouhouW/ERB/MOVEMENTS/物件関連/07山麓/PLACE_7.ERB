@MAP_SEARCH_REPLACEMENT_7(ARG)
#FUNCTION
SELECTCASE ARG
	CASE 703,704,735
	;玄武之澤
		RETURNF 720
	CASE 708
	;大蛤蟆之池
		RETURNF 730
	CASE 720 TO 730
	;仙人宅邸
		RETURNF 750
	CASE 712
	;間欠泉地下中樞
		RETURNF 760
	CASEELSE
	;山麓樹海
		RETURNF 710
ENDSELECT
RETURNF 0