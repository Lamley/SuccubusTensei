@SKIP_MOVE_0(ARG,ARG:1)
;LOCAL一覧
;0	現在の位置

;現在位置取得
LOCAL = CFLAG:MASTER:現在位置
SIF ARG:1
	LOCAL = ARG:1
;暫定目的地に直接移動できる場合
IF CAN_MOVE(LOCAL, ARG) & 1
	;そこに移動
	RETURN ARG
ENDIF

;袋小路から行ける場所は固定なので、そこを目指す
SELECTCASE LOCAL
;4本殿からは3賽銭箱のみ
CASE 4
	RETURN 3
;5手水舎、21守矢神社分社からは2境内のみ
CASE 5, 21
	RETURN 2
;13浴室からは12脱衣所のみ
CASE 13
	RETURN 12
;17小的儲藏室（留琴寝室）、53倉庫、55和室からは9居間のみ
CASE 17, 53, 55
	RETURN 9
;18壁櫥間（卡娜寝室）からは15霊夢私室のみ
CASE 18
	RETURN 15
;14廁所と19大的儲藏室（萃香寝室）からは10走廊のみ
CASE 14, 19
	RETURN 10
;24温泉からは23温泉・脱衣所のみ
CASE 24
	RETURN 23
;29三妖精之家・廚房と30三妖精之家・浴室からは27三妖精之家・大庁のみ
CASE 29, 30
	RETURN 27
;32～34三妖精各私室からは31二階廊下のみ
CASE 32, 33, 34
	RETURN 31
;35三妖精之家・地下室からは28樓梯のみ
CASE 35
	RETURN 28
;40夢幻遺跡・廁所、43夢美私室・44千百合私室からは37夢幻遺跡・一階通路のみ
CASE 40, 43, 44
	RETURN 37
;39夢幻遺跡・浴室からは38夢幻遺跡・洗面所のみ
CASE 39
	RETURN 38
;42夢幻遺跡・調理室からは41夢幻遺跡・食堂兼談話室のみ
CASE 42
	RETURN 41
;二階各部屋（47～51）からは46二階通路のみ
CASE 47 TO 51
	RETURN 46
;52出口からは11庭のみ
CASE 52
	RETURN 11
;56参道的正前方からは22参道のみ
CASE MAXROOM
	RETURN 22
ENDSELECT

