;　　博　寺　里　湖　竹　森　川　山　守　地　月
;　　　　　　　　　　　　　　　　　　　　　　　
;博　－　30　20　50　40　50　70　40　60　20 120
;寺　30　－　10　40　20　40　60　70　90　50 110
;里　20　10　－　30　20　30　50　60　80　40 100
;湖　50　40　30　－　50　30　50　30　50　50　70
;竹　40　20　20　50　－　50　70　80 100　60 120
;森　50　40　30　30　50　－　20　60　80　70 100
;川　70　60　50　50　70　20　－　30　50　50　70
;山　80　70　60　30　80　60　30　－　20　20　40
;守 100　90　80　50 100　80　50　20　－　40　20
;地　20　50　40　50　60　70　50　20　40　－　60
;月 120 110 100　70 120 100　70　40　20　60　－
;山～森は川経由（手動）で行くと10分早い
;月は基本的に守+20
@SHORTCUT(ARG)
#FUNCTION
SELECTCASE ARG
	;守矢組
	CASE 1
		SIF (FLAG:デート相手 >= 31 && FLAG:デート相手 <= 33) || (TARGET >= 31 && TARGET <= 33)
			RETURNF 1
	;天界組
	CASE 2
		SIF (FLAG:デート相手 >= 34 && FLAG:デート相手 <= 35) || (TARGET >= 34 && TARGET <= 35)
			RETURNF 1
	;冥界組
	CASE 3
		SIF GROUPMATCH(FLAG:デート相手, 23, 26, 30, 66, 76) || GROUPMATCH(TARGET, 23, 26, 30, 66, 76)
			RETURNF 1
	;天界
	CASE 4
		IF CFLAG:MASTER:現在位置 == 天界
			SIF SHORTCUT(2)
				RETURNF 2
			RETURNF 1
		ENDIF
	;冥界
	CASE 5
		IF CFLAG:MASTER:現在位置 >= 彼岸 && CFLAG:MASTER:現在位置 <= 白玉楼
			SIF SHORTCUT(3)
				RETURNF 2
			RETURNF 1
		ENDIF
	;竹林組
	CASE 6
		SIF GROUPMATCH(FLAG:デート相手, 52, 53, 61, 62, 63, 67, 68, 72, 75,)
			RETURNF 1
ENDSELECT
RETURNF 0
