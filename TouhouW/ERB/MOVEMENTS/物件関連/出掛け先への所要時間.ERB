
;地域間の所要時間
@TIME_REQUIRED(出発地域,行先)
#FUNCTION
#DIM 出発地域
#DIM 行先

SELECTCASE 出発地域
	CASE 0
		RETURNF TIME_REQUIRED0(行先 + 5000)
	CASE 1
		RETURNF TIME_REQUIRED1(行先 + 5000)
	CASE 2
		RETURNF TIME_REQUIRED2(行先 + 5000)
	CASE 3
		RETURNF TIME_REQUIRED3(行先 + 5000)
	CASE 4
		RETURNF TIME_REQUIRED4(行先 + 5000)
	CASE 5
		RETURNF TIME_REQUIRED5(行先 + 5000)
	CASE 6
		RETURNF TIME_REQUIRED6(行先 + 5000)
	CASE 7
		RETURNF TIME_REQUIRED7(行先 + 5000)
	CASE 8
		RETURNF TIME_REQUIRED8(行先 + 5000)
	CASE 9
		RETURNF TIME_REQUIRED9(行先 + 5000)
	CASE 10
		RETURNF TIME_REQUIRED10(行先 + 5000)
	CASEELSE
		RETURNF 0
ENDSELECT
@TIME_REQUIRED0(ARG)
#FUNCTION
SELECTCASE ARG
	;神社
	CASE 5000
		RETURNF 0
	;命蓮寺
	CASE 5001
		SIF MAIN_MAP != 1 && ((CHK_DATENOW(CFLAG:56:デート中) == 1 || TARGET == 56) && CFLAG:56:現在位置 == 聖徳道場)
			RETURNF 5
		RETURNF 30
	;人間之里
	CASE 5002
		RETURNF 20
	;霧之湖～紅魔館
	CASE 5003
		RETURNF 50
	;迷途竹林
	CASE 5004
		RETURNF 40
	;魔法之森
	CASE 5005
		RETURNF 50
	;三途之川
	CASE 5006
		SIF SHORTCUT(3) && CFLAG:MASTER:現在位置 >= 彼岸 && CFLAG:MASTER:現在位置 <= 白玉楼
			RETURNF 5
		SIF SHORTCUT(5) == 1
			RETURNF 80
		RETURNF 70
	;妖怪之山(山麓)
	CASE 5007
			RETURNF 40
	;妖怪之山(山頂)
	CASE 5008
			SIF SHORTCUT(1) && CFLAG:MASTER:現在位置 >= 守矢神社境内 && CFLAG:現在位置 <= 守矢神社本殿
				RETURNF 5
			SIF SHORTCUT(2) && CFLAG:MASTER:現在位置 >= 天界
				RETURNF 5
			SIF SHORTCUT(4) == 1
				RETURNF 50
		RETURNF 60
	;地底
	CASE 5009
		RETURNF 20
	;月
	CASE 5010
			SIF SHORTCUT(1) && CFLAG:MASTER:現在位置 >= 守矢神社境内 && CFLAG:現在位置 <= 守矢神社本殿
				RETURNF 25
			SIF SHORTCUT(2) && CFLAG:MASTER:現在位置 >= 天界
				RETURNF 25
			SIF SHORTCUT(4) == 1
				RETURNF 70
		RETURNF 80

ENDSELECT
