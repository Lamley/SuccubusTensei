
@KICKOUT_4()
#FUNCTION
SELECTCASE CHK_INHOME(CFLAG:MASTER:現在位置)
	CASE 20
		RETURNF 419
	CASE 41
		RETURNF 434
	CASE 54
		RETURNF 465
ENDSELECT

@BATHROOM_4()
#FUNCTION
IF CFLAG:MASTER:現在位置 >= 401 && CFLAG:MASTER:現在位置 <= 430
	RETURNF 420
ELSE
	RETURNF 441
ENDIF
RETURNF 0

@BATHCHECK_4(ARG)
#FUNCTION
SELECTCASE CHK_INHOME(ARG)
	CASE 20,41,54
		RETURNF 1
	CASEELSE
		RETURNF 0
ENDSELECT
RETURNF 0

@MAP_UNIQUEMESSAGE_4
CALL DRAW_INFORMATIONLINE("Info",1)
IF WITH_MOB()
	IF FLAG:70
		PRINTFORML 因幡們停下了動作
	ELSEIF GROUPMATCH(TIME:2,1,6,7)
		PRINTFORML 因幡們睡着覚…
	ELSEIF !DAY:3 % 10
		IF !TALENT:MASTER:2 & 2
			PRINTFORML 発情中的因幡們陷入了苦悩……
		ELSE
			PRINTFORML 感到了從発情中的因幡們投来的強烈視線……
		ENDIF
	ELSE
		SELECTCASE RAND:5
			CASE 0
				PRINTFORML 因幡們正在閒逛
			CASE 1
				PRINTFORML 因幡們在玩
			CASE 2
				PRINTFORML 因幡們在聊天
			CASE 3
				PRINTFORML 因幡和因幡肩並着肩
			CASE 4
				PRINTFORML 因幡們堆在一起
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF

;-----------------------------------------
;非自宅時に差し替える場所IDを返す関数
;ARG,対象ID
;RETURNF,差し替え済み対象ID
;-----------------------------------------

@MAP_SEARCH_REPLACEMENT_4(ARG)
#FUNCTION
SELECTCASE ARG
	CASE 401 TO 406
		RETURNF 450
	CASE 407 TO 430
		RETURNF 480
	CASE 431
		RETURNF 440
	CASE 432,433,442,445
		RETURNF 430
	CASE 434,435,440,441
		RETURNF 420
	CASE 436,437
		RETURNF 460
	CASE 438,439
		RETURNF 470
	CASE 450 TO 466
		RETURNF 490
	CASE 443,444,446,447
		RETURNF 410
ENDSELECT
RETURNF 0