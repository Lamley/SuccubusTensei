
@PRINT_STATE_JUEL(選択中キャラID)
#DIM 選択中キャラID
#DIM 表示項目数
PRINTL
PRINTL □ 宝珠 □---------------------------------------------------------------------------
表示項目数 = 0
FOR LOCAL, 0, 22
	;PALAM.CSVに名称が登録されていないとCONTINUE
	SIF !STRLENS(PALAMNAME:LOCAL)
		CONTINUE
	;1000,1000000,単位で区切る
	LOCALS = {JUEL:選択中キャラID:LOCAL}
	SIF JUEL:選択中キャラID:LOCAL > 1000000
		LOCALS = {JUEL:選択中キャラID:LOCAL / 1000}k
