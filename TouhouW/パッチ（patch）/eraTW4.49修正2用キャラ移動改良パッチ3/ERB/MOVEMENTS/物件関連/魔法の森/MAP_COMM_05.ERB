@MAP_PLACENAME_5(ARG)
#FUNCTIONS
#DIM PLACEID

IF GET_MAPID(ARG) == MAIN_MAP
	SELECTCASE ARG
		CASE 501 TO (MAXROOM - 1)
			RETURNF STR:(ARG + 8000)
		CASE 598
			RETURNF OMANEKI_PLACE()
		CASE 599
			RETURNF "スキマ空間"
		CASE MAXROOM
			RETURNF "森の外"
		CASEELSE
			RETURNF "不明"
	ENDSELECT
ELSE
	RETURNF "不明"
ENDIF

@MAP_VIEWING_5
SELECTCASE CFLAG:MASTER:現在位置
	CASE 510 TO 515
		CALL MARISA_HOME
	CASE 520 TO 526
		CALL ALICE_HOME_1
	CASE 527 TO 533
		CALL ALICE_HOME_2
	CASE 534 TO 537
		CALL ELEN_HOME
	CASEELSE
		CALL MAP_MAHOUNOMORI
ENDSELECT
RETURN 1

@MAP_CAN_MOVE_5(ARG, ARG:1)
#FUNCTION
#DIM 在宅
RETURNF 1



@MAP_CANNOT_WORD_5(ARG)
