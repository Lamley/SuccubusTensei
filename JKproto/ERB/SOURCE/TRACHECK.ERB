;-------------------------------------------------
;好感度ボーナス
;-------------------------------------------------
@MASTER_FAVOR_CHECK(ARG,ARG:1)
#FUNCTION
SIF !TCVAR:ARG:行為対象
	RETURNF 100
SELECTCASE ARG:1
	CASE 0 TO 3
		RETURNF GET_REVISION(TA:ARG:(TCVAR:ARG:行為対象):0, 200 , 20000) + 100
	CASE 10,16,20
		RETURNF GET_REVISION((TA:ARG:(TCVAR:ARG:行為対象):1 * 2 + TA:ARG:(TCVAR:ARG:行為対象):3) , 200 , 15000) + 100
	CASE 30 TO 33
		RETURNF MAX(50,100 - (TA:ARG:(TCVAR:ARG:行為対象):1 * 2 + TA:ARG:(TCVAR:ARG:行為対象):3) / 200)
	CASEELSE
		RETURNF 100
ENDSELECT
;-------------------------------------------------
;技巧のチェック
;-------------------------------------------------
@TECHNIQUE_CHECK(ARG,ARG:1)
#FUNCTION
SIF !TCVAR:ARG:行為対象
	RETURNF 100
SELECTCASE ARG:1
	CASE 0 TO 3
		RETURNF GET_REVISION(ABL:(TCVAR:ARG:行為対象):技巧 + 2, 500, 10)
	CASEELSE
		RETURNF 100
ENDSELECT
;-------------------------------------------------
;興奮度
;-------------------------------------------------
@EXCITE_CHECK(ARG,ARG:1)
#FUNCTION
SELECTCASE ARG:1
	CASE 10,12,15,16,18,20,21,22
		RETURNF GET_REVISION(BASE:ARG:興奮度, 50 , 500) + 100
	CASE 30 TO 33
		RETURNF 200 - BASE:ARG:興奮度 / 100
	CASEELSE
		RETURNF 100
ENDSELECT
;-------------------------------------------------
;理性
;-------------------------------------------------
@REASON_CHECK(ARG,ARG:1)
#FUNCTION
SELECTCASE ARG:1
	CASE 0,1,2,3,10,12,15,16,18
		RETURNF GET_REVISION(1000 - BASE:ARG:理性, 50 , 500) + 100
	CASE 30 TO 33
		RETURNF 50 + BASE:ARG:理性 / 100
	CASEELSE
		RETURNF 100
ENDSELECT
