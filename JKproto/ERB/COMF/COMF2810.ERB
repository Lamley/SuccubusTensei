@COM2810(ARG)
;行為
VARSET LOCAL
FOR LOCAL,2000,2200
	SIF TRAINNAME:LOCAL == ""
		CONTINUE
	TRYCCALLFORM COM_ABLE{LOCAL}(H)
	IF RESULT
		LOCAL:1 = RAND(100)
		IF LOCAL:2 < LOCAL:1
			LOCAL:2 = LOCAL:1
			LOCAL:3 = LOCAL
			LOCAL:4++
		ENDIF
	ENDIF
	CATCH
PRINTVL LOCAL
	ENDCATCH
NEXT
STR:0 = % "COM" + TOSTR(LOCAL:3) %
IF LOCAL:4
	CALLFORM %STR:0%(H)
ELSE
	CALL COM2199(H)
ENDIF

RETURN 1

