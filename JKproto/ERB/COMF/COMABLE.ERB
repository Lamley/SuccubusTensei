@GLOBAL_COMABLE(ARG)
#FUNCTION
SELECTCASE ARG
	CASE 0 TO 200
		SIF (TCVAR:MASTER:イベント == 4) || (TCVAR:MASTER:イベント == 2 && BASE:(TCVAR:(TFLAG:行為者):行為対象):反抗 == 0) || (TCVAR:MASTER:イベント == 6 && TFLAG:行為者 != MASTER) || (TCVAR:MASTER:イベント == 7 && TFLAG:行為者 == MASTER && FLAG:タイマー >= TIME)
			RETURNF 1
	CASE 1000,1010,1014,1015,1016
		SIF TCVAR:MASTER:イベント == 3 || !TCVAR:MASTER:イベント
			RETURNF 1
	CASE IS < 1000
		SIF TCVAR:MASTER:イベント
			RETURNF 1
	CASE 1200
		SIF TCVAR:MASTER:イベント
			RETURNF 1
	CASE 2800,2801,3007
		SIF TFLAG:Ｎイベント == 8 && TFLAG:行為者 != MASTER
			RETURNF 1
	CASEELSE
		SIF !TCVAR:MASTER:イベント
			RETURNF 1
ENDSELECT
RETURNF 0
