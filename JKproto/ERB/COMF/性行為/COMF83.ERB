;素股させる
@COM_ABLE83(ARG)
VARSET LOCAL
LOCAL = ARG
VARSET ARG
SIF !TCVAR:LOCAL:行為対象
	RETURN 0
;行為者男性器のみ
SIF !GETBIT(TALENT:LOCAL:2,1)
	RETURN 0
;貞操帯不可（行為者）
SIF TEQUIP:LOCAL:下半身下着 == 373
	RETURN 0
;自宅、情人旅馆、宿舍以外は不可
SIF !((CFLAG:LOCAL:現在地区 == 0 && CFLAG:LOCAL:現在位置 == 1) | (CFLAG:LOCAL:現在地区 == 2 && CFLAG:LOCAL:現在位置 == 22) | (CFLAG:LOCAL:現在地区 == 2 && CFLAG:LOCAL:現在位置 == 26))
	RETURN 0
;行為者が名器装着時は不可
SIF TEQUIP:LOCAL:クリトリス == 80
	RETURN 0
;行為者が绳子装着時は不可
SIF TEQUIP:LOCAL:绳子
	RETURN 0

SIF !CAN_COM83(TCVAR:LOCAL:行為対象,LOCAL)
	RETURN 0

RETURN GLOBAL_COMABLE(83)

