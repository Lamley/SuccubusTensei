@TRAIN_MESSAGE_B
SIF FLAG:6 == 0
	RETURN 0

PRINTL  
;爱抚
IF SELECTCOM == 0
	SIF K
		PRINT キスをしながら

	PRINTFORM %CALLNAME:TARGET%の
	SIF TALENT:TARGET:100 <= -2
		PRINT とても小さな
	SIF TALENT:TARGET:100 == -1
		PRINT 未発達な
	IF TEQUIP:90
		PRINTW 体を触手が弄んでいる…
	ELSE
		PRINTW 体を丹念に爱抚した…
	ENDIF

;舔阴
ELSEIF SELECTCOM == 1
	PRINTFORM %CALLNAME:TARGET%の秘唇を丁寧に舐めほぐし
	SIF PALAM:TARGET:11 >= PALAMLV:3
		PRINT 、尖りきったクリトリスを舌で吸い上げ
	PRINTW た…

;肛门爱抚
ELSEIF SELECTCOM == 2
	IF TEQUIP:90
		PRINTFORML 粘液に塗れた触手が%CALLNAME:TARGET%のアナルを刺激している…
	ELSE
		PRINTFORML %CALLNAME:TARGET%のアナルを弄った…
	ENDIF

;自慰
ELSEIF SELECTCOM == 3
	PRINTFORM %CALLNAME:TARGET%は
	SIF TEQUIP:53
		PRINT カメラの前で
	SIF ABL:17 > 3
		PRINT 見せつけるようにして、
	IF TEQUIP:11 && TEQUIP:13
		PRINT くわえ込んだ２本の自慰棒を自ら抜き挿ししながら
	ELSEIF TEQUIP:11
		PRINT ヴァギナに入れられた自慰棒を自ら抜き挿ししながら
	ELSEIF TEQUIP:13
		PRINT 突っ込まれている肛塞を自ら抜き挿ししながら
	ELSE
		PRINT 秘所を自らの指で弄りながら
	ENDIF
	PRINTW 快感を味わっている…

;口交
ELSEIF SELECTCOM == 4
	PRINTFORML %CALLNAME:TARGET%のペニスを口に咥えて舐めまわした…

;舔肛
ELSEIF SELECTCOM == 5
	;猫舌
	IF TALENT:PLAYER:54
		PRINTFORM %CALLNAME:TARGET%の
			SIF EXP:2 <= 10
			PRINT 硬く閉ざされた
		PRINT アヌスを舌先で小刻みにくすぐ
		SIF EXP:2 >= 100 && PALAM:TARGET:11 >= PALAMLV:3
			PRINT り、筒状にした舌を穴の中まで挿し入れてや
		PRINTW った…
	ELSE
		PRINTFORM %CALLNAME:TARGET%の
			SIF EXP:2 <= 10
			PRINT 硬く閉ざされた
		PRINT アヌスを丁寧に舐めほぐし
		SIF EXP:2 >= 100 && PALAM:TARGET:11 >= PALAMLV:3
			PRINT 、舌先を突き入れて穴の中までかきまわしてやっ
		PRINTW た…
	ENDIF

;胸爱抚
ELSEIF SELECTCOM == 6
	PRINTFORM %CALLNAME:TARGET%の
	IF GETBIT(TALENT:TARGET:2,0)
		IF TALENT:105 == -2 && TALENT:TARGET:100 <= -1
			PRINT 完全になだらかな
		ELSEIF TALENT:105 == -1 && TALENT:TARGET:100 == -1
			PRINT 僅かに膨らみが感じられる程度の
		ELSEIF TALENT:105 == -2
			PRINT ほとんど凹凸のない
		ELSEIF TALENT:105 == -1
			PRINT 標準よりやや小さめの
		ELSEIF TALENT:105 == 1
			PRINT 標準よりも大きめの
		ELSEIF TALENT:105 == 2
			PRINT 巨大な胸でペニスを包み込んで
		ELSE
			PRINT 手から溢れそうなほどの
		ENDIF
	ENDIF
	IF TEQUIP:90
		PRINTW 胸を絞り上げるように、幾本もの触手が絡みついた…
	ELSEIF PALAM:TARGET:11 > PALAMLV:3 && TEQUIP:16 == 0 && TEQUIP:15 == 0
		PRINTW 胸の先端の、すっかり硬くなった突起をつまみあげた…
	ELSE
		PRINTW 胸を揉みしだいた…
	ENDIF

;指挿入
ELSEIF SELECTCOM == 7
	PRINTFORM %CALLNAME:TARGET%の
		IF PALAM:TARGET:9 >= PALAMLV:4
			PRINT 濡れそぼった
		ENDIF
		IF TALENT:TARGET:100 <= -2
			PRINT 非常に小さなワレメ
		ELSEIF TALENT:TARGET:100 == -1
			PRINT 幼いワレメ
		ELSE
			PRINT 秘裂
		ENDIF
	PRINTFORML に%CALLNAME:PLAYER%の指をゆっくりと挿入し、かき回した…
ENDIF

@NMESSAGE_2020(ARG,ARG:1)
;接吻

	PRINTFORM %CALLNAME:(ARG:1)%の口腔を隅々まで舐め回
	IF ABL:(ARG:1):10 >= 4
		PRINTFORML すと、%CALLNAME:(ARG:1)%も積極的に舌を絡めてきた…
	ELSE
		PRINTL した…
	ENDIF

