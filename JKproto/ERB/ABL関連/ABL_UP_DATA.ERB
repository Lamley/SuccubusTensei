	CASE 5
		LOCAL:0 = 30000
	CASE 6
		LOCAL:0 = 40000
	CASE 7
		LOCAL:0 = 50000
	CASE 8
		LOCAL:0 = 60000
	CASE 9
		LOCAL:0 = 70000
	CASEELSE
		LOCAL:0 = 20000 * (ABL:(ARG:2):Ｃ感觉 - 6)
ENDSELECT
