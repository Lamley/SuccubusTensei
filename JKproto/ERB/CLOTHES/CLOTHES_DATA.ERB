;---------------------------------------------------------------------------

;アイテムが装備できるか返す
@CAN_EQUIP(ARG)
#FUNCTION
;ARG アイテム番号
;RETURNF 0装着できない 1できる

SELECTCASE ARG
	CASE 1 TO 19
		IF EXP:MASTER:Ｖ经验 >= EQUIP_LIMIT_V(ARG)
			RETURNF 1
		ELSE
			RETURNF 0
		ENDIF
	CASE 60 TO 79
		IF EXP:MASTER:Ａ经验 >= EQUIP_LIMIT_A(ARG)
			RETURNF 1
		ELSE
			RETURNF 0
		ENDIF
ENDSELECT
RETURNF 1
