;装備に必要なV経験
@EQUIP_LIMIT_V(ARG)
#FUNCTION
;ARG アイテム番号
;RETURNF 必要経験

SELECTCASE ARG
	CASE 1
		RETURNF 10
ENDSELECT
RETURNF 0
;---------------------------------------------------------------------------
;---------------------------------------------------------------------------

;装備に必要なアナル経験
@EQUIP_LIMIT_A(ARG)
#FUNCTION
;ARG アイテム番号
;RETURNF 必要経験

SELECTCASE ARG
	CASE 60
		RETURNF 50
	CASE 61
		RETURNF 30
	CASE 62
		RETURNF 60
ENDSELECT
RETURNF 0
